function ptui_bos(str,uin){
	try{
		var c=document.cookie.split(';'),u=Login_Frame?Login_Frame.document.getElementById('u').value:document.getElementById('u').value,uin='',pic=new Image();
		for(var i=0,n=c.length;i<n;i++){
			var v=c[i].split('=');
			if(v[0].replace(/^\s/,'')=='o_cookie') {
				uin=v[1];
				break;
			}
		}
		pic.src="http://btrace.qq.com/collect?sIp=&iQQ="+uin+"&sBiz=microblog&sOp=btnLoginError&iSta="+str+"&iTy=1210&iFlow=0&iFrom=&iPubFrom=&sUrl=&iUrlType=&iPos=&sText=&iBak1=&iBak2=&sBak1=&sBak2="+u;
	}catch(e){}
}

function cleanCache(f){
	var t=document.createElement("iframe");
	t.src = "http://imgcache.qq.com/ptlogin/ac/v7/clearcache.html#"+f;
	t.style.display = "none";
	document.body.appendChild(t);
}

function ptlogin2_onMibaoCancel(){
	document.location.reload();
}

var num = 0;
function beforeSub(){
	if(((typeof(g_loadcheck)!='undefined')&&g_loadcheck)||(typeof(g_loadcheck)=='undefined')){
		num++;
		if(num<3){
			document.getElementById('login_btn').disabled='disabled';
			window.setTimeout(function(){document.getElementById('login_btn').disabled='';
			},1000);
		}else{
			alert("网络连接问题，请刷新页面重试！");
		}
		return true;
	}
	return false;
}

