<script type="text/javascript">
document.write(unescape("%3Cscript src='" + ((document.location.protocol=="https:")?"https://snapabug.appspot.com":"https://www.snapengage.com") + "/snapabug.js' type='text/javascript'%3E%3C/script%3E"));
</script>
<!--
Main script for page. Almost all codes here should be 
within jQuery(document).ready() statements.
-->
<script type="text/javascript">
// detect if this is the create a ticket page & apply changes if so
jQuery(document).ready(function($) { 
if ( /ticketnewwizard/.test(document.location.href) ) {
	jQuery('select option:contains("do not select")').remove();
}
});
// replace Contact Us description
jQuery(document).ready(function($) {
	// if we're not on the appropriate page these selectors will just be empty
	$('#mli_contactUs_link').html('Extra Tools');
	$('.title:contains("Contact Us")').html('Extra Tools');
	$('#contactUs > .splash_link').html('Extra Tools');
	$('#contactUs > .splash_description').html('Extra tools our service representatives may ask you to use for specific requests.');
});
// all pages - prevent session timeout warning
jQuery(document).ready(function($) { 
	window.showSessionTimeoutWarning = function() {
    sessionRenew_Do(false); // renew session, do not pop up an alert
	};
});
// Resize the screen if it's too small - from Paragon Plus
jQuery(document).ready(function($) { 
  if ( (jQuery(window).width() == 700) && (jQuery(window).height() == 400) ) {
  	window.resizeTo(980,760);
  }
});
//set up SnapEngage chat
jQuery(document).ready(function($) {
	var paratureUserEmail = $('#welcome_email').html();
	var userEmail;
	if (paratureUserEmail) {
		userEmail = paratureUserEmail;
	} else {
		userEmail = '';
	}
	var divSnapABug_W = document.createElement("div");
	divSnapABug_W.id  = "SnapABug_W";
	document.body.appendChild(divSnapABug_W);
	var divSnapABug_WP = document.createElement("div");
	divSnapABug_WP.id  = "SnapABug_WP";
	document.body.appendChild(divSnapABug_WP);
	var divSnapABug_Applet = document.createElement("div");
	divSnapABug_Applet.id  = "SnapABug_Applet";
	document.body.appendChild(divSnapABug_Applet);
	SnapABug.setUserEmail(userEmail);
	SnapABug.setDomain('acs.org');
	SnapABug.initAsync("32ed9abd-0c89-48e6-819a-2f5ff40b0a9b","3","80%");
	// now add chat link to top menu options
	$('.mainmenu').append('<li id="mli_live_chat"><a href="#" onclick="return SnapABug.startLink();" id="mli_live_chat_link">Live Chat</a></li>');
});
</script>
