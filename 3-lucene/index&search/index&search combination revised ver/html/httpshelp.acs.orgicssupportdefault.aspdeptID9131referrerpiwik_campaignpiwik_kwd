function sessionRenew_Do() {
	var sessionImg = new Image(1,1);
	sessionImg.src = "sessionRenewal.asp?deptID=9131&i="+Math.random();
	setTimeout(function(){
	    sessionRenew_Do();
	}, 20 * 60 * 1000); 
}
if (20 > 0) {
    setTimeout(function(){
        sessionRenew_Do();
    }, 20  * 60 * 1000); 
}
