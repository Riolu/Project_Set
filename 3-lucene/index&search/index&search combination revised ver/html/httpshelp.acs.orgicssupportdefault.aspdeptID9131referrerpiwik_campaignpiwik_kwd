        var initializeTree = function() {
            try {
                var markupTree = new YAHOO.widget.TreeView("treeViewChildMarkup"); markupTree.render();
                var basehref = document.getElementsByTagName("base")[0].href;
                function replaceAllInvalidLinks() {
                    // we just replace a with base href  # in yahoo tree
                    var links = document.getElementsByTagName("a");
                    for (var i = 0; i < links.length; i++) {
                        if (links[i].className.indexOf("ygtvspacer") != -1 && (links[i].href == basehref + "#" || links[i].href == "#")){
                            links[i].href = "javascript:void(0)"; 
                            }
                    }
                }

                markupTree.subscribe("expandComplete", function() {
                    replaceAllInvalidLinks();
                });
                // replace # in all a in the page
                replaceAllInvalidLinks();
            } catch (e) { }
        };
      YAHOO.util.Event.onDOMReady(initializeTree);
    </script>

