<script>
function exitSupport() {
	var frmExitSupport = document.forms.logoff;
	frmExitSupport.task.value = "signOut";
   	frmExitSupport.redirectURL.value = "/ics/support/default.asp?deptID=9131"; 
	frmExitSupport.submit();
}
</script>

<script type="text/javascript" src="/ics/inc/js/cookies.js?ver=5%2E8%2E8%2E28496"></script>
<script type="text/javascript" src="/ics/support/inc/portalfooter.js?ver=5%2E8%2E8%2E28496"></script>
<script type="text/javascript">
    var encodedURL = encodeURIComponent(document.URL);
    if (getCookie("newMetricsView") == "1" && getCookie("prevURLMetrics") != encodedURL) {
        //update values
        deleteCookie("newMetricsView", "/");
        setCookie("prevURLMetrics", encodedURL, null, "/");

        try {
            var pvUrl = "/services/WebMetrics.bmp?";

            var pvBody = "accountID=9131&deptID=9131&visitorGuid=b01217d178244bfcba110a2d32d3382c";
            pvBody += "&url=" + encodedURL + "&referer=" + encodeURIComponent(document.referrer);
            addImage(pvUrl + pvBody, false);
        } 
        catch (err) {
            //do not do anything, don't want error propagating
        }
        
        
        setTimeout( deleteVisitCookies, 1000);
    }

    function deleteVisitCookies(){
        deleteCookie("VisitGUID", "/");
        deleteCookie("ArticlesVisited", "/");
    }
</script>
