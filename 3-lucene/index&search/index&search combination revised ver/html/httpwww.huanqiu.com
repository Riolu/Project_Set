<script>
	var floatAD = {
    init:function(obj,opt) {
        this.opt = {
            w:120,
            h:300,
            minW:1000
        };
        for(var i in opt) {
            this.opt[i] = opt[i];
        }
        this.o = typeof obj=="string"?$("."+obj):obj;
        this.minW = this.opt.minW;
        this.o
        .css({
            width:this.opt.w,
            height:this.opt.h,
            position:"absolute",
            top:145
        });
        this.pos();
        this.close();
        $(window).on("resize",(function(t) {return function() {t.pos.call(t);};})(this));
    },
    pos:function() {
        this.getW();
        var l = this.W>this.minW+this.opt.w*2?(this.W -  (this.minW + this.opt.w * 2)) / 2:0;
        
        
        this.o.eq(0).css("left",l);
        this.o.eq(1).css("right",l);
        
        this.o.each(function() {
            var $o  = $(this),
                v   = $o.html(),
                val = $o.attr("key"), 
                t   = "";
            !$("#BAIDU_DUP_wrapper_"+val+"_0").html()&&$o.hide();
            v.replace(/(http:\/\/dup.baidustatic.com\/painter\/)([\w\d]*\/)?([\w\d]+\.js){1}/ig,function(word) {
                word.indexOf("floatad")!=-1&&$o.hide();
            });          
        })
    },
    close:function() {
        var $cB = $(document.createElement("div"));
        $cB.css({
            width:"100%",
            height:"20px",
            background:"#999",
            position:"absolute",
            color:"#fff",
            cursor:"pointer",
            "text-align":"center",
            "font-size":"12px",
            "line-height":"20px",
            bottom:0,
            left:0
        })
        .html("关 闭")
        .appendTo(this.o)
        .hover(function() {
            $(this).css("background","#0066cc");
        },function() {
            $(this).css("background","#999");
        })
        .on("click",(function(t) {
            return function() {
                t.o.remove();
            };
        })(this));
    },
    getW:function() {
        this.W = $(document.body).innerWidth();
    }
};
floatAD.init("addl");
</script>
