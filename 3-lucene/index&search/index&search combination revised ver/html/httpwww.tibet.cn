				   location.reload();
				 }
				});
	    //location.reload();
	   }
	   }
	 });
  }
   function logo(){
     var user=$("#username").val();
	 var pass=$("#password").val();
	 var yzm=$("#yz").val();
	 if(user==""){
	   alert("请输入用户名");
	   $("#username").focus();
	   return false;
	 }
	 if(pass==""){
	  alert("请输入密码");
	  $("#password").focus();
	  return false;
	 }
	 if(yzm==""){
	 alert("请输入验证码");
	 $("#yz").focus();
	 return false;
	 }
	 $.ajax({
	  type : 'get',
	  data : 'yzm='+yzm,
	  url  : 'http://trends.tibet.cn/api/code.php',
  dataType : 'jsonp',
     jsonp : 'callback',
   success : function(json){
     if(json==1){
		 $.ajax({
			type : 'get',
			data : {'username':''+user,'password':''+pass,'yzm':''+yzm},
			url  : 'http://trends.tibet.cn/api/login.php',
		dataType : 'jsonp',
		  jsonp  : 'callback',
		 success : function(json){
		   if(json.status==1){
		    $.ajax({
				 type:'get',
				 url:'http://bbs.tibet.cn/login.php',
				 data:{'mod':'logging','action':'login','username':user,'password':pass},
				 dataType:'jsonp',
				 jsonp:'callback',
				 success:function(json){
				   //console.log(json);
				   location.reload();
				 }
				});
			//location.reload();
		   }
		   else{
			alert(json.msg);
		   }
		 }
		 });
	 }else{
	  alert("验证码错误");
	  $("#yz").focus();
	 }
   }
});	 
   }
   
checker();
