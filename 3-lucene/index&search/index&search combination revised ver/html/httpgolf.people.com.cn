<script>
$(function(){
	var index2 = 0;
	var timer2;
	var len2 = $("#pic_list2 li").length;
	
	for(i=0;i<len2;i++)
	{
		$("#num_list2").html($("#num_list2").html()+"<li>"+(i+1)+"</li>");
	}
	
	$("#num_list2 li").click(function(){
		index2 = $("#num_list2 li").index(this);
		showPic2(index2);
	}).eq(0).click();
	
	$("#box2").hover(function(){
		clearInterval(timer2);
	},function(){
		timer2 = setInterval(function(){
			showPic2(index2);
			index2++;
			if(index2==len2){index2=0;}
		},3000)
	}).trigger("mouseleave");
	
	function showPic2(index2){
		$("#num_list2 li").removeClass("active").eq(index2).addClass("active");
		$("#pic_list2 li").eq(index2).stop(true,true).fadeIn(600).siblings("li").fadeOut(600);
	};
});
</script>
