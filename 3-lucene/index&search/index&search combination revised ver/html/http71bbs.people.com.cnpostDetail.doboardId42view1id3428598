<script language="javascript" type="text/javascript">
$(document).ready(function(){
	$("div[content_path]").each(function(){
		var ajax_postId = $(this).attr("content_id") ;
		var ajax_postPath = $(this).attr("content_path") ;
		
		$
    	.ajax({
		    url: ajax_postPath,
		    type: "GET",
		    dataType: "html",
		    ajax_postId: ajax_postId,
		    success: function(html) {
		    	if(parseInt(ajax_postId) < 1877221){
		    		html = html.replace(/&lt;/g, "<").replace(/&gt;/g, ">");
		    	}
		    	$("#post_content_" + ajax_postId).html(html) ;
		    }
		});
	
	}) ;
	
	//生成网友头像
	rmw.global.bindUserPortraits($("img.portrait", $(document))) ;
	
	//获取登录信息
	var guzzLoginUser = rmw.global.getLoginUser() ;
	if(guzzLoginUser){
		//与我的关注关系。只要链接中有“userNick=utf8编码后的昵称”这个参数即可。
		rmw.sns.checkRSWithMe($("a.unfollowed", $(document))) ;
		//绑定关注操作
		rmw.sns.bindFollowActions($("a[href*='usr/friendFollow.do']", $(document))) ;

		delete guzzLoginUser ;
	}else{
		//Not Logged in.
	}
}) ;


</script>


