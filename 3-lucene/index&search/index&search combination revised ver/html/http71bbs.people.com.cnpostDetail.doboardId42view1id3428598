<script>
	function parseHotList(divId, data){
		var items = $(data).find("item") ;
		var max = items.length > 5 ? 5 : items.length;
		var str = "<ul>";
		
		items.each(function(index, ele) {			
			if(index >= max){
				return false ;
			}
			str += "<li><a target='_blank' href='" + $(ele).find("link").text() + "'>" + $(ele).find("title").text() + "</a><span class='blue'>(" + $(ele).find("opTimes").text();	
			str += ")</span></li>";	
        }) ;
        
		str = str + "</ul>" ;
		$("#" + divId).append(str) ;
	}
	
	function XParser(url, divId){
		$
    	.ajax({
		    url: url,
		    type: "GET",
		    dataType: "xml",
		    divId: divId,
		    success: function(xml) {
		    	parseHotList(divId, xml) ;
		    }
		});
	}
	
	XParser("/xml/hot_click_article_all_1.xml", "hotListDiv");
	XParser("/xml/hot_reply_article_all_1.xml", "hotListDiv2");
	XParser("/xml/hot_liaoba.xml", "hotListDivLiaoba");

</script>
