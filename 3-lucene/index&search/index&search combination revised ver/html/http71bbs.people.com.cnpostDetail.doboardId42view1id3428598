				var page_url = '/postDetail.do?boardId=42&view=1&id=3428598&pageNo=' ;
				
				function jump(form){
					var pageNo = form.jumpPageNo.value ;
					var pageTotal = 1 ;
					
					if(isNaN(pageNo)){
						alert("请输入要跳转到的页码") ;
						return false ;
					}
					
					if(pageNo < 1 || pageNo > pageTotal){
						alert("页码必须在1到" + pageTotal + "之间") ;
						return false ;
					}
					
					var m_location = page_url + pageNo ;
					this.location = m_location ;
					
					return false ;
				}				
			</script>
			
			            
