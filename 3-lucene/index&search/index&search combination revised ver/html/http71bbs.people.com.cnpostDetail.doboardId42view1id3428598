<script>

function packageSameTheme(fromObj){
	//var msgbox = new Msgbox(null, fromObj, "推荐给好友", '<iframe width="800px" name="conFrm" height="440" NORESIZE SCROLLING=No FRAMEBORDER=0 MARGINHEIGHT=0 MARGINWIDTH=0 src="./postMail.do?id=3428598"></iframe>');
	window.open("http://71bbs.people.com.cn/postMail.do?id=3428598");
	//new TextParserOfValue("","cDiv");
}
								
function doPrint(){
	var obj = document.getElementById("detail_content");
	var logo_bar = document.getElementById("head_logo");
	var page = document.getElementById("detail_page");
	var cons = document.getElementsByClassName('content_3428598')
	var str ='<html>\n<head>';
	var con = null;

	for(var i = 0 ; i < cons.length ; i += 1){
		con = cons[i];
		con.setAttribute("class","");
		con.setAttribute("className","");
		try{
			con.parentNode.setAttribute("class","");
			con.parentNode.setAttribute("className","");
			con.parentNode.style.padding = "10px";
		}
		catch(e){}
	}
	
	str += '\n<link href="/css/${board.boardStyle.cssFolder}" rel="stylesheet" type="text/css">';
	str += '\n<link href="/css/print.css" rel="stylesheet" type="text/css">';
	str += '\n</head><body onload="window.print();" style="margin:0px;padding:0px;background:#ffffff;"><center>';
	page.style.display = "none";
	str += logo_bar.innerHTML;
	str += obj.innerHTML;
	str += '\n</center></body>\n</html>';
	try{
		document.write(str);
		document.close();
	}catch(e){}
}

function openComplaintDia(fromObj, id){
	window.open('http://71bbs.people.com.cn/disclosureForm.do?id='+id,'投诉帖子','height=495,width=725');
}
</script>

