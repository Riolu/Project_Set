<script type="text/javascript">
	var divWidth=70;var divHeight=77;
	function getFlashWH(d_title){
	if(d_title==''||d_title==null){
		return;
	}
		var d_start=d_title.indexOf("(");
		
		var d_end=d_title.indexOf(")");
		var wh=d_title.substring(d_start+1,d_end);
		var x_index=wh.indexOf("*");
		if(d_start==-1||d_end==-1||x_index==-1){return;}
		//宽*高
		divWidth=wh.substring(0,x_index);//设置广告块宽度
		//alert(divWidth);
		divHeight=wh.substring(x_index+1,wh.length);//设置广告块高度
		//alert(divHeight);	
	}
</script>
<script type="text/javascript">
var divImg='';
var divUrl="";//设置广告链接 

//设置广告图片的URL地址
var varId;//获取setInterval的ID 
var _stepx=2;_stepy=2;//初始化每次偏离的象素 
var moveSpeed=60;//速度 

function moveAd(){//飘浮广告主函数 
var adLeft=parseInt(adDiv.style.left); 
var adTop=parseInt(adDiv.style.top); 
var adWidth=parseInt(adDiv.style.width); 
var adHeight=parseInt(adDiv.style.height);
var _bodyLeft=document.body.scrollLeft;
var _bodyTop=document.body.scrollTop;
var _bodyHeight=document.body.clientHeight+_bodyTop; 
var _bodyWidth=document.body.clientWidth+_bodyLeft; 
if(adLeft<=_bodyLeft){ 
_stepx=2; 
} 
if(adTop<=_bodyTop){ 
_stepy=2; 
} 
if((adLeft+adWidth)>=_bodyWidth){ 
_stepx=-2 
} 
if((adTop+adHeight)>=_bodyHeight){ 
_stepy=-2 
} 
adDiv.style.left=adLeft+_stepx; 
adDiv.style.top=adTop+_stepy; 
} 
function beginMoveAd(){//启动飘浮 
varId=window.setInterval("moveAd()",moveSpeed) 
}
function stopMoveAd(){
	window.clearInterval(varId);
}
if(divImg!=''){

//设置广告块
var divLeft=0;//设置广告块初始左边位置 
var divTop=0;//设置广告块初始顶部位置 
var divTitle="";
var pic_t=/gif|jpg|bmp$/;
if(pic_t.test(divImg)){
document.write("<DIV id=\"adDiv\" style=\"position:absolute; left:"+divLeft); 
document.write("px; top:"+divTop+"px; width:"+divWidth+"px; height:"+divHeight); 
document.write("px; z-index:5;\" onMouseOver=\"stopMoveAd()\""); 
document.write(" onMouseOut=\"javascript:beginMoveAd();\"><a target=\"_blank\" href=\""+divUrl+"\">"); 
document.write("<img src="+divImg+" width="+divWidth+" height="+divHeight+" /></a></DIV>");
}else{
 document.write("<DIV id=\"adDiv\" style=\"position:absolute; left:"+divLeft); 
document.write("px; top:"+divTop+"px; width:"+divWidth+"px; height:"+divHeight); 
document.write("px; z-index:5;\" onMouseOver=\"stopMoveAd()\""); 
document.write(" onMouseOut=\"javascript:beginMoveAd();\"><a target=\"_blank\" href=\""+divUrl+"\">"); 
document.write("<object classid=\"clsid:D27CDB6E-AE6D-11cf-96B8-444553540000\" codebase=\"http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=7,0,19,0\" width=\"100%\" height=\"100%\">          <param name=\"movie\" value=\""+divImg+"\" /><param name=\"wmode\" value=\"Opaque\" /></object></a></DIV>");}
beginMoveAd();
}
//飘浮广告 
</script>
