    <script>
        function tasksFormat(data) {
            var strHtml = "";
            $.each(data, function (idx, item) {
                if (item.IsAllDay == true) {
                    strHtml += "<li style=\"display:none\">" + item.Start.toDate().format("MM/dd W") + " 全天 " + item.Title + "</li>";
                } else {
                    strHtml += "<li style=\"display:none\">" + item.Start.toDate().format("MM/dd W HH:mm") + "~" + item.End.toDate().format("HH:mm") + " " + item.Title + "</li>";
                }
            });
            strHtml += "<li style=\"display:none\" class=\"text-right \"><a href='/Scheduler'>更多</a><i class=\"glyphicon glyphicon-circle-arrow-right\"></i></li>"
            $(".scheduler ul").append(strHtml);
        }

        function scheduler() {
            var url = '/Ajax/Tasks';
            $.post(url, function (data) {
                tasksFormat(data);
                $(".scheduler ul li:first").slideUp(function () {
                    $(".scheduler ul li").not(":first").each(function (inx, item) {
                        var el = this;
                        setTimeout(function () {
                            $(el).fadeIn("slow");
                        }, inx * 200);
                    });
                });
            });
        }

        $(function () {
            $('.carousel').carousel();
            scheduler();
        });
    </script>

