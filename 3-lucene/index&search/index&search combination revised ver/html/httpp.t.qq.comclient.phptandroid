		opt2 = $('opt2'), 
		logo = $('mLogo'), 
		pic = $('mPic'), 
		down = $('mDown'), 
		softname = $('mSoftname'), 
		dataSet = product_weibo['brands'],
		one = new Linkage('opt1', 'opt2', dataSet);
		one.init();
		opt1.options.length = 0;
		opt2.options.length = 0;
		for (var k in dataSet) {
			var name = dataSet[0]["name"], 
			models = dataSet[0]["models"][0],
			optItems1 = new Option(dataSet[k]['namech'], k);
			opt1.options[opt1.options.length] = optItems1;
			UI.A(logo, 'src', 'http://softfile.3g.qq.com:8080/msoft/image/web/img_cellphone_brand_90x35_' + name.toLocaleLowerCase() + '.gif');
			UI.A(pic, 'src', models["logourl"]);
			UI.A(down, 'href', 'http://wt.qq.com/dlink.jsp?brand=' + name + '&model=' + models["name"]);
			softname.innerHTML = models["softName"]
		}
		for (var k in dataSet[0]["models"]){
			optItems2 = new Option(dataSet[0]["models"][k]['name'], k);
			opt2.options[opt2.options.length] = optItems2;
		}
		UI.EA(opt1, 'change', function(){
			UI.A(logo, 'src', 'http://softfile.3g.qq.com:8080/msoft/image/web/img_cellphone_brand_90x35_' + dataSet[opt1.value]["name"].toLocaleLowerCase() + '.gif');
		});
		UI.EA(opt2, 'change', function(){
			var opt1Val = dataSet[opt1.value]["models"][opt2.value];
			UI.A(pic, 'src', opt1Val["logourl"] !='' || null || undefined ? opt1Val["logourl"] : 'http://softfile.3g.qq.com:8080/msoft/image/web/machine/no_model.gif');
			UI.A(down, 'href', 'http://wt.qq.com/dlink.jsp?brand=' + dataSet[opt1.value]["name"] + '&model=' + opt1Val["name"]);
			softname.innerHTML = opt1Val["softName"];
		});
})
