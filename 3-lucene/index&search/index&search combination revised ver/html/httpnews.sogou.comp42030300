    <script type="text/javascript">
        var display = false;
        $(".r_nav").hover(function(){
           display = true; 
        }, 
        function(){ 
            display = false;
        });
        var ids = ["china", "world", "society", "mil", "ent", "business", "sports", "edu", "house", "women", "auto", "sci", "internet", "game", "pic"];
        $(window).bind("scroll", function(){
           if( $(window).scrollTop() == 0 ){
              $("a.go_top").hide();
              
           } else {
              $("a.go_top").show();
           }
            var top = (document.body.scrollTop || document.documentElement.scrollTop);
            var windowHeight = $(window).height();
            //$(".r_nav").toggle(top > windowHeight);
            $(".r_nav").toggle(top > $("#newsBoxTop").height() + $("#newsBoxTop").offset().top);
            for (var i in ids) {
                var offset = $("#" + ids[i]).offset().top - top;
                if(offset > -30){
                    $(".r_nav li.on").removeClass("on");
                    $(".r_nav li:eq("+(parseInt(i)+1)+")").addClass("on");
                    break;
                }
            }
       }).trigger("scroll");
       (function(){
            var init = function(){
                if( $(window).scrollTop() == 0 ){
                  $("a.go_top").hide();
               } else {
                  $("a.go_top").show();
               }    
            };
            var gotop = function(){
               $(window).scrollTop(0);
               return false;
           };
           function m_s() {
            	var w = document.searchForm.query;
            	if (new RegExp("kw=([^&]+)").test(location.search)) {
            		if (w.value.length == 0) w.value = decodeURIComponent(RegExp.$1)
            	}
            };
            $(document).ready(init);
            $(document).ready(m_s);
            var a = document.getElementById("go_top");
            if (typeof a == 'undefined' || a == null)
                return;
            if (typeof window.addEventListener != "undefined") {
                a.addEventListener("click",gotop,false);
            } else {
                a.attachEvent("onclick",gotop);
            }
       })();
    </script>
