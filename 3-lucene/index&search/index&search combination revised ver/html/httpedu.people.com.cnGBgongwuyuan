function go(){
	if (document.publish.content.value==null || document.publish.content.value=="") {
		alert("发言内容不能为空！");
		return;
	} else {
		document.publish.submit();
	}
}
function go1(){
	if (document.publish1.content.value==null || document.publish1.content.value=="") {
		alert("发言内容不能为空！");
		return;
	} else {
		document.publish1.submit();
	}
}
function renew_parent(form,parent_token)
{
	form.pid.value=parent_token;
}
