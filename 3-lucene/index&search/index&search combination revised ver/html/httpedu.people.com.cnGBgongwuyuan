<script language="javascript">
function shows1(n){
	for(var i=1;i<=2;i++){
		var td1=document.getElementById("td1_"+i);
		var con1=document.getElementById("con1_"+i);
		if(i==n){
			td1.className="t08";
			con1.style.display="block";
		}else{
			td1.className="t08";
			con1.style.display="none";
		}
	}
}
function shows2(n){
	for(var i=1;i<=3;i++){
		var td2=document.getElementById("td2_"+i);
		var con2=document.getElementById("con2_"+i);
		if(i==n){
			td2.className="t38";
			con2.style.display="block";
		}else{
			td2.className="t39";
			con2.style.display="none";
		}
	}
}
function shows3(n){
	for(var i=1;i<=6;i++){
		var td3=document.getElementById("td3_"+i);
		var con3=document.getElementById("con3_"+i);
		if(i==n){
			td3.className="aa";
			con3.style.display="block";
		}else{
			td3.className="";
			con3.style.display="none";
		}
	}
}
</script>
