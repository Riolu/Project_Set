<script>
//add by phj 20130131 make these divs can display and hide auto
//默认自动滚动开启，如果鼠标停留在id为 h1multiple 的div区域，则暂时停止；如果鼠标移出该div区域，则恢复滚动
var $isauto = 1;
//是否第一次执行，1 是  0 否
var $isfirst = 1;
//头条切换时间间隔 单位：毫秒
var $slidetime = 3000;
//如果鼠标停留在头条区域，则暂停切换
$("#h1multiple").mouseover(function(){
$isauto = 0;
//alert("on");
});
//如果鼠标移出了头条区域，则继续切换
$("#h1multiple").mouseout(function(){
$isauto = 1;
//alert("off");
});
var $autoFun;
//@Mr.Think***调用自动滚动
//如果第一次执行，把标识置为0，并且延迟指定的切换时间间隔再进行切换
//此处的目的，是防止打开页面之后当前显示的内容被立刻替换掉
if($isfirst == 1)
{
$isfirst = 0;
setTimeout(autoSlide, $slidetime);
}
//@Mr.Think***自动滚动
function autoSlide(){    
    if($isauto == 1)
   {
   //获取头条div中包括的标题总数、当前标题位置（计数从1开始）
   var divtotal = 0;
   var divcurrent = 0;
   $("#h1multiple > div").each(function()
   {
   divtotal++;
   if($(this).css("display") == "block")
   {
   divcurrent = divtotal;
   }
   }
   )
   //如果当前标题不是最后一个，则隐藏当前标题，并显示后一个标题（注意：主要是因为下标是从0开始的，所以需要进行判断）
   if(divtotal != divcurrent)
   {
   $("#h1multiple > div").eq(divcurrent - 1).css("display","none");
      $("#h1multiple > div").eq(divcurrent).css("display","block");
   //$("#h1multiple > div").eq(divcurrent - 1).fadeOut(1000);
      //$("#h1multiple > div").eq(divcurrent).fadeIn(1000);
   }
   else
   {
   $("#h1multiple > div").eq(divcurrent - 1).css("display","none");
      $("#h1multiple > div").eq(0).css("display","block");
   //$("#h1multiple > div").eq(divcurrent - 1).fadeOut(1000);
      //$("#h1multiple > div").eq(0).fadeIn(1000);
   }
   }
    else if($isauto == 0)
   {
    clearAuto();
   }
    $autoFun = setTimeout(autoSlide, $slidetime);//此处不可使用setInterval,setInterval是重复执行传入函数,这会引起第二次划入时停止失效
}
//@Mr.Think***清除自动滚动
function clearAuto(){
    clearTimeout($autoFun);
}
</script>
