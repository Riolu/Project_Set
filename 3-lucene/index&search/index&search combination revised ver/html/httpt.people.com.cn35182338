<script type="text/javascript">
var heUserId = '27682338';

$(function(){
	RMWeibo.Widgets.pullMenu.pull($("dl[data-type=pullMenu]"));
    var _open=1;
	$("a[data-type=openMail]").click(function(){
	    if(_open==1){
            $(".tips_layer").slideDown(50);
            $("#mailMsg").val("");
            _open=0;
        }else{
            $(".tips_layer").slideUp(50);
            _open=1;
        }
	});
	$(".arr_mm").click(function(){
		if($("#mailMsg").val()==""){
			$("#mailMsg").focus();
		}else{
			RMWeibo.Widgets.toHeMsg.saveMail($(this).attr("data-user"),$("#mailMsg").val(),$(".tips_layer"));
            _open=1;
        }
	});
	
	//企业版客服在线
	$(".arr_pb2").click(function(){
		if($("#mailText").val()==""){
			$("#mailText").focus();
			RMWeibo.tools.showmsg('请填写内容！', 250);
		}else{
			RMWeibo.Widgets.toHeMsg.saveMail($(this).attr("data-user"),$("#mailText").val(),$(".minipb_b"));
			$("#mailText").val("");
			$(".minipb_b").hide();
			$(".minipb_a").show();
			RMWeibo.tools.showmsg('发送成功！', 250,"ok");
        }
	});
	//企业版客服在线
	
	$("a[data-type=setgruop]").click(function(){
		///alert($(this).attr("data-nickName"));
		RMWeibo.Widgets.userGroup.open($(this).attr("data-nickName"),"设置分组");
	});
	$("a[data-type=addBlack]").click(function(){
			var $_self=$(this);
			RMWeibo.tools.confirm('你确定要拉黑吗？', 220, function(value){
				if(value === true){
					RMWeibo.Widgets.blackUser.add($_self.attr("data-userId"),$("a[data-type=cancelBlack]"));	
				}
			});
	});
	$("a[data-type=cancelBlack]").click(function(){
			var $_self=$(this);
			RMWeibo.tools.confirm('你确定要取消吗？', 220, function(value){
				if(value === true){
					RMWeibo.Widgets.blackUser.cancel($_self.attr("data-userId"),$("a[data-type=cancelBlack]"));	
				}
			});
	});
})
</script>

