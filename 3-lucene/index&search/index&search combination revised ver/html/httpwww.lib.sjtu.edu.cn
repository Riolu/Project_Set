function initToolbar() {
    /*toolbar*/

    $("#toolbar ul li a").hover(function () {
        var rel = $(this).attr("data-rel");
        $(".toolbar-outside").addClass("hide");

        if (rel == undefined) return;

        //for qr code
        if (rel == "qr") {
            var src = static_path + "img/" + $(this).attr("data-qr");
            $("#toolbar-outside-" + rel).find("img").eq(0).attr("src", src);
        }

        $("#toolbar-outside-" + rel).removeClass("hide");

    }, function () {});

    $("#toolbar-outside-share li a").on("click", function (e) {
        e.preventDefault();
        e.stopPropagation();

        shareToSNS($(this).attr("data-sns"));
    });

    $(".toolbar-outside").mouseleave(function () {
        $(this).addClass("hide");
    });

    //toolbar
    $("#toolbar .trigger").click(function (e) {
        e.preventDefault();
        e.stopPropagation();

        var el = $("#toolbar");

        if (el.hasClass("toolbar-closed")) {
            el.removeClass("toolbar-closed");
            el.addClass("toolbar-opened");
        } else {
            el.addClass("toolbar-closed");
            el.removeClass("toolbar-opened");
        }

    });

    $(document).click(function () {
        if(!$("#toolbar").hasClass("toolbar-closed")) {
        	$("#toolbar").removeClass("toolbar-opened").addClass("toolbar-closed");
        }
    })

    setTimeout(function () {
        //made qrcode 
