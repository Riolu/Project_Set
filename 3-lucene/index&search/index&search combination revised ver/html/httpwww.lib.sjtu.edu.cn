	});
	
	function changeSlideIdentifier(args){
		var i = args.currentSlideNumber - 1;
		var el = $("#spotlight-wrapper div.slide");
		var target = $("#spotlight-wrapper .ui-more");
		log(i, el.eq(0), target.find(".ui-title").html())

		target.find(".ui-title").html("<a href='" + el[i].getAttribute("data-url") + "'>" + el[i].getAttribute("data-title") + "</a>")
		target.find(".ui-desc").html(el[i].getAttribute("data-desc"));
		target.fadeIn();
		
		$(".ui-index a.active").removeClass("active");
		$(".ui-index a").eq(i).addClass("active");
	}

}
</script>
