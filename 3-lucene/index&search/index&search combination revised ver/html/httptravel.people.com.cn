<script type="text/javascript">
	var wrap = document.getElementById('fixedImage');
	function resize(){
		var left =document.documentElement.scrollLeft+ (document.documentElement.clientWidth -980)/2 - 200;
		if (left < 0) {
			left = 0;
		}
		wrap.style.right = left + 'px';
	}
	window.onresize = resize;
	resize();
	var deleteElem = document.getElementById('floatDelete');
	deleteElem.onclick = function() {
		var wrap = document.getElementById('fixedImage');
		wrap.parentNode.removeChild(wrap);
	};


//获取推荐新闻
$.ajax({  
        type : "get",  
        async: false,  
        url : "http://paike0.people.com.cn/newssearch/queryrect?num=10",  
        dataType : "jsonp",
        jsonp: "callback",
        success : function(data){
			//data["2"][0]表示第二页第一条
			var xwtj = '';
            for( var i = 0; i < data["1"].length; i++ )
			{
				var title = data["1"][i].title;
				var newslink = data["1"][i].newsLink;
				title = title.length > 14 ? title.substr(0,14) + '…' : title;
				xwtj += '<li><a href="' + newslink + '" target="_blank">' + title + '</a></li>';
				//alert(data["1"][i].title + data["1"][i].newsLink);
			}
			document.getElementById("ba_3_ul").innerHTML = xwtj;
        }  
    });


</script>
