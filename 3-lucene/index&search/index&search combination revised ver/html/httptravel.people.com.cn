<script type="text/javascript">
		var speed_a = 0; //速度(毫秒)
		var space_a = 10; //每次移动(px)
		var PageHeight_a = 66; //翻页宽度
		var fill_a = 0; //整体移位
		var moveLock_a = false;
		var moveTimeObj_a;
		var moveWay_a = "right";
		var comp_a = 0;
		var autoPlayObj_a=null;
		
		GetObj_a("List2_a").innerHTML=GetObj_a("List1_a").innerHTML;
		GetObj_a('ISL_Cont_a').scrollTop=fill_a>=0?fill_a:GetObj_a('List1_a').scrollHeight-Math.abs(fill_a);
		GetObj_a("ISL_Cont_a").onmouseover=function(){clearInterval(autoPlayObj_a)}
		GetObj_a("ISL_Cont_a").onmouseout=function(){AutoPlay_a()}
		AutoPlay_a();
		
		function GetObj_a(objName){
			if(document.getElementById){return eval('document.getElementById("'+objName+'")')}else{return eval('document.all.'+objName)}
		}
		
		function AutoPlay_a(){
			clearInterval(autoPlayObj_a);
			autoPlayObj_a=setInterval('ISL_GoDown_a();ISL_StopDown_a();',3000)
		}
		
		function ISL_GoUp_a(){
			if(moveLock_a)return;
			clearInterval(autoPlayObj_a);
			moveLock_a=true;
			moveWay_a="left";
			moveTimeObj_a=setInterval('ISL_ScrUp_a();',speed_a);
		}
		
		function ISL_StopUp_a(){
		if(moveWay_a == "right"){return};
		clearInterval(moveTimeObj_a);
		if((GetObj_a('ISL_Cont_a').scrollTop-fill_a)%PageHeight_a!=0){
		comp_a=fill_a-(GetObj_a('ISL_Cont_a').scrollTop%PageHeight_a);
		CompScr_a()}else{moveLock_a=false}
		AutoPlay_a()}
		
		function ISL_ScrUp_a(){
		if(GetObj_a('ISL_Cont_a').scrollTop<=0){GetObj_a('ISL_Cont_a').scrollTop=GetObj_a('ISL_Cont_a').scrollTop+GetObj_a('List1_a').offsetHeight}
		GetObj_a('ISL_Cont_a').scrollTop-=space_a
		}
		
		function ISL_GoDown_a(){
			clearInterval(moveTimeObj_a);
			if(moveLock_a)return;
			clearInterval(autoPlayObj_a);
			moveLock_a=true;
			moveWay_a="right";
			ISL_ScrDown_a();
			moveTimeObj_a=setInterval('ISL_ScrDown_a()',speed_a)
		}
		
		function ISL_StopDown_a(){
			if(moveWay_a == "left"){return};
			clearInterval(moveTimeObj_a);
			if(GetObj_a('ISL_Cont_a').scrollTop%PageHeight_a-(fill_a>=0?fill_a:fill_a+1)!=0){
				comp_a=PageHeight_a-GetObj_a('ISL_Cont_a').scrollTop%PageHeight_a+fill_a;
				CompScr_a()
			}
			else{moveLock_a=false}
			AutoPlay_a()
		}
		
		function ISL_ScrDown_a(){
		if(GetObj_a('ISL_Cont_a').scrollTop>=GetObj_a('List1_a').scrollHeight){GetObj_a('ISL_Cont_a').scrollTop=GetObj_a('ISL_Cont_a').scrollTop-GetObj_a('List1_a').scrollHeight}
		GetObj_a('ISL_Cont_a').scrollTop+=space_a
		}
		
		function CompScr_a(){
			if(comp_a==0){moveLock_a=false;return}
			var num,TempSpeed=speed_a,TempSpace=space_a;
			if(Math.abs(comp_a)<PageHeight_a/2){
				TempSpace=Math.round(Math.abs(comp_a/space_a));
				if(TempSpace<1){TempSpace=1}
			}
			if(comp_a<0){
				if(comp_a<-TempSpace){
					comp_a+=TempSpace;
					num=TempSpace
				}
				else{num=-comp_a;comp_a=0}
				GetObj_a('ISL_Cont_a').scrollTop-=num;
				setTimeout('CompScr_a()',TempSpeed)
			}	
			else{if(comp_a>TempSpace){comp_a-=TempSpace;num=TempSpace}
			else{num=comp_a;
			comp_a=0}
			GetObj_a('ISL_Cont_a').scrollTop+=num;
			setTimeout('CompScr_a()',TempSpeed)}
		}
		</script>
