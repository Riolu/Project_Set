				img_url = '<a target="_blank" href="'+result[o].url+'"><img src="'+img+'"></a>';
			}else{
				img_url = '<img src="'+img+'">';
			}

			var sub_str = '';
			if(result[o].is_sub){
				sub_str = '<a class="zz_sxhbtn zz_sxhbtn_ok" id="subscribe_category" rel="'+result[o].term_id+'" href="javascript:void(0);">已订阅</a>';
			}else{
				sub_str = '<a class="zz_sxhbtn" id="subscribe_category" rel="'+result[o].term_id+'" href="javascript:void(0);">+ 订阅</a>';
			}
			
			  resultstr+='<div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 sxh_one">' + is_recommend + 
			  '<div>'+ img_url + 
				'<a target="_blank" href="'+result[o].url+'"><p class="zz_sxhtxt textflow">'+title+'</p></a>'+
