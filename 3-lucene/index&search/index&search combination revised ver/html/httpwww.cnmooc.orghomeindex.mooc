$(function(){
    //获取语言
    $(".lang_option").click(function(){
        var lang = $(this).attr("lang");
        if (!lang) return;
        var currLang = $(".lang-info").attr("lang");
        if (currLang == lang) return;
        $.ajax({
            type:'post',
            data:{
                lang:lang
            },
            url:CONTEXTPATH+"/home/setLanguage.mooc",
            success:function(result){
                if (result.returnCode=="success"){
                    location.reload();
                }
            }
        })
    })
})
</script>

