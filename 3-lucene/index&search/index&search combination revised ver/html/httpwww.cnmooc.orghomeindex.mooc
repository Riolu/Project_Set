<script type="text/javascript">
$(".to-down").click(function(){
    var $target=$(this).parent();
    $("html,body").animate({
        scrollTop:$target.offset().top
    },500);
});

$(function() {
	//海报效果
	var advertiseCount = $("#advertiseCount").val();
	if (advertiseCount>1){
		$('#slides').slidesjs({
			 navigation: false,
			 height:460,
			 pagination: {
				  active: true,
				  effect: "fade"
			},
			effect:{
				fade:{
					speed: 1000
				} 
			},
			play: {
				  active: true,
				  effect: "fade",
				  interval: 4000,
				  auto: true,	
				  swap: true,	
				  pauseOnHover: true,
				  restartDelay: 2500
			}
		});
	}
	
	var coursePageCount = '8';
	if (coursePageCount>1){
		$('.video-slides').slidesjs({
			navigation:{
				active:true
			},
			pagination:{
				active:false
			},
			effect:{
				slide:{
					speed: 1500
				}
			},
			play: {
				 active:true,
				 effect: "slide",
				 interval: 5000,
				 auto: true,
				 swap: true,
				 pauseOnHover: true,
				 restartDelay: 2500
			}
		});
	}
	//左右滚动按钮触发的事件
	$(".slidesjs-next,.slidesjs-previous").click(function(){
		//启动自动滚动
		$(".video-slides .slides-item").each(function() {
				var parent = $(this).find(".view-show");
				if (parent.attr("current") == "true") {
					var playerId = parent.attr("playerId");
					var localImage = parent.attr("localImage");
					try {
						jwplayer(playerId).pause();
						jwplayer(playerId).destroy(); //销毁播放器
					}catch(e) {}

					var html = '<div class="video-box" id="'+playerId+'" ><img src="'+CONTEXTPATH+localImage+'"><div class="video-play-shadow viedoPlay"><i class="icon-player"></i></div></div>';
					parent.html(html);
					$(".viedoPlay").click(videoPlay);
					parent.attr("current","false");
                    $(".slidesjs-play").click();
				}
		});
	});

	var videoPlay = function(){
		var $this = $(this).parents(".view-show");
		$this.attr("current", "true");
		var playerId = $this.attr("playerId");
		var localImage = $this.attr("localImage");
		var resourcePath = $this.attr("resourcePath");
        var localPcVideo = $this.attr("localPcVideo");
        var mediaHost = $this.attr("mediaHost");
        var inSchool = $this.attr("inSchool");
        initPreviewPlay(playerId, localImage, localPcVideo, 320, 180, resourcePath, true, mediaHost, inSchool);
      //幻灯片停止自动滚动
		$(".slidesjs-stop").click();
	}
	//点击播放
	$(".viedoPlay").click(videoPlay);

	//更多教师加载
	$(".moreTeacher").click(function(){
		var totalCount = parseInt($(this).attr("totalCount"));
		var currentNum = parseInt($(this).attr("currentNum"));
		$.ajax({
			type:'post',
			data:{
				beginIndex:currentNum
			},
			url:CONTEXTPATH+"/home/getMoreTeacher.mooc",
			success:function(result){
				$(".st-item:last").parent().append(result);
				var total = parseInt($("#currentNum").val());
				$(".moreTeacher").attr("currentNum",total);
				$("#currentNum").remove();
				if (totalCount<=total){
					$(".newest-more").addClass("publicTip");
				}
			}
		})
	})
})

</script>











