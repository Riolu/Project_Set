var s = [ {'videoname':spAdd1},
		{'videoname':spAdd2},
		{'videoname':spAdd3},
		{'videoname':spAdd4},
		{'videoname':spAdd5},
		{'videoname':spAdd6},
		{'videoname':spAdd7},
		{'videoname':spAdd8},
		{'videoname':spAdd9}
		 ];

var n=0;

function VideoSwitch() {
	this.show = function(m){
		G_PLAYER_INSTANCE.close();
		if(s[m-1]!='is undefined'){
			G_PLUGIN_MOVIE.playBysUrl(s[m-1].videoname,true);
		}
	};
}

function change(src){
	var vs = new VideoSwitch();
	n = src;
	vs.show(src);
}

function kkPlayerLoaded(){
	var spAdd1 = $("#spAdd span").eq(0).html();
	var spPic1 = $.trim($("#spAdd2").html());
	G_PLUGIN_MOVIE.setFrontImg(spPic1);
	G_PLUGIN_MOVIE.playBysUrl(spAdd1);
	G_PLUGIN_MOVIE.addCallBack("onEnd",function(){
		if(n > 0 && n < s.length + 1){
			G_PLAYER_INSTANCE.close();
			G_PLUGIN_MOVIE.playBysUrl(s[n].videoname,true);
		}
		if(n == 0){
			G_PLAYER_INSTANCE.close();
			G_PLUGIN_MOVIE.playBysUrl(s[0].videoname,true);
		}
	});
	if(getParameter("vid")){
		G_PLUGIN_MOVIE.playSvodById(getParameter("vid"),false);
	}else if(getParameter("url")){
		G_PLUGIN_MOVIE.playSvodByUrl(getParameter("url"),false);
	}else if(getParameter("gcid")){
		G_PLUGIN_MOVIE.playSvodByGcid(getParameter("gcid"),false);
	}
}
</script>
