<script>
($('followBtn')||{}).onclick=function(){
    var _this = this;
     MI.follow('vip', this, function(isFollow) {
        UI.hide(_this);
        UI.show(UI.after(UI.html('<span class="gb_foed gb_fo1" id="unfollowBtn"><i class="icon icon_sfo"></i><span class="t">已收听</span></span>')[0], _this));
