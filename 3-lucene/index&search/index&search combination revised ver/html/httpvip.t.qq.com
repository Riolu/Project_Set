function disabForm(){
    UI.A($('vsug'),'disabled',true);
    UI.each($$('#vCheck input'),function(input,i){
        UI.A(input,'disabled',true);
    });
}
($('voteBtn')||{}).onclick=function(){
    var vmsg = $('vmsg');
    var data = {};
    var pass,_this=this;
    UI.each($$('#vCheck input'),function(input,i){
        data['item'+(i+1)]=+ input.checked  ;
        if(!pass&& input.checked){
            pass = true;
        }
    });
    data.vsug = $('vsug').value;
    if(!pass&& data.vsug.match(/\S/)){
        pass = true;
    }
    if(!pass){
        MI.alert('您最想要的会员特权是什么?<br/>现在就去选择或填写');
        return;
    }
    if(data.vsug.length>300){
        vmsg.innerHTML='字数过长!';
        return;
    }
    UI.A(_this,'disabled',true);
    MI.ajax({
        url:"/vote/submit",
        data:data,
        success:function(data){
            data = MI.json(data);
            if (data.res===true) {
                _this.value=('已投票');
                disabForm();
                vmsg.innerHTML='提交成功！';
            }else{
                vmsg.innerHTML = (data.res);
                if(data.auth){_this.removeAttribute('disabled');}
                else{disabForm();};
            }
        },
        fail:function(){
            _this.removeAttribute('disabled');
        }
    });
};
