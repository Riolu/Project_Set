<script type="text/javascript">
	function previmg(){
		var pt = 11;
		var pn = 1;
		if (pn == 1)
			alert("已经是第一张了");
		else {
			pn = pn - 1;
			location.href = "http://media.sjtu.edu.cn/photo!list.do?cid=7&pn="+pn+"&isshow=2";
		}
	}
	function nextimg(){
		var pt = 11;
		var pn = 1;
		if (pt <= pn)
			alert("已经是最后一张了");
		else {
			pn = pn + 1;
			location.href = "http://media.sjtu.edu.cn/photo!list.do?cid=7&pn="+pn+"&isshow=1";
		}
	}
</script>
<script type="text/javascript">
	$(function(){ $(".mimg").autoZoomLoadImage(true, 320, 200); })
	$(document).ready(function(){
		$(".imgbox").hover(function(){
			$(this).addClass('over');
		},function(){
			$(this).removeClass('over');
		});
		$('#imgul a').lightBox();
		var isshow = 1;
		if(isshow == 1)
			$('#img_0').click();
		else if(isshow == 2)
			$('#img_8').click();
	});
</script>
