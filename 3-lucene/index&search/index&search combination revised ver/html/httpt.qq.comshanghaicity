btnBox=$$('.mblistenBtn');
UI.ready(function(){
	UI.each(btnBox,function(o,i){
		o.onclick=function(){
			if (!MI.user.account){
				mb_quick_reg(this,'');
			}
			else {
				var check = $$(this.parentNode.parentNode,'.listenerList li .headFollow'),arr = [];
				for (var i = 0,num = check.length;i < num;i++){
					if(UI.A(check[i],'account')){
						arr.push(UI.A(check[i],'account'));  //兼容投放的广告账户格式不一致
					}
					else{
						arr.push(check[i].href.match(/[^\/]+$/g)[0]);
					}
				}
				if (arr.length) {
					MI.follow(arr.join(','),document.body,function(){
						if (!MI.user.fun.followList || arr.length > 1) {
							MI.tip('收听成功');
							UI.each($$(o.parentNode.parentNode,'.listenerList .attentBoxWrap'),function(o,i){
								$$(o, 'a')[0].style.display = 'none';
								$$(o, '.disabled')[0].style.display = 'block';
							})
						}

					});
				}
				else {
					MI.alert('请选择要收听的人');
				}
			}
			return false;
		}
	});

});</script>
<script type="text/javascript">
var adjustHeight = function(){
    var colSub = UI.GC("div.col-sub")[0];
    var colMain = UI.GC("div.col-main")[0];
    if(!colSub) return;
    if(!colMain) return;
    var talk = UI.GC(colMain, "div.AL" )[0];
    if(!talk) return;
    var delta = UI.height(colSub) - UI.height(colMain);
    if(delta > 0){
        talk.style.height = delta + UI.height(talk) + 'px';
    }
    var colSubMain = UI.GC("div.inner-main")[0];
    var colSubExtra = UI.GC("div.inner-extra")[0];
    if(!colSubMain) return;
    if(!colSubExtra) return;
    var delta = UI.height(colSubExtra) - UI.height(colSubMain);
    if(delta > 0){
        talk.style.height = delta + UI.height(talk) + 'px';
    }
};
UI.EA(window,'size',function(){
    adjustHeight();
});
</script>
