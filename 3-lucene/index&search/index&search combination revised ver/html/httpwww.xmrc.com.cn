<script type="text/javascript">
    $(function () {

        $(".webMenu .homepage").each(function () {
            $(this).removeClass("homepage");
        });

        $(".webMenu .img").each(function () {
            $(this).attr("src", "/net/images/menubg2.gif");
        });

        var hasAddClass = false;


        $(".webMenu a").each(function () {
            if (hasAddClass) return;


            var menuhref = $(this).attr("href").replace("http://", "");
            var menuurl = menuhref.substring(0, menuhref.lastIndexOf("/")).toLowerCase();
            var href = window.location.href.toLowerCase();
            if (menuurl != "" && href.indexOf(menuurl) > -1) {

                //人才中心
                if ((href.indexOf('intro.aspx') > -1 || href.indexOf('introduction.aspx') > -1 || href.indexOf('sector.aspx') > -1) && menuhref == '/net/wcm/') {
                    return;
                }
                else if (href.indexOf('students.aspx') > -1 && menuhref == '/net/wcm/') {
                    return;
                }
                else if ((href.indexOf('intro.aspx') == -1 && href.indexOf('introduction.aspx') == -1 && href.indexOf('sector.aspx') == -1 && menuhref == '/net/wcm/intro.aspx') ||
                (href.indexOf('students.aspx') == -1 && menuhref == '/net/wcm/Students.aspx?cid=105')) {
                    return;
                }
               

                

                

                $(this).parent("td").addClass("homepage");

                var nextTd = $(this).parent("td").next("td");
                if (nextTd.length > 0) {
                    nextTd.eq(0).children("img").each(function () {
                        $(this).attr("src", "/net/images/menubg1.gif");
                    });
                }

                hasAddClass = true;
                
            }
        });



        if (!hasAddClass) {
            $("#homepage0").addClass("homepage");
            $("#homepage1").addClass("homepage");
            $("#homepage2").attr("src", "/net/images/menubg1.gif");
            
        }

    });
</script>

