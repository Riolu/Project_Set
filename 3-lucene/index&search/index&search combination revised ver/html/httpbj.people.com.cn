<script type="text/javascript">
$(function(){
	var index = 0;
	var timer;
	var len = $("#pic_list li").length;
	
	for(i=0;i<len;i++)
	{
		$("#num_list").html($("#num_list").html()+"<li>"+"</li>");
	}
	
	$("#num_list li").click(function(){
		index = $("#num_list li").index(this);
		showPic(index);
	}).eq(0).click();
	
	$("#box").hover(function(){
		clearInterval(timer);
	},function(){
		timer = setInterval(function(){
			showPic(index);
			index++;
			if(index==len){index=0;}
		},3000)
	}).trigger("mouseleave");
	
	function showPic(index){
		$("#num_list li").removeClass("active").eq(index).addClass("active");
		$("#pic_list li").eq(index).stop(true,true).fadeIn(600).siblings("li").fadeOut(600);
	};
});
$(function(){
	var len = $("#pic_list01 li").length;
	var index = 0;
	var timer;
	
	if($("#num_list01 li"))
	{
		//for(i=0;i<len;i++)
		//{
			$("#num_list01 .pic_list01 ul").html($("#pic_list01").html());
		//}
	}
	
	$("#num_list01 li i").remove();
	$("#num_list01 li a").removeAttr("href");
	//alert($("#num_list").html())
	
	$("#num_list01 li").click(function(){
		index = $("#num_list01 li").index(this);
		showPic(index);
	}).eq(0).click();
	
	$("#box01").hover(function(){
		clearInterval(timer);
	},function(){
		timer = setInterval(function(){
			showPic(index);
			index++;
			if(index==len){index=0;}
		},4000)
	}).trigger("mouseleave");
	function showPic(index){
		$("#num_list01 li").removeClass("active").eq(index).addClass("active");
		$("#pic_list01 li").eq(index).stop(true,true).fadeIn(500).siblings("li").fadeOut(500);
		//alert(index);
		var num1=$("#pic_list01 li").length-2;
		var num=(index-1)*-90;
		if(index>=2&&index<=num1){
		$(".pic_list01 ul").animate({marginTop:num});
			}
		if(index<2){
			$(".pic_list01 ul").animate({marginTop:0});
		}
		if(index>num1){
			$(".pic_list01 ul").animate({marginTop:num+90});
		}
		

	};
	$(".up").click(function(){
		if(index>0&&index<=$("#pic_list01 li").length){
		index=index-1;
		showPic(index);
		var num=(index-1)*-90;
	if(index!=0){
		$(".pic_list01 ul").animate({marginTop:num});
	}
			}
			else{
			index=$("#pic_list01 li").length-1
			showPic(index);
			}

	})
	$(".down").click(function(){
		if(index<$("#pic_list01 li").length-1){
		index=index+1;
		showPic(index);
		}
		else{
			index=0;
			showPic(index);
			}
	})

});
</script>


