	for (var i = 0,num = check.length;i < num;i++){
				if(UI.A(check[i],'account')) arr.push(UI.A(check[i],'account'));  //兼容投放的广告账户格式不一致
				else arr.push(check[i].href.match(/[^\/]+$/g)[0]);
