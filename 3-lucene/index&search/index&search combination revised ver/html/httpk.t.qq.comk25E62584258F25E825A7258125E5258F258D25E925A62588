<script type="text/javascript">

var OW = {
	go:function() {
		if($('chkOW').checked) {
			window.open('http://t.qq.com/openwall/wall_control.php?key=%E6%84%8F%E8%A7%81%E5%8F%8D%E9%A6%88');
		} else {
			window.open('http://t.qq.com/topic_wall.php?key=%E6%84%8F%E8%A7%81%E5%8F%8D%E9%A6%88');
		}
		this.hide();
	},
	show:function() {
		var wallLayer = $('wallLayer');
		var backgroundLayer = $('backgroundLayer');
		wallLayer.parentNode.style.display='block';
		backgroundLayer.style.display='block';
		wallLayer.style.display='block';
		var de=document.documentElement;
		wallLayer.style.top = (de.clientHeight-wallLayer.clientHeight)/2+(de.scrollTop|document.body.scrollTop)+'px';
		wallLayer.style.left = (de.clientWidth-wallLayer.clientWidth)/2+de.scrollLeft+'px';
		backgroundLayer.style.width = de.scrollWidth+'px';
		backgroundLayer.style.height = de.scrollHeight+'px';
	},
	hide:function() {
		var wallLayer = $('wallLayer');
		var backgroundLayer = $('backgroundLayer');
		wallLayer.parentNode.style.display='none';
		backgroundLayer.style.display='none';
		wallLayer.style.display='none';
	}
};

function operateTopic(id,el){
	isDel = UI.hasClass(el,'cancelTopic');
	type = isDel?'2':'1';
	MI.ajax({
		url : MI.url.topicFollow,
		data : {tid:id,type:type,r:MI.random()},
		success : function(data){
			data = MI.json(data);
			if(data.result == 0) {
				el.className = isDel ? 'submitTopic' : 'cancelTopic';
				el.innerHTML = isDel ? '+订阅此话题' : '-取消订阅此话题';
				if(isDel)
					MI.Bos({id:1158,name:'btnUnTopicCol',sBak1:$('topic').value.split('#')[1]});
				else
					MI.Bos({id:1158,name:'btnTopicCol',sBak1:$('topic').value.split('#')[1]});
			}
		}
	});
}

UI.ready(function(){
    var topicName = '意见反馈';
    MI.FollowBtn.followCall = function() {
        MI.Bos({
            name : 'btnHeadFollow',
            sBak1 : topicName,
            id : 1158
        });
		MI.feedBack({name:'topic',id:1191,iPos:72,iBak1:3,sBak1:''});
    }
    MI.FollowBtn.unFollowCall = function() {
    MI.Bos({
        name : 'btnHeadUnfollow',
        sBak1 : topicName,
        id : 1158
       });
		MI.feedBack({name:'topic',id:1191,iPos:72,iBak1:8,sBak1:''});
    }
    UI.each(UI.GC(".foFun"), function(o) {
        var re = "/addAttention/i";
        if (-1 != o.className.search(re)) {
            UI.EA(o, 'click', function() {
               MI.Bos({
                name : 'btnHeadFollowText',
                sBak1 : topicName,
                id : 1158
               });
            });
        }
    })
