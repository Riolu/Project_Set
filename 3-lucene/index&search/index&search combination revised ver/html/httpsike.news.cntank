//分页
function page_str(pagenum,total_count,type){

	var str = '';

	var pagenum = parseInt(pagenum);
	var total_count = parseInt(total_count);
	
	if(total_count > 1 && pagenum != 1){
		str += '<a href="javascript:;" class="click_page_'+type+'" rel = "'+(pagenum-1)+'">&lt;&nbsp;</a>';
	}
	
	for (var i=1; i<= total_count; i++)
	{
		if(i == pagenum){
			str += "<a href='javascript:;' class='click_page_"+type+" now' rel = "+i+">"+i+"</a>";
		}else{
			if(((pagenum-i) < 3 && (pagenum-i) > 0) 
					|| ((i-pagenum) < 3 && (i-pagenum) > 0)){
				str += "<a href='javascript:;' class='click_page_"+type+"' rel = "+i+">"+i+"</a>";
			}else if(i==1
					|| i==total_count){
				str += "<a href='javascript:;' class='click_page_"+type+"' rel = "+i+">"+i+"</a>";
			}else if((pagenum-i) == 3 || (i-pagenum) == 3){
				str += "...";
			}
		}
	}
	if(total_count > 1 && pagenum != total_count ){
		str += '<a href="javascript:;" class="click_page_'+type+'" rel = "'+(pagenum+1)+'">&nbsp;&gt;</a>';
	}
	return str;
}

