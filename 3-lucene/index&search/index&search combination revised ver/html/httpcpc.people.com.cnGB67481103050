<script type="text/javascript">
function disa(){
	var winWidth ;
    var winHeight ;
	var ly_a=document.getElementById("liuyan_a");
	var ly_b=document.getElementById("liuyan_b");

             if (window.innerWidth)
                   winWidth = window.innerWidth;
             else if ((document.body) && (document.body.clientWidth))
                   winWidth = document.body.clientWidth;
            
	ly_a.style.display="block";
	ly_b.style.display="block";
	ly_b.style.width=winWidth+"px";
	ly_b.style.height=2200+"px";
	//alert(winHeight);
	}
function closea(){
	  document.getElementById("liuyan_a").style.display="none";
	  document.getElementById("liuyan_b").style.display="none";
	}

</script>
