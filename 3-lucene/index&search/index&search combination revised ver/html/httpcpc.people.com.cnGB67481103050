<script type="text/javascript">
		var speed_1 = 10; //速度(毫秒)
		var space_1 = 10; //每次移动(px)
		var PageHeight_1 = 284; //翻页宽度
		var fill_1 = 0; //整体移位
		var moveLock_1m = false;
		var moveTimeObj_1;
		var moveWay_1m = "down";
		var comp_1m = 0;
		var autoPlayObj_1m=null;
		
		GetObj("List_m2").innerHTML=GetObj("List_m1").innerHTML;
		GetObj('Cont_m').scrollTop=fill_1>=0?fill_1:GetObj('List_m1').scrollHeight-Math.abs(fill_1);
		GetObj("Cont_m").onmouseover=function(){clearInterval(autoPlayObj_1m)}
		GetObj("Cont_m").onmouseout=function(){AutoPlay_1m()}
		AutoPlay_1m();

		function GetObj(objName){
			if(document.getElementById){return eval('document.getElementById("'+objName+'")')}else{return eval('document.all.'+objName)}
		}
		
		function AutoPlay_1m(){
			clearInterval(autoPlayObj_1m);
			autoPlayObj_1m=setInterval('GoDown_m();StopDown_m();',3000)
		}
		
		function GoUp_m(){
			if(moveLock_1m)return;
			clearInterval(autoPlayObj_1m);
			moveLock_1m=true;
			moveWay_1m="up";
			moveTimeObj_1=setInterval('ScrUp_m();',speed_1);
		}
		
		function StopUp_m(){
		if(moveWay_1m == "down"){return};
		clearInterval(moveTimeObj_1);
		if((GetObj('Cont_m').scrollTop-fill_1)%PageHeight_1!=0){
		comp_1m=fill_1-(GetObj('Cont_m').scrollTop%PageHeight_1);
		CompScr_1m()}else{moveLock_1m=false}
		AutoPlay_1m()}
		
		function ScrUp_m(){
		if(GetObj('Cont_m').scrollTop<=0){GetObj('Cont_m').scrollTop=GetObj('Cont_m').scrollTop+GetObj('List_m1').offsetHeight}
		GetObj('Cont_m').scrollTop-=space_1
		}
		
		function GoDown_m(){
			clearInterval(moveTimeObj_1);
			if(moveLock_1m)return;
			clearInterval(autoPlayObj_1m);
			moveLock_1m=true;
			moveWay_1m="right";
			ScrDown_m();
			moveTimeObj_1=setInterval('ScrDown_m()',speed_1)
		}
		
		function StopDown_m(){
			if(moveWay_1m == "left"){return};
			clearInterval(moveTimeObj_1);
			if(GetObj('Cont_m').scrollTop%PageHeight_1-(fill_1>=0?fill_1:fill_1+1)!=0){
				comp_1m=PageHeight_1-GetObj('Cont_m').scrollTop%PageHeight_1+fill_1;
				CompScr_1m()
			}
			else{moveLock_1m=false}
			AutoPlay_1m()
		}
		
		function ScrDown_m(){
		if(GetObj('Cont_m').scrollTop>=GetObj('List_m1').scrollHeight){GetObj('Cont_m').scrollTop=GetObj('Cont_m').scrollTop-GetObj('List_m1').scrollHeight}
		GetObj('Cont_m').scrollTop+=space_1
		}
		
		function CompScr_1m(){
			if(comp_1m==0){moveLock_1m=false;return}
			var num,TempSpeed=speed_1,TempSpace=space_1;
			if(Math.abs(comp_1m)<PageHeight_1/2){
				TempSpace=Math.round(Math.abs(comp_1m/space_1));
				if(TempSpace<1){TempSpace=1}
			}
			if(comp_1m<0){
				if(comp_1m<-TempSpace){
					comp_1m+=TempSpace;
					num=TempSpace
				}
				else{num=-comp_1m;comp_1m=0}
				GetObj('Cont_m').scrollTop-=num;
				setTimeout('CompScr_1m()',TempSpeed)
			}	
			else{if(comp_1m>TempSpace){comp_1m-=TempSpace;num=TempSpace}
			else{num=comp_1m;
			comp_1m=0}
			GetObj('Cont_m').scrollTop+=num;
			setTimeout('CompScr_1m()',TempSpeed)}
		}
		</script>
