			  var speed=18
			  demo_b2.innerHTML=demo_b1.innerHTML
			  function Marquee(){
			  if(demo_b2.offsetWidth-demo_b.scrollLeft<=0)
			  demo_b.scrollLeft-=demo_b1.offsetWidth
			  else{
			  demo_b.scrollLeft++
			  }
			  }
			  var MyMar=setInterval(Marquee,speed)
			  demo_b.onmouseover=function() {clearInterval(MyMar)}
			  demo_b.onmouseout=function() {MyMar=setInterval(Marquee,speed)}
