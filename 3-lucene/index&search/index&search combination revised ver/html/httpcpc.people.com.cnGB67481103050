<script language="javascript">
function submitBbs(formbbs){
	var baseurl="http://comments.people.com.cn/bbs_new/app/src/main/?action=parse_publish&op=ins&channel_id=64036&nid=";
	var nid=null;
	var cont=formbbs.content.value;
	if(cont==null||cont==""){
		alert("请输入留言内容");
		return false;
	}
	if(formbbs.bbstype[0].checked){//个人身份
		nid=formbbs.select5.value;
		if(nid==null||nid<=0){
			alert("请选择所在城市");
			return false;
		}else{
			formbbs.action=baseurl+nid;
			formbbs.yonghuming.value=formbbs.yonghuming1.value;
			if(formbbs.yonghuming.value=='不署名则默认为匿名发表')formbbs.yonghuming.value="";
			formbbs.yonghuming1.value="";
			formbbs.yonghuming2.value="";
			
			return true;
		}
	}else if(formbbs.bbstype[1].checked){//党组织(单位)身份
		nid="http://comments.people.com.cn/bbs_new/app/src/main/?action=parse_publish&op=ins&channel_id=64036&bid=9754015";
		if(formbbs.yonghuming2.value=='请填写党组织名称'||formbbs.yonghuming2.value==''){
			alert("请填写党组织名称");
			return false;
		}else{
			formbbs.action=nid;
			formbbs.yonghuming.value=formbbs.yonghuming2.value;
			return true;
		}
	}
	return false;
}

function checkBbs(cityselect){
	var baseurl="http://comments.people.com.cn/bbs_new/app/src/main/?action=list&channel_id=64036&nid=";
	var baseurl1="http://comments.people.com.cn/bbs_new/app/src/main/?action=list&channel_id=64036&uid=";
	var nid=null;
	nid=cityselect.value;
	if(formbbs.bbstype[1].checked){
		window.open("http://comments.people.com.cn/bbs_new/app/src/main/?action=list&nid=103731");
		}
	else if(nid==null||nid<=0){
		alert("请选择所在城市");
		return false;
	}else{
		if(cityselect.selectedIndex==0)
			window.open(baseurl1+nid);
		else 
			window.open(baseurl+nid);
	}
}

function initSelect(){
initSelectArea(document.all.formbbs.select4);
initSelectArea(document.all.selecta);
initSelectArea(document.all.selectc);
}

function mail(){
document.all.mail.href='mailto:'+ document.all.UserName22.value + '?subject=人民网十七大特别报道：对党说句心里话' + '&body=' + 'http://cpc.people.com.cn/GB/67481/103050/';
}
function copyToClipBoard(){
var clipBoardContent="";
clipBoardContent+=document.title;
clipBoardContent+="|";
clipBoardContent+=this.location.href;
window.clipboardData.setData("Text",clipBoardContent);
alert("复制成功，您可以将其粘贴到QQ/MSN上的好友啦！");
}
</script>
