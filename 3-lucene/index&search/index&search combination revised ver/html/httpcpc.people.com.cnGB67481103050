function postMsg(){
 if (document.form1.content.value==null || document.form1.content.value=="" ) {
  alert("发言内容不能为空！");
  return;
 }
  else {
  document.form1.submit();
 }
}
function renew_parent(form,parent_token)
{
 form.pid.value=parent_token;
}
function checkboard(){
	var res = true;
	if (form1.content.value==null || form1.content.value=="") {
		alert("发言内容不能为空！");
		res = false;
	}
	return res;
}
</script>
