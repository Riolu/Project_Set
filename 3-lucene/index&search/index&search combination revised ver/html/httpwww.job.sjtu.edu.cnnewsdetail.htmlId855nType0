		                		var myDate=new Date()
		                		document.getElementById("myDate").innerHTML=myDate.getFullYear()+"年"+(myDate.getMonth()+1)+"月"+myDate.getDate()+"日";
		                		var day =myDate.getDay();
		                		var week ="";
		                		if(day==0){
		                			week ="星期日"
		                		}
		                		if(day==1){
		                			week ="星期一"
		                		}
		                		if(day==2){
		                			week ="星期二"
		                		}
		                		if(day==3){
		                			week ="星期三"
		                		}
		                		if(day==4){
		                			week ="星期四"
		                		}
		                		if(day==5){
		                			week ="星期五"
		                		}
		                		if(day==6){
		                			week ="星期六"
		                		}
		                		document.getElementById("myWeek").innerHTML=week;
		                	</script>
