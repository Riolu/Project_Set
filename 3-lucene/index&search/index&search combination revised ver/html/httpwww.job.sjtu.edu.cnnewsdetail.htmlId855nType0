                	function queryInfo(){
                		var strTitle = document.getElementById("strTitle").value;
                		if(strTitle!=null && strTitle.length>0){
                			var url = "/news/result.htm?strTitle=" + strTitle;
							url = encodeURI(url).replace(/#/g, '%23');
							window.open(url);
                		}
                	}
