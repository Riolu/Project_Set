                  <a href="${idea.path}">
                    ${idea.title}
                  </a>
                </li>
              {{/each}}
            </ul>
            {{if more_suggestions}}
              <p class="uvSearch-more-results"><a href="/search?filter=ideas&query=${response_data.query}">More ideas &rarr;</a></p>
            {{/if}}
          </script>

