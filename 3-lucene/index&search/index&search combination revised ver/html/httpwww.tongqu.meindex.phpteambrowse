    <script>
        "use strict";
        var g_url = {};
        g_url.site_url = function (url) {
            var site_url = "http://www.tongqu.me/index.php";// http://tongqu.me/index.php
            // ltrim the '/'
            if (url.indexOf('/') === 0) {
                url = url.substr(1);
            }
            return site_url + '/' + url;
        };

        g_url.base_url = function (url) {
            var base_url = "http://www.tongqu.me/";// http://tongqu.me/
            if (url.indexOf('/') === 0) {
                url = url.substr(1);
            }
            return base_url + url;
        };

    </script>

