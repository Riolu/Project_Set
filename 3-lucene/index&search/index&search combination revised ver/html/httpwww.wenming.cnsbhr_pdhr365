<script type="text/javascript">
    $(function () {
        $('#bigImg').attr('src', $('#demo-01 li:eq(0) .pannel_img img').attr('src'));
		$('#demo-01 ul li:eq(0) .pannel_img').addClass('pannel_Cimg');
        $("#demo-01 ul li").click(function () {
			$('.pannel_Cimg').removeClass('pannel_Cimg');
			$(this).find('.pannel_img').addClass('pannel_Cimg');
            $('#bigImg').attr({ 'src': $(this).find('img').attr('src') });
            $('#bigImghref').attr('href', $(this).find('.title a').attr('href'));
            $('#imgtxt').html($(this).find('.title').html());
        });
        $("#demo-01").jCarouselLite({ btnPrev: "#prev-01", btnNext: "#next-01",auto:3000,circular:true,visible: 3, scroll: 3, speed: 5000 });
		
		$(".panel").hide();
		  $(".flip").click(function(){
			$(".panel").slideToggle();
		 });
    });
</script>
<script language="javascript">
 $(function(){
  $('.box').hover(function(){
    $(this).addClass('box-a01');  
   },function(){
    $(this).removeClass('box-a01'); 
   }
  );
  $('.box01').hover(function(){
    $(this).addClass('box-a01');  
   },function(){
    $(this).removeClass('box-a01'); 
   }
  );

  	var maxH, contentObj , currH;
	$(".myclick").click(function () {
	   contentObj = $(this).siblings('.myhidden');
	   maxH = contentObj.find('.content').height();
       currH = contentObj.height();
            if (currH + 1100 > maxH) {
                currH = maxH;
                $(this).remove();
            }
            else {
                currH += 1100;
            }
            contentObj.height(currH);
     });
  
 });
</script>


