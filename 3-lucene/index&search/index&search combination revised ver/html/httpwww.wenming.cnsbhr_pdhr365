			if(_nCurrIndex==i){
				document.write("<span class=\"action_num\">"+(i+1)+"</span>&nbsp;");
			}
			else if(i==0){
				document.write("<a href=\""+_sPageName+"."+_sPageExt+"\">"+(i+1)+"</a>&nbsp;");
			}
			else{
				document.write("<a href=\""+_sPageName+"_" + i + "."+_sPageExt+"\">"+(i+1)+"</a>&nbsp;");
