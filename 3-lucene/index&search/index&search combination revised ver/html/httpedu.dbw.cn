function sideGuide(){
var cWidth = document.body.clientWidth,
guide = pc.getElem('#jSideGuide');
var JMark = pc.getElem("#part1");
if(!JMark) return;
var _ot = JMark.getBoundingClientRect().top;
if(_ot <= 0 && cWidth > 1200){
pc.end(guide);
pc.show(guide);
pc.fadeTo(guide, 1, 100, function(){});
}else{
pc.end(guide);
pc.hide(guide);
pc.fadeTo(guide, 0, 100, function(){});
}
if(!pc.browser.ie6){
guide.style.marginTop = -guide.clientHeight / 2 + 'px'
}
}
window.onscroll = window.onresize = function(){
sideGuide();
}
