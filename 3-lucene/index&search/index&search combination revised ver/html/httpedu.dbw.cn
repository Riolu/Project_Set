<script>
var curCity=pc.getCookie("PCity");
function loadDefaultData() {
var defaultCity="全国";
window.IPCallBack = function(d){
curCity = d.city;
pc.setCookie('PCity', curCity, 1);
callbackFn_ip();
}
if(!curCity){
pc.getScript('#');
}else{callbackFn_ip();} 
}
function callbackFn_ip(){
var cityData=[{
area:"北京",
intData:"#",
url:"#"
},
{
area:"上海",
intData:"#",
url:"#"
},
{
area:"广州",
intData:"#",
url:"#"
},
{
area:"全国",
intData:"#",
url:""
}];
for(i=0;i<cityData.length;i++){
var subCity=curCity.slice(0,2);
if(subCity==cityData[i].area){
loadTg(cityData[i].intData);
if(i<cityData.length-1&&pc.getElems("#ipArea a").length==6){
var domA=pc.getElems("#ipArea a")[4];
var nElement = document.createElement("a");
nElement.href=cityData[i].url;
nElement.target="_blank";
nElement.className="aNav";
nElement.innerHTML=subCity;
pc.insertAfter(domA, nElement);
}
}else{
loadTg(cityData[3].intData)
}   
}
}       
pc.ready(function(){
pc.addEvent(document.body, 'click', function(e){
thisObj = document.all?event.srcElement:e.target;
do{
if(thisObj.className.indexOf("ks-inTxt")>=0){ return};
if(thisObj.className.indexOf("ks-inBtn")>=0){ return};
if(thisObj.tagName.toUpperCase() == "BODY"|| thisObj.tagName.toUpperCase() == 'HTML'){document.getElementById('formKs').className=' ';return;};
thisObj = thisObj.parentNode;
}while(thisObj.parentNode);
});
//设备判断给杂志不同链接
if(document.getElementById("JappStore")){
var ua = navigator.userAgent.toLowerCase()
if(ua.indexOf("android") > -1 || ua.indexOf("iphone") > -1 || ua.indexOf("ipod")>-1){
document.getElementById("JappStore").href = "#" 
}else{
document.getElementById("JappStore").href = "#"     
} 
}
});
function loadTg(url) {
var head = document.getElementsByTagName("head")[0];
var script = document.createElement("script");
script.src = url ;
script.onload = script.onreadystatechange = function(){
var f=script.readyState;
if(f&&f!="loaded"&&f!="complete") return;
if (window.PCHOUSE_data_areatg) {
document.getElementById('cityData').innerHTML = PCHOUSE_data_areatg;
}
};
head.appendChild(script);
}
loadDefaultData();
