<script language="javascript">

function openthis(a){
    var b=getElement(a);
    if(b.selectedIndex && b.selectedIndex!=0)
	{
	   window.open(b.value);
	   b.selectedIndex=0;
    }else{
	   alert("请选择一项行业报告!");}
}
 
