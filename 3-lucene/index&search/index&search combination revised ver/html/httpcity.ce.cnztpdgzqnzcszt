<script type="text/javascript"> 
 
 
 
//<![CDATA[
 
function pic_run_string_trim(str){
    return str.replace(/(^\s*)|(\s*$)/g, '');
}
 
function pic_run_add_one_info(a_src, a_href, a_title, target_blank){
	var dom_ul = $$$$("pic_run_idSlider2");
	if(dom_ul){
		var dom_li = dom_ul.appendChild(document.createElement("li"));
		if(dom_li){
			var dom_a = dom_li.appendChild(document.createElement("a"));
			if(dom_a){
				dom_a.href = a_href;
				if(target_blank){
					dom_a.target = "_blank";
				}else{
					dom_a.target = "_self";
				}
				var dom_img = dom_a.appendChild(document.createElement("img"));
				if(dom_img){
					dom_img.src = a_src;
					dom_img.alt = a_title;
				}
			}
		}
	}
}
 
var pic_run_html_list = new Array();
var pic_run_html_list_title = new Array();
var pic_run_html_list_info = new Array();
 
if($$$$("pic_run_html_list_id")){
	var pic_run_list_li = $$$$("pic_run_html_list_id").getElementsByTagName("li");
	if(pic_run_list_li){
		for(var i=0; i<pic_run_list_li.length; i++){
			var list_span = pic_run_list_li[i].getElementsByTagName("span");
			if(list_span){
				if(list_span.length > 3){
					var one_info = new Array();
					for(var j=0; j<list_span.length; j++){
						one_info[j] = pic_run_string_trim(list_span[j].innerHTML);
					}
					pic_run_html_list.push(one_info);
				}
			}
		}
	}
}
 
for(var i=0; i<pic_run_html_list.length; i++){
	var one_info = pic_run_html_list[i];
	if(one_info.length > 3){
		//alert(one_info[0] + "|" + one_info[1]+ "|" + one_info[2]);
		var ahref_begin = '<a target="_blank" href="' + one_info[1] + '">';
		var ahref_end = '</a>';
		pic_run_add_one_info(one_info[0], one_info[1], one_info[2], true);
		pic_run_html_list_title.push(ahref_begin + one_info[2] + ahref_end);
		pic_run_html_list_info.push(one_info[3] + ahref_begin  + ahref_end);
	}
}
 
var pic_run_nums = [], pic_run_timer, pic_run_n = $$$$("pic_run_idSlider2").getElementsByTagName("li").length,
	pic_run_st = new pic_run_SlideTrans("pic_run_idContainer2", "pic_run_idSlider2", pic_run_n, {
		onStart: function(){//设置按钮样式
			pic_run_forEach(pic_run_nums, function(o, i){ 
				o.className = pic_run_st.Index == i ? "ononon" : ""; 
				//设置新闻标题
				if($$$$("pic_run_span_title")){
					var nowindex = parseInt(pic_run_st.Index);
					if(nowindex < pic_run_html_list_title.length){
						$$$$("pic_run_span_title").innerHTML = pic_run_html_list_title[nowindex];
						$$$$("pic_run_span_info").innerHTML = pic_run_html_list_info[nowindex];
					}else{
						$$$$("pic_run_span_title").innerHTML = "";
						$$$$("pic_run_span_info").innerHTML = "";
					}
				}
			})
		}
	});
for(var i = 1; i <= pic_run_n; pic_run_AddNum(i++)){};
function pic_run_AddNum(i){
	var num = $$$$("pic_run_idNum").appendChild(document.createElement("li"));
	num.innerHTML = i--;
	num.onmouseover = function(){
		pic_run_timer = setTimeout(function(){ num.className = "ononon"; pic_run_st.Auto = false; pic_run_st.Run(i); }, 200);
	}
	num.onmouseout = function(){ clearTimeout(pic_run_timer); num.className = ""; pic_run_st.Auto = true; pic_run_st.Run(); }
	pic_run_nums[i] = num;
}
pic_run_st.Run();
 
 
//]]>
</script>
 
