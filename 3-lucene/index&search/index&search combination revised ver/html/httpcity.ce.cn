<script type="text/javascript">

function scrollDoor() {}
scrollDoor.prototype = {
	sd: function(menus, divs, openClass, closeClass) {
		var _this = this;
		if (menus.length != divs.length) {
			alert("菜单层数量和内容层数量不一样!");
			return false;
		}
		for (var i = 0; i < menus.length; i++) {
			_this.$(menus[i]).value = i;
			_this.$(menus[i]).onmouseover = function() {
				for (var j = 0; j < menus.length; j++) {
					_this.$(menus[j]).className = closeClass;
					_this.$(menus[j]).getElementsByTagName("span")[0].style.display="none";
					_this.$(divs[j]).style.display = "none";
				}
				_this.$(menus[this.value]).className = openClass;
				_this.$(menus[this.value]).getElementsByTagName("span")[0].style.display="block";
				_this.$(divs[this.value]).style.display = "block";
			}
		}
	},
	$: function(oid) {
		if (typeof(oid) == "string") return document.getElementById(oid);
		return oid;
	}
}
window.onload = function() {
	var SDmodel = new scrollDoor();
 SDmodel.sd(["bt1","bt2"],["nr1","nr2"],"sd01","sd02");
 
}
</script>









<script type="text/javascript">
	$(document).ready(function(){		
		var showpic=1;
		var index=0;
		var picnum=$("#piclist li").length;
		var limit=picnum-showpic;
		var picwidth=$("#piclist li").width();
		var range=parseInt($("#piclist li").css("margin-right"));		
		var picorder=0;		
		var currpic=0;
		
		var timer=setInterval(bpicShow,8000);
		
		
		$("#preUp").click(function(){
			clearInterval(timer);
			if(picorder>0)
			{
				picorder=picorder-1;
			}
			else
			{
				picorder=picnum-1;
			}
			
			$("#bpiclist li").eq(picorder).fadeIn(500);
			$("#bpiclist li").eq(picorder).siblings().fadeOut(300);
			if(picorder<=limit)
			{
				index=picorder;
				
			}
			else{index=limit
			}
			clearInterval(timer);
			$("#piclist li").eq(picorder).addClass("on").siblings().removeClass("on");
			
		})
		$("#nextUp").click(function(){
			clearInterval(timer);
			bpicShow();
		
		})
		
		
		
		function bpicShow(){
			if(picorder<(picnum-1))
			{
				picorder++;
			}
			else
			{
				picorder=0;
			}
			
			$("#bpiclist li").eq(picorder).fadeIn(500);
			$("#bpiclist li").eq(picorder).siblings().fadeOut(800);
			$("#piclist li").eq(picorder).addClass("on").siblings().removeClass("on");
			
			if(picorder<=limit)
			{
				index=picorder;
			}
		}
		
		$("#piclist li").each(function(i){
			$(this).click(function(){
				$(this).addClass("on").siblings().removeClass("on");
				clearInterval(timer);
				picorder=i-1;
				bpicShow();
				timer=setInterval(bpicShow,2000);
			})
		})
	})
    </script>
    
    




 

