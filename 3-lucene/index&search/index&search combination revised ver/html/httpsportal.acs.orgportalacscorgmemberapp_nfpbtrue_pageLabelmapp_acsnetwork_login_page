	<title>ACS Network: Unsupported Browser</title>


<script>
    if (top != self) {
        var isSafeTopUrl = false;
        try {
            var topUrl = top.location.href;
            var topUrlQsIdx = topUrl.indexOf('?');
            if (topUrlQsIdx > 0) {
                topUrl = topUrl.substring(0, topUrlQsIdx);
            }
            topUrl = topUrl.toLowerCase();
            var safeTopUrls = new Array( 'https://communities.acs.org/theme-site.jspa', 'https://communities.acs.org/skin-advanced.jspa', 'https://communities.acs.org/skin-advanced!UploadSkinZip.jspa' );
            for (var i = 0; i < safeTopUrls.length; i++) {
                if (safeTopUrls[i].toLowerCase() == topUrl) {
                    isSafeTopUrl = true;
                    break;
                }
            }
        } catch (err) {
        }
        if (!isSafeTopUrl) {
            top.location = location;
        }
    }
</script>

