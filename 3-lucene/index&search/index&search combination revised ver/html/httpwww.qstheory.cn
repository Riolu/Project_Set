//	},5000);
		$("#hd2left").click(function(){
			if(np2>0){
				np2--;
				nl2=-pWidth2*np2;	
				$("#hdi2").animate({left:nl2+"px"},"slow");	
			}
		});
		$("#hd2right").click(function(){
			if(np2<(pCount2-1)){
				np2++;
				nl2=-pWidth2*np2;	
				$("#hdi2").animate({left:nl2+"px"},"slow");	
			}
		});
});
</script>
