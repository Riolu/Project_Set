<script>

var JPlaceHolder = {
    //检测
    _check : function(){
        return 'placeholder' in document.createElement('input');
    },
    //初始化
    init : function(){
        if(!this._check()){
            this.fix();
        }
    },
    //修复
    fix : function(){
        jQuery(':input[placeholder]').each(function(index, element) {
            var self = $(this), txt = self.attr('placeholder');
            self.wrap($('<div></div>').css({position:'relative', zoom:'1', border:'none', background:'none', padding:'none', margin:'none'}));
            var pos = self.position(), h = self.outerHeight(true), paddingleft = self.css('padding-left');
            var holder = $('<span></span>').text(txt).css({position:'absolute', left:pos.left, top:pos.top, height:h, lienHeight:h, paddingLeft:paddingleft, color:'#aaa'}).appendTo(self.parent());
            self.focusin(function(e) {
                holder.hide();
            }).focusout(function(e) {
                if(!self.val()){
                    holder.show();
                }
            });
            holder.click(function(e) {
                holder.hide();
                self.focus();
            });
        });
    }
};
//执行
jQuery(function(){
    JPlaceHolder.init();    
});

</script>

<script>
var homeurl = 'http://sike.news.cn';
var ajaxurl = '/ajax-recv.php';
var tbase = 'http://sike.news.cn/resources/themes/sike';
var current_user_id = '0';

$.fn.extend({
	setCursorPosition: function (position) {
		if (this.lengh == 0) return this;
		return $(this).setSelection(position, position);
	},
	setSelection: function (selectionStart, selectionEnd) {
		if (this.lengh == 0) return this;
		input = this[0];

		if (input.createTextRange) {
			var range = input.createTextRange();
			range.collapse(true);
			range.moveEnd('character', selectionEnd);
			range.moveStart('character', selectionStart);
			range.select();
		} else if (input.setSelectionRange) {
			input.focus();
			input.setSelectionRange(selectionStart, selectionEnd);
		}

		return this;
	},
	focusEnd: function () {
		this.setCursorPosition(this.val().length);
	}
});

function showMsgBox(content, action, hidebox) {
	$('#popup_msg_content').html(content);
	toggleWindow('popup_error_box', hidebox);

	if (action.ok) {
		$('#popup_msg_ok').show();
		$('#popup_msg_ok').die().live('click', action.ok);
	}
	if (action.cancel) {
		$('#popup_msg_cancel').show();
		$('#popup_msg_cancel').die().live('click', action.cancel);
	}
}

function pageName() {
	var a = location.href;
	var b = a.split("/");
	var c = b.slice(b.length-1, b.length).toString(String).split("?");
	return c.slice(0, 1);
}

//解决页面下方多出background问题
function resolve_background() {
	if($("#totop").height() > $(".bgall").height()){
		$("body").css("height",$(".bgall").height());
	}
}

function MoveBox(obj) {
	var divTop = $(obj).offset().top;
	var divLeft = $(obj).offset().left;
	$(obj).css({ 
		// "position": "absolute", 
		"z-index": "500", 
		"left": divLeft + "px", 
		"top": divTop + "px",
		"border": "1px solid #F00"
	});
	$(obj).animate({ 
		"left": ($("#collectBoxCont").offset().left - $("#collectBoxCont").width()) + "px", 
		"top": ($(document).scrollTop() + 30) + "px", 
		"width": "100px", 
		"height": "70px" 
	}, 500, function () {
		$(obj).animate({ 
			"left": $("#collectBoxCont").offset().left + "px", 
			"top": $("#collectBoxCont").offset().top + "px" 
		}, 500).fadeTo(0, 0.1).hide(0);
	});
}

jQuery(document).ready(function ($) {
	
	function init() {
		var bgall = $('.bgall');
		var bgallOffset = bgall.offset();
		bgall.css('min-height', $(window).height() - bgallOffset.top);
		var clientWidth = document.body.clientWidth; //获取当前窗口的宽度
		var isMove = clientWidth >= width; //宽度是否大于500
		if (isMove) { //如果宽度大于500，监听滚动事件
			if(pname == 'tank' || pname == 'tanker' || pname == 'theme'){
				var nav = $('#navbar');
				nav.removeClass('nav1,navadd,navbar-static-top').addClass('nav2,navbar-fixed-top');
				
				var logo_zk = 'http://sike.news.cn/resources/themes/sike' + '/img/logozk.png';
				$("title").html('思客-智库');
				$("#navmain_sike_tank").show();
				$("#navmain_sike").hide();
				$("#logo-image").attr("src",logo_zk);
				$("#logo-image").parent().attr("href",'http://sike.news.cn/tank');
			}else{
				var nav = $('#navbar');
				if (nav.hasClass('nav2')) {
					nav.removeClass('nav2');
					nav.addClass('nav1');
					nav.addClass('navadd');
					nav.removeClass('navbar-fixed-top');
					nav.addClass('navbar-static-top');
					$('#logo-image').attr('src', tbase + '/img/logo1.png');
				}
			}

			if (typeof ($('#rank-list').attr('class')) == 'undefined') {
				return false;
			}
			//设置最右侧的div为fixed
			var rankList = $('#rank-list'); //获取推读div对象，替换文档时候注意此ID
			var atrsJoin = $('#atrs_join'); //获取参与我们div对象，替换文档时候注意此ID
			var rankListTop = $(rankList).offset().top; //获取推读div距离文档头的高度
			var atrsJoinTop = $(atrsJoin).offset().top - rankListTop; //获取参与我们和推读排行之间的距离
			$(window).bind('scroll', menu_bar = function() {
				var scrollTop = $(window).scrollTop(); //获取滚动条距离文档头的距离
				if (scrollTop > rankListTop) { //判断是否滚动到要监听的div
					//推读排行
					$(rankList).css('position', 'fixed');
					$(rankList).css('top', '0px');
				} else {
					$(rankList).css('position', 'static');
					$(rankList).css('top', '');
				}
			});
			
		} else {
			
			//设置导航为nav2等
			var nav = $('#navbar');
			nav.removeClass('nav1');
			nav.removeClass('navadd');
			nav.addClass('nav2');
			nav.removeClass('navbar-static-top');
			nav.addClass('navbar-fixed-top');
			$('#logo-image').attr('src', tbase + '/img/logo3.png');
			//解决手机端内部跳转问题
			$('a').removeAttr('target');

			if(pname == 'tank' || pname == 'tanker' || pname == 'theme'){
				var logo_zk = 'http://sike.news.cn/resources/themes/sike' + '/img/logozk.png';
				$("title").html('思客-智库');
				$("#navmain_sike_tank").show();
				$("#navmain_sike").hide();
				$("#logo-image").attr("src",logo_zk);
				$("#logo-image").parent().attr("href",'http://sike.news.cn/tank');
			}
		}
	}
	var width = 767;

	$(window).resize(function () {
		init();
	});

	init();

	$('#myTab a').click(function (e) {
		e.preventDefault()
		$(this).tab('show')
	})
	
	$(".mctext").click(function () {
		var kd = $(this).next('.kdlists');
		$(this).siblings().next('.kdlists').slideUp();
		if (kd.is(':visible')) {
			$(this).next('.kdlists').slideUp();
		} else {
			$(this).next('.kdlists').slideDown();
		}
	});

	$("#back-to-top").hide();

	$(window).scroll(function () {
		if ($(window).scrollTop() > 100) {
			$("#back-to-top").fadeIn(1500);
		} else {
			$("#back-to-top").fadeOut(1500);
		}
	});

	$("#backtop").click(function () {
		$('body,html').animate({
			scrollTop: 0
		}, 1000);
		return false;
	});

// 	lazyload
	$('img.lazy').lazyload({
		effect: 'fadeIn'
	});

// 	注册
	$('#register_email, #register_username, #register_password').blur(function() {
		var id = $(this).attr('id');
		var name = $(this).attr('rel');
		var value = $(this).val();
		
		if(name == 'password'){
			value = $.base64.btoa(value);
		}
		
		$.ajax({
			type: 'POST',
			url: 'http://login.home.news.cn/api/sike/regVerify.do',
			data: {
				name: name,
				value: value
			},
			dataType: 'jsonp',
			jsonp: 'jsonpCallback',
			success: function(res) {
				if (res.code == 200) {
					$('#' + id + '_succico').show();
					$('#' + id + '_errico, #' + id + '_tips').hide();
				} else {
					$('#' + id + '_succico').hide();
					$('#' + id + '_errico').show();
					$('#' + id + '_tips').html(res.msg);
					$('#' + id + '_tips').show();
				}
			}
		});
	});
	$('#register_check_password').blur(function() {
		var id = $(this).attr('id');
		var password = $('#register_password').val();
		var check_password = $(this).val();
		if (check_password == '') {
			$('#' + id + '_succico').hide();
			$('#' + id + '_errico').show();
			$('#' + id + '_tips').html('输入的确认密码不能为空');
			$('#' + id + '_tips').show();
			return false;
		}
		if (password == check_password) {
			$('#' + id + '_succico').show();
			$('#' + id + '_errico, #' + id + '_tips').hide();
		} else {
			$('#' + id + '_succico').hide();
			$('#' + id + '_errico').show();
			$('#' + id + '_tips').html('两次输入的密码不一致');
			$('#' + id + '_tips').show();
		}
		return false
	});
	$('#register_new_user').click(function() {
		var username = $('#register_username').val();
		var password = $('#register_password').val();
		var email = $('#register_email').val();
		
		jQuery.ajax({
			type: 'POST',
			url: ajaxurl,
			data: {
				action: 'register_new_user',
				username: username,
				password: password,
				email: email
			},
			dataType: 'json',
			success: function(res) {
				if (res.code == 200) {
					window.location.href = '/';
				} else {
					showMsgBox(res.msg, {
						ok: function() {
							toggleWindow('register_user_box', 'popup_error_box');
							return false;
						},
						cancel: function() {
							toggleWindow('popup_error_box');
							return false;
						}
					}, 'register_user_box');
				}
			}
		});
		return false
	});

// 	登录
	$('#do_login').click(function() {
		var username = $('#login_username').val();
		var password = $('#login_password').val();
		$.ajax({
			type: 'POST',
			url: ajaxurl,
			data: {
				action: 'user_signon',
				username: username,
				password: password
			},
			dataType: 'json',
			success: function(res) {
				if (res.code == 200) {
					location.reload();
				} else {
					toggleWindow('login_msg_box');
					showMsgBox(res.msg, {
						ok: function() {
							toggleWindow('login_msg_box', 'popup_error_box');
							return false;
						},
						cancel: function() {
							toggleWindow('popup_error_box');
							return false;
						}
					}, 'register_user_box');
					$('#popup_msg_cancel').hide();
				}
			}
		});
		return false;
	});

// 	退出登录
	$('#do_logout,#do_logout_small').click(function() {
		var is_remove = $.removeCookie('ak-1');
		$.ajax({
			type: 'POST',
			url: ajaxurl,
			data: {
				action: 'user_signout'
			},
			dataType: 'json',
			success: function(res) {
				$.cookie('is_third_login','1');
				window.location.reload();
			}
		});
		return false;
	});

	// 	用户头像滑动显示
	$("#user_zone_closed,#user_login_nav").hover(function(){
		$('#user_login_nav').show();
	},function(){
		$('#user_login_nav').hide();
	});
	
// 	用户飘浮
	$("div.tx36 img:not(.small_author_pic)").die().live("hover",function(event){
		if(event.type =='mouseenter'){
			$(this).parent().parent().find(".yin_tx").css("margin-top","");
			if($(this).offset().top - $(window).scrollTop() + $(this).parent().parent().find(".yin_tx").height() > $(window).height()){
				var num = $(this).offset().top - $(window).scrollTop() + $(this).parent().parent().find(".yin_tx").height() - $(window).height();
				$(this).parent().parent().find(".yin_tx").css("margin-top",0-num);
			}
			$(".yin_tx").hide();
			if(280 + $(this).offset().left > $(window).width()){
				$(this).parent().parent().find(".yin_tx").css("left",280-$(this).parent().parent().find("#floatview").width()).show();
			}else{
				$(this).parent().parent().find(".yin_tx").show();
			}
			
		}else{
			
		}
	})

	$(".txAndfloat_view,div.tx40").die().live("hover",function(event){
		if(event.type == 'mouseenter'){
			$(this).find(".yin_tx").css("margin-top","");
			if($(this).offset().top - $(window).scrollTop() + $(this).find(".yin_tx").height() > $(window).height()){
				var num = $(this).offset().top - $(window).scrollTop() + $(this).find(".yin_tx").height() - $(window).height();
				$(this).find(".yin_tx").css("margin-top",0-num-60);
			}
			
			$(".yin_tx").hide();
			if(280 + $(this).offset().left > $(window).width()){
				$(this).find(".yin_tx").css("left",280-$(this).find("#floatview").width()).show();
			}else{
				$(this).find(".yin_tx").show();
			}
		}else{
			$(this).find(".yin_tx").hide();
		}
	})
	
	
	//先清除然后添加hover事件
	$(".yin_tx").die().live("hover",function(event){
		if(event.type == 'mouseenter'){
			$(".yin_tx").hide();
			$(this).show();
		}else{
			$(".yin_tx").hide();
		}
	});
	
// 	稍后阅读
	$('.readit, #footer_readit,#marked_small').die().live('click', function () {
		var can_click = true;
		var readit_this = $(this);
		if ($.trim($(this).text()) != '稍后阅读' && $.trim($(this).text()) != '取消收藏' && $.trim($(this).text()) != '已加书签') {
			return false;
		}
		if (!can_click) {
			return false;
		}

		var is_footer = $(this).attr('rel');
		
		can_click = false;
		var postid = $(this).attr('data');
		if ($.trim($(this).text()) == '稍后阅读') {
			jQuery.ajax({
				type: 'POST',
				url: ajaxurl,
				data: {
					action: 'add_bookmark',
					postid: postid
				},
				dataType: 'json',
				success: function (res) {
					can_click = true;
					if (res.code == false) {
						if (res.msg == '您还未登录') {
							showMsgBox('给文章加个书签，有空的时候一气呵成读完才叫畅快。', {
								ok: function () {
									toggleWindow('login_msg_box', 'popup_error_box');
									return false;
								},
								cancel: function () {
									toggleWindow('popup_error_box');
									return false;
								}
							});
							$('#popup_msg_cancel').hide();
							$('#popup_msg_ok').text('我要登录');
						} else {

						}
						return false;
					}
					readit_this.after('<div id="shyd_'+postid+'" style="display:none;color:red;float:left;">书签+1</div>');
					$('#shyd_'+postid).animate({opacity: 'show'}, 400).delay(2000).animate({opacity: 'hide'}, 2000,function() {$('#shyd_'+postid).remove();});

					
					readit_this.html('已加书签');
					if(readit_this.attr('id') == 'marked_small'){
						readit_this.removeClass('marked');
					}else{
						readit_this.addClass('readited');
					}
					
				}
			});
		}

		if ($(this).text() == '取消收藏' || $.trim($(this).text()) == '已加书签') {
			readit_this = $(this);
			jQuery.ajax({
				type: 'POST',
				url: ajaxurl,
				data: {
					action: 'delete_bookmark',
					postid: postid
				},
				dataType: 'json',
				success: function (res) {
					can_click = true;
					if (res.code == false) {
						showMsgBox(res.msg, {
							ok: function () {
								toggleWindow('popup_error_box');
								return false;
							},
							cancel: function () {
								toggleWindow('popup_error_box');
								return false;
							}
						});
						return false;
					}
					readit_this.text('稍后阅读');
					if(readit_this.attr('id') == 'marked_small'){
						readit_this.addClass('marked');
					}else{
						readit_this.removeClass('readited');
					}
				}
			});
		}

		return false;
	});
	
// 	推荐
	$('#fab_button').click(function () {
		if ($('.btn_tjian').hasClass('btn_tjianed')) {
			return false;
		}
		var postid = $(this).attr('data');
		var current_user_id = $(this).attr('current_user_id');
		var count = $("#fab_count").text();
		if (postid == '' || current_user_id == '') {
			return false;
		}
		jQuery.ajax({
			type: 'POST',
			url: ajaxurl,
			data: {
				action: 'add_fab',
				postid: postid,
				current_user_id: current_user_id,
				count: count
			},
			dataType: 'json',
			success: function (res) {
				if (res.code == 'false' || res.code == false) {
					showMsgBox(res.msg, {
						ok: function () {
							toggleWindow('popup_error_box');
							return false;
						},
						cancel: function () {
							toggleWindow('popup_error_box');
							return false;
						}
					});
					$("#popup_msg_cancel").remove();
				} else {
					$('#fab_count, #fab_count1').html(res.code);
					$('.btn_tjian').addClass('btn_tjianed');
					$('#fab_button h4').text("已推荐");
				}

			}
		});
	});

	$('#fab_button h4').click(function() {
		if($(this).text() == '已推荐') {
			showMsgBox("您已推荐过此文", {
				ok: function() {toggleWindow('popup_error_box'); return false;},
				cancel: function() {toggleWindow('popup_error_box'); return false;}
			});
		}
	});
	
	$('#img').attr('src',tbase + '/img/logo1.png');
	var m_st, m_po= 1;
	var m_tpot=1;
	var	pname =pageName();
	if(pname=='handwriting'||
		pname=='order'||
		pname=='comment'||
		pname=='recommend'||
		pname=='contribute'||
		pname=='ctrbtcheck'||
		pname=='ctrbtdraft'||
		pname=='ctrbtbin'||
		pname=='useredit'||
		pname=='search'||
		pname=='subject-article'||
		pname=='404'||
		pname == 'tank' || 
		pname == 'tanker' || 
		pname == 'theme'){
		$("#navbar").removeClass('navbar-static-top nav1 navadd')
		.addClass('navbar-fixed-top nav2');
		$('#logo-image').attr('src', tbase + '/img/logo3.png'); 
		$('#user_login_nav').removeClass().addClass("dropdown-menu");
	}
	<!--if(pname=='subject') $(".rightcon").css("top","382px");临时 谷歌浏览器-->
	$(window).scroll(function () {
	var	pname =pageName();

	if(pname == 'tank' || pname == 'tanker' || pname == 'theme')
		return false;
	
	m_st = Math.max(document.body.scrollTop || document.documentElement.scrollTop);
	document.getElementById("navbar").style.display = 'block';
	if (m_st > m_po) {
		if(m_tpot>m_st){
			$('#img').attr('src', tbase + '/img/logo3.png'); 
			$('#user_login_nav').removeClass().addClass("dropdown-menu");
			$("#navbar").removeClass('navbar-static-top nav1 navadd').addClass('navbar-fixed-top nav2');
			$("#logo-image").attr('src', tbase + '/img/logo3.png');
		}
		else{
			$('#navbar').hide();
		}
	} else {
		if(pname=='handwriting'||
				pname=='order'||
				pname=='comment'||
				pname=='recommend'||
				pname=='contribute'||
				pname=='ctrbtcheck'||
				pname=='ctrbtdraft'||
				pname=='ctrbtbin'||
				pname=='useredit'||
				pname=='search'||
				pname=='subject-article'||
				pname=='404'||
				pname == 'tank' || 
				pname == 'tanker' || 
				pname == 'theme'
					){
			$("#navbar").removeClass('navbar-static-top nav1 navadd').addClass('navbar-fixed-top nav2');
			$('#logo-image').attr('src', tbase + '/img/logo3.png'); 
			$('#user_login_nav').removeClass().addClass("dropdown-menu");
		}
		else{
			var clientWidth = document.body.clientWidth; //获取当前窗口的宽度
			if(clientWidth > width){
				$("#navbar").removeClass('navbar-fixed-top nav2').addClass('navbar-static-top nav1 navadd')
				$('#logo-image').attr('src', tbase + '/img/logo1.png'); 
				$('#user_login_nav').removeClass().addClass("dropdown-menu");
			}
		}
	}
	m_tpot = m_st;
	
	});
	
	$('.ipt_serh').keydown(function(e){
		if(e.keyCode==13){
		   var search = $('.ipt_serh').val();
		   search = encodeURIComponent(search);
		   if($.trim($('.ipt_serh').val()) != '' &&
				   $.trim($('.ipt_serh').val()) != null){

			   var url =  "http://"+window.location.host+"/search?search="+encodeURIComponent(search);
			   window.location.href=url;
			   return false;
		   }else{
			   showMsgBox("请输入搜索词!", {
					ok: function() {toggleWindow('popup_error_box'); return false;},
					cancel: function() {toggleWindow('popup_error_box'); return false;}
				});
		   }
		}
	});

	$('.btn_serh').click(function(){
	   var search = $('.ipt_serh').val();
	   search = encodeURIComponent(search);
	   if($.trim($('.ipt_serh').val()) != '' &&
			   $.trim($('.ipt_serh').val()) != null){
		   var url =  "http://"+window.location.host+"/search?search="+encodeURIComponent(search);
		   window.location.href=url;
		   return false;
	   }else{
		   showMsgBox("请输入搜索词!", {
				ok: function() {toggleWindow('popup_error_box'); return false;},
				cancel: function() {toggleWindow('popup_error_box'); return false;}
			});
	   }
	});

