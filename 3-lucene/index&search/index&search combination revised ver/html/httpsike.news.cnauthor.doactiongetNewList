  $(function(){
   var browser = {
              versions: function(){
                  var u = navigator.userAgent;
                  var app = navigator.appVersion;
                  var uu = navigator.appName;
                  
                  return {
                      ios: !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/),
                      android: u.indexOf('Android') > -1 || u.indexOf('Linux') > -1, 
                      iPad: u.indexOf('iPad') > -1, 
                      weixin: u.indexOf('MicroMessenger') > -1,
                      ie6: uu === "Microsoft Internet Explorer" && u.indexOf("MSIE 6.0") > 0,
                      ie7: uu === "Microsoft Internet Explorer" && u.indexOf("MSIE 7.0") > 0,
                      ie8: uu === "Microsoft Internet Explorer" && u.indexOf("MSIE 8.0") > 0,
                     
                  };
              }()
    };


