<script type="text/javascript">


	$(document).ready(function(){
		//add author page
		var author_pagenum = 1;

		var total_count = 5;
		
		var str = page_str(author_pagenum,total_count,'author');
		
		if(5 > 1){
			$("#author_pagenum").append(str);
		}

		var authorpagenum = 6;
		
		$(".click_page_author").live('click',function(){

			var author_this = $(this);
			var authorcurrentnum = $(this).attr('rel');
			
			$.ajax({ 
				type: 'POST',
				url: ajaxurl,
				data: {
					action: 'get_recommend_author',
					authorpagenum :authorpagenum,
					authorcurrentnum :authorcurrentnum
				},
				dataType: 'json',
				success: function(data) {
 	 			   $(".click_page_author").removeClass("now");
					author_this.addClass("now");
 	 			   $(".recommend_author").html(data);

 	 			 var str = page_str(authorcurrentnum,total_count,'author');
 	 			$("#author_pagenum").html(str);
//  	 			$("#author_pagenum").append(str);
				}
			});
		
		});

