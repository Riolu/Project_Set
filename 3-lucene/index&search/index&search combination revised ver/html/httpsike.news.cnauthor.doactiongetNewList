<script type="text/javascript" src="http://image.sike.news.cn/resources/themes/sike/js/touchScroll.js"></script>
<script type="text/javascript" src="http://image.sike.news.cn/resources/themes/sike/js/touchslider.dev.js"></script>
<script>
function editor(){
// 	document.getElementById("readers").className='';
// 	document.getElementById("editor_chie").className='now';
// 	document.getElementById("subscription").className='';

	$('#readers').removeClass('now');
	$('#editor_chie').addClass('now');
	$('#subscription').removeClass('now');
	
	$('.editor').show();
	$('.reader').hide();
	$('.subscription').hide();
	$('#sub_btn_more').hide();
}
function reader(){
// 	document.getElementById("editor_chie").className='';
// 	document.getElementById("readers").className='now';
// 	document.getElementById("subscription").className='';

	$('#editor_chie').removeClass('now');
	$('#readers').addClass('now');
	$('#subscription').removeClass('now');

	$('.reader').show();
	$('.editor').hide();
	$('.subscription').hide();
	$('#sub_btn_more').hide();
}
function subscribe(){
// 	document.getElementById("editor_chie").className=''
// 	document.getElementById("readers").className=''
// 	document.getElementById("subscription").className='now';

	$('#editor_chie').removeClass('now');
	$('#subscription').addClass('now');
	$('#readers').removeClass('now');

	$('.reader').hide();
	$('.editor').hide();
	$('.subscription').show();
	$('#sub_btn_more').show();
}

</script>
<script type="text/javascript">
     var active=0,
	as = $('#pagenavi a');

	//var t1=new TouchScroll({id:'wrapper','width':5,'opacity':0.7,color:'#555',minLength:20});

    var t2=new TouchSlider({id:'slider', speed:600, timeout:6000, before:function(index){
		as[active].className='';
		active=index;
		as[active].className='active';
	}});

	for(var i=0;i<as.length;i++){	
		(function(){
			var j=i;
			as[i].onclick=function(){
				t2.slide(j);
				return false;
			}
		})();


		//alert(as[i].innerHTML);
	}


  var active1=0,
	as1 = $('#pagenavi1 a');

	var t3=new TouchSlider({id:'slider1', speed:600, timeout:6000, before:function(index){
			as1[active1].className='';
			active1=index;
			as1[active1].className='active';
		}});


	for(var i=0;i<as1.length;i++){
		//alert(as[i].html());

		(function(){
			var j=i;
			as1[i].onclick=function(){
				t3.slide(j);
				return false;
			}
		})();
	}
	
</script> 
<script type="text/javascript">
$(document).ready(
	function () {
		$('#smgz').click(function(){	
			if(document.getElementById("yin_ewm").style.display=="block"){
				$(".yin_ewm").css("display","none");
			}
			else{
				$(".yin_ewm").css("display","block");
			}
		});


		var is_login =  '';
		
		$('#footer_bookmark').click(function(){
			if(is_login){
				var url =  "http://"+window.location.host+"/handwriting";
				 window.location.href=url;
			}else{
				showMsgBox('您尚未登录，请登录后进行操作', {
					ok: function () {
						toggleWindow('login_msg_box', 'popup_error_box');
						return false;
					},
					cancel: function () {
						toggleWindow('popup_error_box');
						return false;
					}
				});
			}
		});
		
		
	})
	

</script>
<script type="text/javascript" src="http://image.sike.news.cn/resources/themes/sike/js/bootstrap.min.js"></script>
