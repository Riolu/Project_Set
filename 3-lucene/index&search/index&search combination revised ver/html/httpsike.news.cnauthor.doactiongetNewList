 <script type="text/javascript">
$("img.lazy").lazyload();  

category_load_next = true;
word_load_next = true;

function get_categories() {
	var categories = new Array();
			categories.push('497');
			categories.push('496');
			categories.push('499');
			categories.push('498');
			categories.push('492');
			categories.push('494');
			categories.push('493');
			categories.push('501');
			categories.push('678');
			categories.push('623');
			categories.push('500');
			categories.push('495');
		categories = categories.reverse();
	return categories;
}

function get_words() {
	var words = new Array();
			words.push('');
			words.push('A');
			words.push('B');
			words.push('C');
			words.push('D');
			words.push('E');
			words.push('F');
			words.push('G');
			words.push('H');
			words.push('J');
			words.push('K');
			words.push('L');
			words.push('M');
			words.push('N');
			words.push('O');
			words.push('P');
			words.push('Q');
			words.push('R');
			words.push('S');
			words.push('T');
			words.push('W');
			words.push('X');
			words.push('Y');
			words.push('Z');
		words = words.reverse();
	return words;
}

function autobrowse_category(categories) {
	var categories_length = categories.length - 1;
	var current_category = categories.pop();
	$('#category_author_cont_body').autobrowse({
		url: function (offset) {
			if (category_load_next) {
				return ajaxurl;
			} else {
				return false;
			}
		},
		postData: function() {
			var current_category = categories.pop();
			data = {
				action: 'fetch_category_authors',
				term_id: current_category,
			}
			return data;
		},
		template: function (response) {
			return response.content;
		},
		complete: function(response) {
			return false;
		},
		itemsReturned: function (response) {
			return 1;
		},
		useCache: false,
		max: categories_length,
	});
}

function autobrowse_words(words) {
	var words_length = words.length - 1;
	var current_word = words.pop();
	$('#word_author_cont_body').autobrowse({
		url: function (offset) {
			if (word_load_next) {
				return ajaxurl;
			} else {
				return false;
			}
		},
		postData: function() {
			var current_word = words.pop();
			data = {
				action: 'fetch_word_authors',
				first_char: current_word,
			}
			return data;
		},
		template: function (response) {
			return response.content;
		},
		complete: function(response) {
			return false;
		},
		itemsReturned: function (response) {
			return 1;
		},
		useCache: false,
		max: words_length,
	});
}

$(function() {

	var categories = get_categories();
	var words = get_words();
	
	$('#category_index_liuwenhao_btn').click(function() {
		category_load_next = true;
		$(this).addClass('now');
		$('#word_index_liuwenhao_btn').removeClass('now');
		$('.categoryliu').show();
		$('.wordliu').hide();
		$('#category_author_cont_body').show();
		$('#word_author_cont_body').hide();

// 		$('#category_tab').children().first().click();
		
// 		categories = get_categories();
// 		autobrowse_category(categories);
	});

	$('#word_index_liuwenhao_btn').click(function() {
		word_load_next = true;
		$(this).addClass('now');
		$('#category_index_liuwenhao_btn').removeClass('now');
		$('.wordliu').show();
		$('.categoryliu').hide();
		$('#category_author_cont_body').hide();
		$('#word_author_cont_body').show();

// 		var current_status = $(this).hasClass('now');
// 		$('#word_tab').children().first().click();
	});

	autobrowse_category(categories);
	autobrowse_words(words);
	
	$('#switch_author_category').die().live('click', function() {
		var term_id = $(this).attr('data');
		$.ajax({
			type: 'POST',
			url: ajaxurl,
			data: {
				action: 'fetch_category_authors',
				term_id: term_id
			},
			dataType: 'json',
			success: function(res) {
				category_load_next = false;
				$('#category_author_cont_body').html(res.content);
			}
		});
		return false;
	});

	$('#switch_author_char').die().live('click', function() {
		var first_char = $(this).attr('data');
		$.ajax({
			type: 'POST',
			url: ajaxurl,
			data: {
				action: 'fetch_word_authors',
				first_char: first_char
			},
			dataType: 'json',
			success: function(res) {
				word_load_next = false;
				$('#word_author_cont_body').html(res.content);
			}
		});
		return false;
	});
});
</script>
