	});

	// 手机端显示
	$("#user_zone_closed_small,#user_login_nav_small").toggle(function(){
		$('#user_login_nav_small').show();
		$("#dropdown_small").addClass("open");
	},function(){
		$('#user_login_nav_small').hide();
		$("#dropdown_small").removeClass("open");
	});

	//解决手机端跳转问题
	$("#user_login_nav_small li a").click(function(){
		window.location.href=$(this).attr("href");
	});

	$('#login_username, #login_password').keydown(function(e){
		if(e.keyCode==13){
			$('#do_login').click();
		}
	});

	if(pname == 'tank' || pname == 'tanker' || pname == 'theme'){

		var logo_zk = 'http://sike.news.cn/resources/themes/sike' + '/img/logozk.png';
		$("title").html('思客-智库');
		$("#navmain_sike_tank").show();
		$("#navmain_sike").hide();
		$("#logo-image").attr("src",logo_zk);
		$("#logo-image").parent().attr("href",'http://sike.news.cn/tank');
	}

	//订阅推荐作者
	$("#subscribe_recommend_author").live('click',function () {
		
		var _this = $(this);
		var author_id = $(this).attr('data');
		
		if($(this).hasClass('zz_sxhbtn_ok')){
			return false;
		}
		
		$.ajax({
			type: 'POST',
			url: ajaxurl,
			data: {
				action: 'add_Subscription',
				author_id: author_id,
				user_id: current_user_id
			},
			dataType: 'json',
			success: function(res) {
				if(res == true){
					_this.text("已订阅").addClass("zz_sxhbtn_ok");
				}else{
					showMsgBox('属于你的思想邮箱，订阅内容、重磅文章一个都不会少。', {
						ok: function () {
							toggleWindow('login_msg_box', 'popup_error_box');
							return false;
						},
						cancel: function () {
							toggleWindow('popup_error_box');
							return false;
						}
					});
					$('#popup_msg_cancel').hide();
					$('#popup_msg_ok').text('我要登录');
				}
			}
		});
		return false;
	});
	
})

//分页
function page_str(pagenum,total_count,type){

	var str = '';

	var pagenum = parseInt(pagenum);
	var total_count = parseInt(total_count);
	
	if(total_count > 1 && pagenum != 1){
		str += '<a href="javascript:;" class="click_page_'+type+'" rel = "'+(pagenum-1)+'">&lt;&nbsp;</a>';
	}
	
	for (var i=1; i<= total_count; i++)
	{
		if(i == pagenum){
			str += "<a href='javascript:;' class='click_page_"+type+" now' rel = "+i+">"+i+"</a>";
		}else{
			if(((pagenum-i) < 3 && (pagenum-i) > 0) 
					|| ((i-pagenum) < 3 && (i-pagenum) > 0)){
				str += "<a href='javascript:;' class='click_page_"+type+"' rel = "+i+">"+i+"</a>";
			}else if(i==1
					|| i==total_count){
				str += "<a href='javascript:;' class='click_page_"+type+"' rel = "+i+">"+i+"</a>";
			}else if((pagenum-i) == 3 || (i-pagenum) == 3){
				str += "...";
			}
		}
	}
	if(total_count > 1 && pagenum != total_count ){
		str += '<a href="javascript:;" class="click_page_'+type+'" rel = "'+(pagenum+1)+'">&nbsp;&gt;</a>';
	}
	return str;
}

