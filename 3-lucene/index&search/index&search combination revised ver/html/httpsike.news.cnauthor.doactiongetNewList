<script>
function toggleWindow(windowID, windowHide){
	if (windowHide) {
		$('#' + windowHide).hide();
	}
// 	$('#' + windowID).toggle();
	$('#' + windowID).toggle(0, function() {
// 		$('input').placeholder();
	});
	return false;
}

</script>

<script>
function toQzoneLogin()
{
	var protocol = window.location.protocol;
	var host = window.location.host;
	var A=window.open(protocol + "//" +host+"/resources/mods/sike-third/qq/oauth/qq_login.php","TencentLogin","width=450,height=320,menubar=0,scrollbars=1, resizable=1,status=1,titlebar=0,toolbar=0,location=1");
} 
</script>

