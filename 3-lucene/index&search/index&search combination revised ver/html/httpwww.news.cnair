        var scrollAdLiLen = $_scrollAdLi.length;
        var str = "";
        if(scrollAdLiLen==1){
            $("#productBody .scrollAd .btn").hide();
        }
        for(i=0;i<scrollAdLiLen;i++){
            str+='<li></li>';
        };
        $(".scrollAd .hd").append(str);
        $_scrollAdLi.show();
