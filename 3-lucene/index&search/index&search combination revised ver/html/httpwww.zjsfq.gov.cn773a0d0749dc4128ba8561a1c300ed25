    $(".js-font-size").click(function(e) {
      e.preventdefault();
      $(".content-body").removeclass("small").removeclass("big").addclass($(this).data("size"));
      $.ajax({ url: "/fontsize", data: { size: $(this).data("size") }, method: "post" });
    });

    $.each($('.content-body img'), function() {
      var width = $(this).width();
      var height = $(this).height();
      var imgsrc = $(this).attr('src');
      if ($(this).width() > 668) {
        $(this).width(668);
        $(this).height(height * 668 / width);
        $(this).click(function() {
          $.magnificpopup.open({
            items: { src: imgsrc },
            type: 'image'
          }, 0);
        });
      }
    });

