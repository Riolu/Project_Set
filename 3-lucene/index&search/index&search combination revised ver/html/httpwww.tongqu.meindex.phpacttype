</div><div class="wrap" ng-controller="ActTypeCtrl" ng-cloak>
    <div class="user-option center-content">
        <div>
            <span class="title">
                分类：
            </span>
            <span class="item" ng-repeat="type in types" ng-class="{active:($parent.type.typeid==type.typeid)}"
                  ng-click="changeType(type)" ng-bind="type.typename">
            </span>
        </div>
        <div>
            <span class="title">
                状态：
            </span>
            <span class="item" ng-repeat="status in statuses|orderBy:'time_status'"
                  ng-class="{active:($parent.status.time_status==status.time_status)}"
                  ng-click="changeStatus(status)" ng-bind="status.status_name">
            </span>
        </div>
    </div>
    <div class="sort-bar">
        <div class="center-content">
            <div class="col-xs-1">
                <i class="fa fa-sliders bar-icon"></i>
            </div>
            <div ng-repeat="method in sortMethods" class="col-xs-2 one-sort"
                ng-class="{active:(order==method.key)}"
                ng-click="sortActs(method.key)">
                <span ng-bind="method.name"></span>
            </div>
            <div style="clear:both"></div>
        </div>
    </div>
    <div class="center-content">
        <div class="col-lg-8 list">
            <tq-one-act ng-repeat="act in acts" tq-act="act" tag="1">
            </tq-one-act>
            <div class="help-block" ng-hide="acts.length">抱歉，暂时没有该类型活动。</div>
            <tq-page total-items="total_count" items-per-page="number" callback="changePage($page)"></tq-page>
        </div>
        <div class="col-lg-4 recommend" ng-hide="mobileView">
            <a href="http://www.tongqu.me/index.php/act/detail/create" class="btn tq-btn-orange create-act"><i class="fa fa-plus"></i> 发布活动
            </a>
            <div class="title">推荐活动</div>
                <tq-one-act ng-repeat="act in acts_recommend" tq-act="act" tag="2"></tq-one-act>
        </div>
        <div style="clear:both"></div>
    </div>
</div>

<script type='text/ng-template' id='tqOneAct.html'>
    <div ng-if="mobileView" class="one-list-act mobile" fade-in>
        <div class="act-poster col-xs-4" ng-click="showDetail(act.actid)">
            <act-poster class="poster" poster="{{act.poster}}" type="{{act.type}}" delay-load="{{$parent.$index * 300}}"></act-poster>
        </div>
        <div class="act-info col-xs-8">
            <div class="act-title" ng-click="showDetail(act.actid)">
                {{act.name}}
            </div>
            <div class="act-desc">
                <div class="act-status label"
                    ng-class="{'label-danger':act.time_status==0,
                              'label-success':act.time_status==1,
                              'label-info':act.time_status==2||act.time_status==3,
                              'label-default':act.time_status>3}">
                    {{actStatus[act.time_status]}}
                </div>
                <p class="time" ng-switch="act.time_status">
                    <span ng-switch-when="2">报名：{{act.sim_sign_start_time}}</span>
                    <span ng-switch-when="1">截止：{{act.sim_sign_end_time}}</span>
                    <span ng-switch-when="6">开始：{{act.sim_start_time}}</span>
                    <span ng-switch-default>结束：{{act.sim_end_time}}</span>
                </p>
                <p>人数：{{act.member_count}}&nbsp;/&nbsp;{{act.max_member != '0' ? act.max_member : '人数不限' }}
                </p>
            </div>
        </div>
    </div>
    <div ng-if="tag==1&&!mobileView" class="one-list-act" fade-in>
        <div class="act-poster col-xs-3" ng-click="showDetail(act.actid)">
            <act-poster class="poster" poster="{{act.poster}}" type="{{act.type}}"></act-poster>
        </div>
        <div class="act-info col-xs-9">
            <div class="act-title" ng-click="showDetail(act.actid)">
                {{act.name}}
            </div>
            <div class="act-desc">
                <div class="act-status label"
                    ng-class="{'label-danger':act.time_status==0,
                              'label-success':act.time_status==1,
                              'label-info':act.time_status==2||act.time_status==3,
                              'label-default':act.time_status>3}">
                    {{actStatus[act.time_status]}}
                </div>
                <p ng-show="act.time_status==0||act.time_status>=3">时间：{{act.start_time}} --- {{act.end_time}}</p>
                <p ng-show="act.time_status==1||act.time_status==2">报名：{{act.sign_start_time}} --- {{act.sign_end_time}}</p>
                <p>地点：{{act.location}}</p>
                <p>人数：{{act.member_count}}&nbsp;/&nbsp;{{act.max_member != '0' ? act.max_member : '人数不限' }}
                </p>
                <p>发起：<span ng-hide="act.source">无</span><span ng-click="showTeam(act.team_id)"
                    ng-class="{'tq-redder':act.team_id && act.team_id!=0,
                    'tq-cursor':act.team_id && act.team_id!=0}">{{act.source | limitTo:14}}</span>
                </p>
            </div>
        </div>
    </div>
    <div ng-if="tag==2" class="one-related-act" fade-in>
        <div class="poster-con" ng-click="showDetail(act.actid)">
            <act-poster poster="{{act.poster}}" type="{{act.type}}"></act-poster>
        </div>
        <div class="desc-con">
            <div class="name" ng-click="showDetail(act.actid)" title="{{act.name}}" ng-class="{small:act.name.length>25,ssmall:act.name.length>30}">
                {{act.name}}
            </div>
            <div class="time">
                <span class="num" ng-bind="getDiffTimeNum(act.end_time)"></span>
                <span ng-bind="getDiffTimeUnit(act.end_time)"></span>后结束
            </div>
            <div class="source">
                <span ng-click="showTeam(act.team_id)" ng-class="{'tq-redder':act.team_id && act.team_id!=0, 'tq-cursor':act.team_id && act.team_id!=0}">
                    {{act.source}}
                </span>
            </div>
        </div>
        <div style="clear:both"></div>
    </div>
</script>

