function followCall(isFollow){
	UI.toggle('followTxt');
	UI.toggle('followBtn');
	$('followBtn').className='addAttention';
	$('unfollowBtn').className='delAttention';
	var btn=$('sFollowBtn');
        var followerNum = $$('a.followerNum')[0];
		if(isFollow==1){
		UI.show(btn);
		MI.Bos('btnFollowHis');
	}
	else {
		UI.hide(btn);
		MI.Bos('btnUnFollowHis');
		UI.hide($('followBtn').parentNode);
		UI.removeClass(btn,'foLight');
	}
		var note = $('addNote'),noteBtn = $$(note,'a')[0];
	if(isFollow==1){
		UI.show(note);
		UI.hide($('followBtn').parentNode);
                if(followerNum){
                    followerNum.innerHTML = parseInt(followerNum.innerHTML, 10) + 1;
                }
	}
	else{
		UI.hide(note);
		UI.A(noteBtn,'note','');
		noteBtn.innerHTML = '添加备注';
		UI.show($('followBtn').parentNode);
                if(followerNum){
                    var num = parseInt(followerNum.innerHTML, 10) - 1;
                    if ( num < 0 ) num = 0;
                    followerNum.innerHTML = num;
                }
	}
}


