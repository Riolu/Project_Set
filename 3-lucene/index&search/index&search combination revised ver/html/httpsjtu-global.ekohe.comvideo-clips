<script>
  (function() {
    $(function() {
      window.showOverlay = "";
      if (window.showOverlay === "true") {
        grayscale.prepare($('.logo'));
        grayscale.prepare($('.carousel-inner img'));
        grayscale.prepare($('.academic img'));
        grayscale.prepare($('.partner img'));
        return grayscale($('body'));
      }
    });
  
    window.onload = function() {
      if (window.showOverlay === "true") {
        return grayscale($('body'));
      }
    };
  
  }).call(this);
</script>
