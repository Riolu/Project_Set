<script type="text/javascript">

//从接口返回直播数据函数
function getDirectData(fid,fStatus){
	$
	.post("/directJson.do",
	    {
	    fid:fid,
	    status:fStatus
	    },
	    function(data,status){
			var objs = eval('('+data+')');
			if(objs.length==0){
				$("#directDiv").hide();
			}
			
			var str = "";
			for(var i=0;i<objs.length;i++){
				var obj = objs[i];
				
				var row = '';
				if(obj.type=='1')
					row = '<li class=\"ft_question\">'
				else
					row = '<li>'
				
	            row = row + '<p><strong>'+obj.userNick+'</strong>'+
	            '</p><p>'+obj.content+'</p></li>';
				str = str + row;
			}
			$("#ft_liveFeed").html(str);
		}
	);
};

//15秒，重新请求次数据
function refreshData(reData,fid,fStatus){
	reData = window.setInterval(function(){
		getDirectData(fid,fStatus)
	},1000*15);
	return reData;
}

