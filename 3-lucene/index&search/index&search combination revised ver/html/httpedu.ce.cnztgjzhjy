  <script type="text/javascript">
$(document).ready(function(){
	$("#focus .piclist li").eq(0).addClass("show");
	$("#focus .pictitle li").eq(0).addClass("currenttitle");
	var timer=setInterval(play,3000);
	var i=0;
	function play()
	{
		if(i<4)
		{
			i++;
		}
		else
		{
			i=0;
		}
		$("#focus .piclist li").eq(i).fadeIn();
		$("#focus .piclist li").eq(i).siblings().hide();
		$("#focus .pictitle li").eq(i).show();
		$("#focus .pictitle li").eq(i).siblings().hide();
		$("#focus .pointlist li").eq(i).addClass("point_sel");
		$("#focus .pointlist li").eq(i).siblings().removeClass("point_sel");
	}
	$("#focus .pointlist li").each(function(j){
		$(this).mouseover(function(){
			clearInterval(timer);
			i=j;
			$("#focus .piclist li").eq(i).fadeIn();
			$("#focus .piclist li").eq(i).siblings().hide();
			$("#focus .pictitle li").eq(i).show();
			$("#focus .pictitle li").eq(i).siblings().hide();
			$("#focus .pointlist li").eq(i).addClass("point_sel");
			$("#focus .pointlist li").eq(i).siblings().removeClass("point_sel");
			timer=setInterval(play,3000);
		})
	})
	$("#focus .bt_l").click(function(){
		clearInterval(timer);
		if(i==0)
		{
			i=4;
		}
		else
		{
			i--;
		}
		$("#focus .piclist li").eq(i).fadeIn();
		$("#focus .piclist li").eq(i).siblings().hide();
		$("#focus .pictitle li").eq(i).show();
		$("#focus .pictitle li").eq(i).siblings().hide();
		$("#focus .pointlist li").eq(i).addClass("point_sel");
		$("#focus .pointlist li").eq(i).siblings().removeClass("point_sel");
		timer=setInterval(play,3000);
	})
	$("#focus .bt_r").click(function(){
		clearInterval(timer);
		if(i<4)
		{
			i++;
		}
		else
		{
			i=0;
		}
		$("#focus .piclist li").eq(i).fadeIn();
		$("#focus .piclist li").eq(i).siblings().hide();
		$("#focus .pictitle li").eq(i).show();
		$("#focus .pictitle li").eq(i).siblings().hide();
		$("#focus .pointlist li").eq(i).addClass("point_sel");
		$("#focus .pointlist li").eq(i).siblings().removeClass("point_sel");
		timer=setInterval(play,3000);
	})
})
</script>
