<script type="text/javascript">
$(function(){
	var d=new Date();
		var year=d.getFullYear();
		var month=d.getMonth()+1;
		var day=d.getDate();
		var weekNum=d.getDay();
		var weeks=['星期日','星期一','星期二','星期三','星期四','星期五','星期六'];
		$('#dates').text(year+'年'+month+'月'+day+'日 '+weeks[weekNum]).css({'float':'right','line-height':'38px'});
	$('.bg,.text').css({'opacity' : 0})
	$('.pic1,.pic2,.pic3,.pic4').hover(function(){
		var height=$(this).find('.text').height();	
		var width=$(this).find('.text').width();	
		$(this).find('.bg').height(height);
		$(this).find('.bg').stop().animate({'opacity' : 0.3});
		$(this).find('.text').stop().animate({'opacity' : 1})
	},function(){
		$('.bg,.text').stop().animate({'opacity' : 0})
	})
	$(".L3 li:gt(9)").hide();
	$('.more a').click(function(){
		$('.L3 li').slideDown();
		$('.more').fadeOut();
	})
	$('.banner_inner ul li').hover(function(){
		var index=$(this).index()+1;
		$(this).removeClass('on'+index);
		if(index>1){
			$('.banner_inner ul li:first').addClass('on1');
		}
	},function(){		
		var index=$(this).index()+1;
		$('.banner_inner ul li').not($('.banner_inner ul li:first')).addClass('on'+index);
		$('.banner_inner ul li:first').removeClass('on1');

	})
})
	</script>
