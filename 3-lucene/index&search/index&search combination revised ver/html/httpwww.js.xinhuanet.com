<script type="text/javascript">
a = 0
function BigPic(id){
for (i = 1; i <= 6; i++) {//循环{i赋值1；当i小于或等于5的时候；i递增加1}
    //在循环中判断，这里每次执行都会判断5次。
    if (id == i) {//判断传递过来的ID是否等于i，如果等于。
        document.getElementById("lt_tab_" + i).style.display = "block"
document.getElementById("tb" + i).style.backgroundColor = "#649ac8"
document.getElementById("tb" + i).style.color = "#ffffff"
        //将id等于lt_tab_+这里的i是1-5.其中的一个数字。将此ID显示出来
    } else {
        document.getElementById("lt_tab_" + i).style.display = "none"
document.getElementById("tb" + i).style.backgroundColor = "#ffffff"
document.getElementById("tb" + i).style.color = "#1468b4"
        //其他的全部隐藏起来
    }
}

}

function Clear(id){
BigPic(id);
clearTimeout(vart)
}

function TimeOut(b) {
a=b
if(a>=6){
a=1
}else{
a=a+1
}
BigPic(a);
vart=setTimeout("TimeOut("+a+")",6000);
}

TimeOut(0);
</script>
