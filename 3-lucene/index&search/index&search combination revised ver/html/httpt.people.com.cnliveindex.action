<script type="text/javascript">
 	var showType = "";
 	var objectTop,objectLeft;
 	var t_settime_up,t_settime_down;
 	var currObj;
 	var showtime;
	$(document).ready(function(){
		
		$(".userinfoview").live({
		   mouseenter:function() {
			//处理鼠标进入的事件
		   currObj = this;
		   showtime = setTimeout(function(){
		      var userLinkTop = $(currObj).offset().top;
				var scrollTop = $(document.documentElement).scrollTop();
				if(typeof(browser) != "undefined") {
					if(browser == "Netscape") {
						scrollTop = $(document.body).scrollTop();
					}
				}
				var halfe = jQuery(currObj).children("img").height()/2;
				var halfPageTop =$(window).height() /2;
		        var hideInfos;
				var left;
				var top;
		        if((userLinkTop-scrollTop-halfe)>halfPageTop){
					clearTimeout(t_settime_up);
		        	 showType = "up";
				     hideInfos = jQuery("#pop_up");
					 left=(jQuery(currObj).offset().left);
				     top=(jQuery(currObj).offset().top - hideInfos.height());
				     if(typeof(browser) != "undefined") {
						if(browser == "Netscape" && jQuery(currObj).height()==12) {
							top=(jQuery(currObj).offset().top - hideInfos.height() - 50);
							objectTop = jQuery(currObj).offset().top - 50;
						}else{
							top=(jQuery(currObj).offset().top - hideInfos.height());
							objectTop = jQuery(currObj).offset().top;
						}
					 }
				     objectLeft = jQuery(currObj).offset().left;
			    }else{
					clearTimeout(t_settime_down);
			    	 showType = "down";
		             hideInfos = jQuery("#pop_down");
					 left=(jQuery(currObj).offset().left);
					 if(jQuery(currObj).height() == 0) {
					 	top=(jQuery(currObj).offset().top+50);
					 }else{
				     	top=(jQuery(currObj).offset().top+jQuery(currObj).height());
				     }
				}
				updateDiv();
				hideInfos.show();
				hideInfos.offset({"top":top,"left":left});
		        usertag.getUserInfoPage(jQuery(currObj).attr("id"),initUserInfo);
		       },1000);
		   },
		   mouseleave:function() {
		        clearTimeout(showtime);
				t_settime_up = setTimeout("jQuery(\"#pop_up\").hide()", 500);
				t_settime_down = setTimeout("jQuery(\"#pop_down\").hide()", 500);
		   }
		});
		
		
		$("#pop_up").live({
		   mouseenter:function() {
		      clearTimeout(t_settime_up);
