	function initUserInfo(followUserVO) {
	    var tagList = followUserVO.tags;
	    var medalList = followUserVO.medals;
	    
		var html = "";
		html = html + "<dl>";
		html = html + "<dt><img width=\"50\" height=\"50\" alt=\"" + followUserVO.nickName +"\" src=\"/" + followUserVO.userHeadMiddle +"\"/></dt>";
		html = html + "<dd>";
		html = html + "<p>";
		html = html + "<a class=\"userName\" href=\"/" + followUserVO.personUrl +"\">" + followUserVO.nickName +"</a>";
		if(followUserVO.isValidate == "1"){
			html = html + "<span><img  alt=\"实名认证用户\" src=\"/microblog/images/valid_1_s.jpg\"/></span>";
		}else if(followUserVO.isValidate == "2"){
			html = html + "<span><img  alt=\"人民日报人民网记者\" src=\"/microblog/images/valid2_1_s.jpg\"/></span>";
		}
		html = html + "</p>";
		html = html + "<p><span>" + followUserVO.homeCityName +"</span></p>";
		html = html + "<p>";
		var numList = followUserVO.panelNumList;
		for(var i=0;i<numList.length;i++) {
			var objects = numList[i];
			html = html + "<span class=\"mr_7\"><a href=\"/" + objects[3] +"\">" + objects[1] +"</a>" + objects[2] +"</span>";
			if(i==2)
			  break; 
		}
		html = html + "</p>";
		html = html + "</dd>";
		html = html + "</dl>";
		html = html + "<table width=\"276\" border=\"0\" cellspacing=\"0\" cellpadding=\"0\" class=\"letter_synopsis\">";
	   
		if (followUserVO.infoCheckState==1){
		html = html + "<tr>";
		html = html + "<th width=\"36\">简介：</th>";
		html = html + "<td> "+ followUserVO.userInfo +"</td>";
		html = html + "</tr>";
		}else
		{
			
			 	if(followUserVO.userId==-1){
				    html = html + "<tr>";
					html = html + "<th width=\"36\">简介：</th>";
					html = html + "<td> "+ followUserVO.userInfo +"</td>";
					html = html + "</tr>";
			    }
		    
		}
		if(tagList.length>0){
			html = html + "<tr>";
			html = html + "<th width=\"36\">标签：</th>";
			html = html + "<td>";
			for(var i=0;i<tagList.length;i++) {
				html = html  + "<a href=\"usertagsearch.action?keyWords=" + tagList[i].tagName + "\" class=\"mr_7\"> " + tagList[i].tagName + "</a>";
			}
			
			html = html + "</td>";
			html = html + "</tr>";
		}
		
		if(medalList.length >0){
			html = html + "<tr>";
			html = html + "<th width=\"36\">勋章：</th>";
			html = html + "<td>";
			for(var i=0;i<medalList.length;i++){
				  html = html + "<img width=\"24\" height=\"24\" alt=\"" + medalList[i][0] +"\" src=\"/MedalRule_IMG/" + medalList[i][1] +"\"/>&nbsp;";
			}
			
			html = html + "</td>";
			html = html + "</tr>";		
		}

		html = html + "</table>";

		
		if(!followUserVO.isMe){
			//判断是否已经关注
			html = html + "<div class=\"pop_intro_handle\">";
			if(followUserVO.isMyFocuson == 0){
            	html = html + "<a href=\"javascript:;\" onclick=\"javascript:UserCommonApp.addmyflwtgr('" + followUserVO.userId +"',this,'" + followUserVO.nickName +"')\" class=\"bt_attention\"></a>"
			}else if(followUserVO.isMyFocuson == 1){
				html = html + "<span class=\"info\"><a href=\"javascript:;\" onclick=\"javascript:joinGroup('" + followUserVO.userId +"','" + followUserVO.nickName +"')\">设置分组</a></span>";
				html = html + "<span class=\"btn\">已关注&nbsp;&nbsp;|&nbsp;&nbsp;<a href=\"javascript:;\" onclick=\"javascript:PublishApp.cancelMyFocuson(" + followUserVO.userId +"," + followUserVO.sex +",'" + followUserVO.nickName +"'," + followUserVO.isFocusonMe +")\">取消</a></span>";
			}
			html = html + "</div>";	
		}
		$("#pop_up_main").html(html);
		$("#pop_down_main").html(html);
		if(showType == "up") {
			var thehideInfos = jQuery("#pop_up");
			var thetop = objectTop - thehideInfos.height();
			thehideInfos.offset({"top":thetop,"left":objectLeft});
		}
	}
</script>

