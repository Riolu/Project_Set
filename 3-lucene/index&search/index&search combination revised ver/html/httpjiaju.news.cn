<script>
$(function() {
	 //地方站选择   
	$(".location .open").click(function(){
		/*判断是否是打开状态*/
		if($(this).hasClass("on"))
		{
			/*如果打开，则给关闭样式并隐藏地区框*/
			$(this).removeClass("on");
			$(".location .areabox").hide();
		}
		else
		{
			/*如果关闭，则给打开样式并显示地区框*/
			$(this).addClass("on");
			$(".location .areabox").show();
		}
	});
	/*地区框内地区点击事件*/
	$(".location .area_list li a").each(function(index){
		$(".location .area_list li a").eq(index).click(function(){
			$(".location h2 span").text($(this).text()); //把目前地区的内容换成当前点击的地区内容
			$(".location .on").removeClass("on"); //给关闭样式
			$(".location .areabox").hide(); //关闭地区框
		});
	});
	/*鼠标移除地区框区域事件*/
	$(".location .areabox").mouseleave(function(){
		$(".location .on").removeClass("on"); //给关闭样式
		$(".location .areabox").hide(); //关闭地区框
	});

	//大图滚动
	focusSwiper = new Swiper('#focusImg',{
