		autoplay:5000,
		loopedSlides:1,
		onSlideNext:function(){
			var $_this = $("#focusImg .swiper-slide").eq(focusSwiper.activeIndex);
			if($_this.find(".lazyload").length){
				$_this.find(".lazyload").each(function(){
					$(this).attr("src",$(this).attr("data-original")).removeClass("lazyload");
				});
			};
		}
	});
	$('#focusImg .arrow-left').on('click', function(e){
		e.preventDefault()
		focusSwiper.swipePrev()
	});
	$('#focusImg .arrow-right').on('click', function(e){
		e.preventDefault()
		focusSwiper.swipeNext()
	});
