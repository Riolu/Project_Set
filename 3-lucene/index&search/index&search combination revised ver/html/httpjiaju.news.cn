	$("#ycspImg .swiper-wrapper .swiper-slide .newsVideo .play").click(function(){
			$('#ycspImg .arrow-left').hide();
			$('#ycspImg .arrow-right').hide();
			$(".Interaction .tabBox .tab_box .ycsp .close").show();
            var _src = $(this).next().html();
            var ifrStr = '<iframe frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="'+_src+'"></iframe>';
            $("#ycspImg .swiper-wrapper").parent().append(ifrStr);
        });
        $(".Interaction .tabBox .tab_box .ycsp .close").click(function(){
            $(this).hide();
            $("#ycspImg .swiper-wrapper").parent().find("iframe").remove();
            $('#ycspImg .arrow-left').show();
			$('#ycspImg .arrow-right').show();
        });

   	/*地产海报*/
