		var a = rwkInfos[i].pinyin.substring(0,1).toUpperCase();
		if(name == a){
			b += '<li><p class="pic"><a href="'+rwkInfos[i].pageUrl+'" target="_blank"><img src="'+rwkInfos[i].picPath+'" /></a></p><p class="name"><a href="'+rwkInfos[i].pageUrl+'" target="_blank">'+rwkInfos[i].name+'</a></p></li>';
		 	//if (d.indexOf(a) == -1) d.push(a);
		}
	}
	return b;
}

//生成籍贯
var jiguan = ["北京","天津","河北","山西","内蒙古","辽宁","吉林","黑龙江","上海","江苏","浙江","安徽","福建","江西","山东","河南","湖北","湖南","广东","广西","海南","重庆","四川","贵州","云南","西藏","陕西","甘肃","青海","宁夏","新疆","香港","澳门","台湾"];


function cx_jg(){
	var cx_b="";
	for( var j=0; j<jiguan.length; j++){
		if(pd_jiguang(jiguan[j])!=="")
		{
			cx_b += '<div><h3 id="'+jiguan[j]+'">'+jiguan[j]+'</h3><ul>'+pd_jiguang(jiguan[j])+'</ul></div>';
		}
	}
	$("#cx_jiguan").append(cx_b);
}

//判断籍贯并返回人员名单
function pd_jiguang(a){
	var pd_c="";
	for( var i=0;i<rwkInfos.length;i++){
		var pd_b = rwkInfos[i].nativePlace;
		if(pd_b.indexOf(a)>-1){
			pd_c += '<li><p class="pic"><a href="'+rwkInfos[i].pageUrl+'" target="_blank"><img src="'+rwkInfos[i].picPath+'" /></a></p><p class="name"><a href="'+rwkInfos[i].pageUrl+'" target="_blank">'+rwkInfos[i].name+'</a></p></li>';
		}
	}
	return pd_c;
} 



//查询出生日期
function cx_scnf(){
	var cx_b="";
	

	for( var j=0; j<5; j++){
		var a = 1920 + j*10;
		var b = a+9;
		if( pd_scnf(a,b)!=="")
		{
			cx_b += '<div><h3 id="C'+j+'">'+a+'-'+b+'年</h3>'+pd_scnf(a,b)+'</div>';
		}
	}
	$("#cx_chusheng").append(cx_b);
}

//判断籍贯并返回人员名单
function pd_scnf(a,b){
	var pd_c="",pd_c1="",pd_c2="";
	var j = parseInt(a);
	var k = parseInt(b);
	for(j;j<k+1;j++){
		
		pd_c1 = '';
		pd_c2 = '';
		
		for( var i=0;i<rwkInfos.length;i++){
			var pd_b = parseInt(rwkInfos[i].birthday.substring(0,4));
			if(pd_b == j){
				pd_c2 += '<li><p class="pic"><a href="'+rwkInfos[i].pageUrl+'" target="_blank"><img src="'+rwkInfos[i].picPath+'" /></a></p><p class="name"><a href="'+rwkInfos[i].pageUrl+'" target="_blank">'+rwkInfos[i].name+'</a></p></li>';
				pd_c1 = '<h4>'+j+'</h4><ul>';
			}
		}
		
		//alert(j+"pd_c1:"+pd_c1+"pd_c2:"+pd_c2);
		pd_c += pd_c1 + pd_c2;
		
	}
	
	
	pd_c += '</ul>';
	//alert(pd_c);
	return pd_c;
} 


//搜索数据
function rwk_ss(name){
	//if(name == "请输入姓名") return false;
	
	var pd_c="";
	for( var i=0;i<rwkInfos.length;i++){
		var pd_b = rwkInfos[i].name;
		if(pd_b == name){
			pd_c += '<div class="ss_box"><p class="pic"><a href="'+rwkInfos[i].pageUrl+'" target="_blank"><img src="'+rwkInfos[i].picPath+'" /></a></p><p class="name"><a href="'+rwkInfos[i].pageUrl+'" target="_blank">'+rwkInfos[i].name+'</a></p></div>';
		}
	}
	if(pd_c=="") pd_c = '<div class="ss_none">未检索到信息，请重新搜索！</div>';
	$("#myModal .content").html(pd_c);
} 




