<script>
var islogin = false;
var userId = 0;
function getCookie(name){
 var arr,reg=new RegExp("(^| )"+name+"=([^;]*)(;|$)");
 if(arr=document.cookie.match(reg))
  return unescape(arr[2]);
 else
  return null;
} 
function delCookie(objName) {
   var date = new Date();
   date.setTime(date.getTime() - 10000);
   document.cookie = objName + "=a;path=/;domain=cncnews.cn; expires=" + date.toGMTString();
}

 $(function() {
    $( "#dialog" ).dialog({
      autoOpen: false,
      height: 400,
      width: 500,
      modal: true
    });
  function isloging(username){
	var url = "http://login.cncnews.cn/user/islogin.cc?login_name="+username+"&callback=showData";
	$.getScript(url);
  }

	var shortname ="";
    var username = getCookie("uid");
    if(username !='' && username != null ){
	 	   isloging(username);
    }
	
	var key=request('key'); 
    var time=request('time');
	var reg=request('type');
	if(key!=""&&time!=""){
		var url="";
		if(reg!=""){
		 url = "http://login.cncnews.cn/user/validate.cc?key="+key+"&time="+time ; 
		}else{
		 url = "http://login.cncnews.cn/user/validatemail.cc?key="+key+"&time="+time ; 
		}
		$("#cframe").attr("src",url);
		if($( "#dialog" ).css("display")=='none'||$( "#overlay" ).css("display")=='none'){
			$( "#dialog" ).css("display","block");
			$( "#overlay" ).css("display","block")
		}  
		$( "#dialog" ).dialog( "open" );
		return false;
	}
  });
	
	function request(paras){ 
		var url = location.href;  
		var paraString = url.substring(url.indexOf("?")+1,url.length).split("&");  
		var paraObj = {}  
		for (i=0; j=paraString[i]; i++){  
			paraObj[j.substring(0,j.indexOf("=")).toLowerCase()] = j.substring(j.indexOf("=")+1,j.length);  
		}  
		var returnValue = paraObj[paras.toLowerCase()];  
		if(typeof(returnValue)=="undefined"){  
			return "";  
		}else{  
			return returnValue;  
		}  
	} 
  

  function showData(data){
	if(data.msg =="success"){
           islogin = true;
	  if(data.username.length >22){
		shortname = data.username.substring(0,22);
		$("#loginname").html(shortname);
	 }else{
		$("#loginname").html("您好,"+data.username);
	 }
	  $("#loginname").attr("onclick","").attr("title",data.username);
	  $("#logout").html("退出");
	  $("#logout").removeAttr("onclick");
	  $("#logout").click(function(){
logout();
             })
	  var validateinfo = getCookie("validateinfo");
	  if(validateinfo == data.username){
		delCookie("validateinfo");
		filldata(validateinfo);      
	  }
           userId = data.userid;
	} 
 }
  function logout(){
		delCookie("uname");
		delCookie("uid");
		window.location.reload();
  }
  function filldata(username){
	var url = "http://login.cncnews.cn/user/tofilldata.cc?userName="+username ; 
	$("#cframe").attr("src",url);
	if($( "#dialog" ).css("display")=='none'||$( "#overlay" ).css("display")=='none'){
		$( "#dialog" ).css("display","block");
		$( "#overlay" ).css("display","block")
	}  
	$( "#dialog" ).dialog( "open" );
	return false;
 }
 function login(){
  $("#cframe").attr("src","http://login.cncnews.cn/user/weblogin.cc");
  if($( "#dialog" ).css("display")=='none'||$( "#overlay" ).css("display")=='none'){
    $( "#dialog" ).css("display","block");
    $( "#overlay" ).css("display","block")
   }  
  $( "#dialog" ).dialog( "open" );
  return false;
 }
 function reg(){
  $("#cframe").attr("src","http://login.cncnews.cn/user/reg.cc");
  if($( "#dialog" ).css("display")=='none'||$( "#overlay" ).css("display")=='none'){
    $( "#dialog" ).css("display","block");
    $( "#overlay" ).css("display","block")
   }  
  $( "#dialog" ).dialog( "open" );
  return false;
 }

 </script>
