function getnews(obj,page,flag){

	$(".xin_c_yaowen p a").each(function(){
		$(this).removeClass("djt").removeClass("djt_m");
		if($(this).attr('id')==obj){
            if($(this).html().length>2){			
                $(this).addClass("djt_m");
            }else{
				$(this).addClass("djt");
            }
		}
	});
	setCookie('channel',obj);
         if(obj==1){flag=false;obj=$("a[name='last']").html();}
	if (flag == 'hidden') {// 处理+号下拉列表的
        flag = true;
        if ($("#" + obj).html().length > 2) {
            $("a[name='last']").html($("#" + obj).html()).addClass("djt_m");
        } else {
            $("a[name='last']").html($("#" + obj).html()).addClass("djt");
        }
		$("#dropmenu li a").each(function() {
			$(this).css('display', 'block');
			if ($(this).attr('id') == obj) {
				$(this).css("display", "none");
			}
		});
        $("a[name='last']").attr("id", obj);
    }
	
	if(flag){//栏目搜索新闻
		setCookie('type',1);
		document.domain = "cncnews.cn";
		if(page ==1){
			$.getScript("http://www.cncnews.cn/static/"+obj+".js?time="+new Date().getTime(),function(){
				showHtml(obj,data,page,flag);
			});
		}else{
			var newpage = eval(page)+1;
			$.getScript("http://data.cncnews.cn/cnc/news_list.jsp?column_id="+obj+"&pageno="+newpage+"&pagesize=15&type=1&time="+new Date().getTime(),function(){

				showHtml(obj,data,page,flag);

			});
		}
	}else{//关键词搜索
		setCookie('type',2);
		if (obj.length > 2) {
			$("a[name='last']").html(obj).addClass("djt_m").attr("id","1");
		}else{
			$("a[name='last']").html(obj).addClass("djt").attr("id","1");
		}
		$("#dropmenu li a").each(function() {
			$(this).css('display', 'block');
		});
		
		$.getScript("http://data.cncnews.cn/cnc/news_list.jsp?column_id="+obj+"&pageno="+page+"&pagesize=30&type=2&time="+new Date().getTime(),function(){
			showHtml(obj,data,page,flag);
		});
	}
	
	
}

function setCookie(name, value) { 
    var argv = setCookie.arguments; 
    var argc = setCookie.arguments.length; 
    var expires = (argc > 2) ? argv[2] : null; 
    if(expires!=null) { 
        var LargeExpDate = new Date (); 
        LargeExpDate.setTime(LargeExpDate.getTime() + (expires*1000*3600*24));         
    } 
    document.cookie = name + "=" + escape (value)+((expires == null) ? "" : ("; expires=" +LargeExpDate.toGMTString())); 
}

