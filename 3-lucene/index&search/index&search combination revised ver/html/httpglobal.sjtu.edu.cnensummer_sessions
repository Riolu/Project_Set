<script>
  (function() {
    $(function() {
      if ("8" <= 5) {
        $('.summer-schools-view-more').hide();
      }
      return $(document).on('click', '.summer-schools-view-more', function(e) {
        var currentItems;
        e.preventDefault();
        currentItems = $('.sort-lists li').size();
        return $.get("/summer_sessions/more", {
          currentItems: currentItems
        }, function(data) {
          return -(data.length <= 1 ? $('.summer-schools-view-more').hide() : ($('.sort-lists').append(data), $('.sort-lists li').size() % 5 !== 0 ? $('.summer-schools-view-more').hide() : void 0));
        });
      });
    });
  
  }).call(this);
</script>

