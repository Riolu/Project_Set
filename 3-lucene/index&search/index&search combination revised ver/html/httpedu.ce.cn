<script type="text/javascript">
	$(function(){
		$(".topBtn").hide();
		$('.news9 dl:first').css('padding-top','0')
		$('h5 span:eq(1)').css('color','#2fb8fe').find('a').css('color','#2fb8fe');
		$('.qwfb ul:first').show();
		$('.focusR li:last').css('border','none');
		$('.pic1,.pic2,.pic3,.pic4,.pic5').hover(function(){
			$(this).find('.bg').stop(true,true).slideDown();
			$(this).find('p').stop(true,true).slideDown();
		},function(){
			$(this).find('.bg').stop(true,true).slideUp()
			$(this).find('p').stop(true,true).slideUp();
		})
		$('.qwfb h5 span').hover(function(){
			var index=$(this).index();
			$('h5 span').css('color','#f2f2f2').find('a').css('color','#000');
			$(this).css('color','#2fb8fe').find('a').css('color','#2fb8fe');
			$('.qwfb ul').hide();
			$('.qwfb ul:eq('+index+')').show();
		})
		$(window).scroll(function(){
			if ($(window).scrollTop()>100){
				$(".topBtn").show();
			}
			else
			{
				$(".topBtn").hide();
			}
		});
		$(".topBtn").click(function(){
			$('body,html').animate({scrollTop:0},500);
			return false;
		});

		
	})
	</script>
