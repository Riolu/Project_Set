    <script type="text/javascript">
    //global settings
    window.domain = window.location.protocol + "//www.lib.sjtu.edu.cn";
    window.catid = "";
    window.lang = "en";
    window.static_path = "/statics/v2/";
    window.isFlipFallback = !!navigator.userAgent.match(/Trident\/7\./) || navigator.userAgent.match("Chrome\/21");

    function initHeader(){

        //fixed ie11 bug
        if(window.isFlipFallback) {
            var html = document.getElementsByTagName("html")[0];
            html.className = "ie ie11 cn";
        }
    }
    
    function fixLinks(){
        //自动添加语言后缀
        $.each($("a"),function(i,n){
	        
	        if(!$(this).attr("href")) return;
	        
            var $href = $(this).attr("href");
            
            var $outlink = $href && (($href.indexOf("http://") > -1 || $href.indexOf("https://") > -1) && $href.indexOf(window.domain) < 0);
        
            if(!$outlink && $href.indexOf("lang=zh") < 0 && $href.indexOf("javascript:") < 0) {
                if($href.indexOf("?") < 0)
                    $(this).attr("href",$href+"?lang=en");    
                else
                    $(this).attr("href",$href+"&lang=en");    
            }
            
                
            //给外链增加target=new
            if($outlink) {
                $(this).attr("target", "_blank");
            }
        });
    }

    </script>
    
