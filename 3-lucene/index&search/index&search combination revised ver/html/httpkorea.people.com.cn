					$(this).find('td').each(function(n){
					data_huilv.push($(this).text());
				});
			});

			//显示汇率
			
			if( data_huilv.length >= 12 )
			
			{
			
				document.getElementById("yingbang").innerHTML = data_huilv[0];
				document.getElementById("yingbang_value").innerHTML = data_huilv[1];
				document.getElementById("gangbi").innerHTML = data_huilv[2];
				document.getElementById("gangbi_value").innerHTML = data_huilv[3];
				document.getElementById("meiyuan").innerHTML = data_huilv[4];
				document.getElementById("meiyuan_value").innerHTML = data_huilv[5];
				document.getElementById("riyuan").innerHTML = data_huilv[6];
				document.getElementById("riyuan_value").innerHTML = data_huilv[7];
				document.getElementById("ouyuan").innerHTML = data_huilv[8];
				document.getElementById("ouyuan_value").innerHTML = data_huilv[9];
				document.getElementById("hanyuan").innerHTML = data_huilv[10];
				document.getElementById("hanyuan_value").innerHTML = data_huilv[11];
			
				var arr_mon = Array( '欧元', '美元', '英镑', '港币', '日元', '韩元' );
				var arr_img = Array( 'oumeng.jpg', 'meiguo.gif', 'yingguo.jpg', 'xianggang.jpg', 'riben.jpg', 'hanguo.jpg' );
				
				document.getElementById("yingbang_img").src = '/img/2012koreacn/images/' + arr_img[arrpos(arr_mon,data_huilv[0])];
				document.getElementById("gangbi_img").src = '/img/2012koreacn/images/' + arr_img[arrpos(arr_mon,data_huilv[2])];
				document.getElementById("meiyuan_img").src = '/img/2012koreacn/images/' + arr_img[arrpos(arr_mon,data_huilv[4])];
				document.getElementById("riyuan_img").src = '/img/2012koreacn/images/' + arr_img[arrpos(arr_mon,data_huilv[6])];
				document.getElementById("ouyuan_img").src = '/img/2012koreacn/images/' + arr_img[arrpos(arr_mon,data_huilv[8])];
				document.getElementById("hanyuan_img").src = '/img/2012koreacn/images/' + arr_img[arrpos(arr_mon,data_huilv[10])];
			}

		});
	});


function arrpos(arr, val){
	for(var i=0; i<arr.length; i++){
		if(arr[i] == val)
			return i;
	}
	return -1;
}



//获取并解析股票xml文件

$(function(){

