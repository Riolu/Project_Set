<script language="javascript">


//获取并解析天气xml文件

$(function(){

		var data_tianqi = new Array( );
	
		$.get('/rss/tianqi.xml',function(data){
			//查找所有的forecast节点
			$(data).find('forecast').each(function(i){
				var theday=$(this).attr('date');
				var t = theday.split(' ');
				theday = t[0];
				var today = new Date( );
					if( theday == today.getDate() )
					{

						data_tianqi.push( $(this).attr( "low" ) );
				
						data_tianqi.push( $(this).attr( "high" ) );
				
					}

			});
			
			
			//显示天气
			if( data_tianqi.length >= 4 )
			
			{
			
				document.getElementById("tianqi_high_bj").innerHTML = data_tianqi[1];
			
				document.getElementById("tianqi_low_bj").innerHTML = data_tianqi[0];
			
				document.getElementById("tianqi_high_se").innerHTML = data_tianqi[3];
			
				document.getElementById("tianqi_low_se").innerHTML = data_tianqi[2];
			
			}
			
		});
	});





//获取并解析汇率xml文件

$(function(){

		var data_huilv = new Array( );

		$.get('/rss/huilv.xml',function(data){
				//查找所有的tr节点
