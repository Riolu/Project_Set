<script type="text/javascript">
var xmlObj;
function GetXML(){
    if (window.ActiveXObject){ 
        xmlObj = new ActiveXObject("Microsoft.XMLDOM"); 
    } else if (document.implementation && document.implementation.createDocument) { 
        xmlObj = document.implementation.createDocument("","",null); 
    } 
    xmlObj.async=false; 
    xmlObj.load("/rss/2008/top.xml")
    GetReady();
}
var City = function(qyname,name,url,number){
    this.qyname = qyname;
	this.name = name;
	this.url=url;
    this.number = number;	
    this.toString = show;
}
function show(){
return "<td width=\"100\"><div style=\"overflow:hidden;height:25px;width:100px;\">"+this.qyname+"</div></td><td width=\"64\"><a href=\""+this.url+"\" target=\"_blank\">"+this.name+"</a></td><td width=\"55\">"+this.number;
}
function GetReady(){
    var cityArray = new Array;
    var cityList = xmlObj.documentElement.getElementsByTagName("school");
    var count = cityList.length;
    for (var i=0;i<count;i++){
		var qyname = cityList[i].getAttribute("qyname");
        var name = cityList[i].getAttribute("name"); 
		var url = cityList[i].getAttribute("url");  
		var number = parseInt(cityList[i].getAttribute("number"));
        cityArray[i] = new City(qyname,name,url,number);
    }
    //这里进行排序，升序为修改sObj.number-tObj.number
    cityArray.sort(function(tObj,sObj){return sObj.number-tObj.number});
    //程序结束，这里只是打印出来显示
    var str="";
	var l=0;
    str+="<ul>";
    for (var j=0;j<10;j++){
	l=j+1;
        str+="<li><span>"+l+"</span>"+"&nbsp;&nbsp;<strong>"+cityArray[j].toString()+"</strong></li>";                
    }
	str+="</ul>";
    document.getElementById("text").innerHTML=str;    
}
</script>
