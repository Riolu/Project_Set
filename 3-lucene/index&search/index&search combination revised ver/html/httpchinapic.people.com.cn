var tt = top_top - count; 
tt = (tt < 300) ? 0 : tt; 
if (top_top > 0) 
//window.scrollTo(tt, tt);
window.scrollTo(0, 0); 
else 
clearInterval(timer); 
}, speed) 
}; 
} 
