//声明一个包含img和title的div 
function addDiv() {

if( ran >= count )
return;

//title层 
var title_div = document.createElement("div");
title_div.innerHTML = "[<a href='" + flinkArray[ran] + "' title='" + fhintArray[ran] + "' target='_blank'>" + forumArray[ran] + "</a>]" + "<a href='" + linkArray[ran] + "' title='" + hintArray[ran] + "' target='_blank'>" + hintArray[ran] + "</a><br>";
title_div.innerHTML += "<li class='fl mr5'><a href='" + alinkArray[ran] + "' target='_blank'><img src='" + avatarArray[ran] + "' style='width:30px;height:30px;'></a></li><li class='mt10'><a href='" + alinkArray[ran] + "' target='_blank'>" + authorArray[ran] + "</a>&nbsp;&nbsp;" + dateArray[ran] + "</li>";
title_div.className = "title clearfix";


//内部img
var img = document.createElement("img"); 
img.alt = hintArray[ran]; 
img.src = imgArray[ran];
img.style.width = imgWidth;
img.style.maxHeight = "350px";
var a = document.createElement("a");
a.href = linkArray[ran];
a.target = "_blank";
a.appendChild(img);
//包含img的层 
var img_div = document.createElement("div"); 
img_div.className = "img"; 
img_div.appendChild(a); 

var div1 = document.createElement("div"); 
div1.className = "item_t"; 
div1.appendChild(img_div);
div1.appendChild(title_div);

var like = document.createElement("a");
like.className = "like_btn ml10";
like.style.color = "#FFFFFF";
like.style.fontWeight = "bold";
like.style.paddingLeft = "19px";
like.style.paddingTop = "3px";
like.innerHTML = "热度";
var num = document.createElement("em");
num.className = "bold";
num.innerHTML = viewsArray[ran];
var like_div = document.createElement("div");
like_div.className = "items_likes fl";
like_div.appendChild(like);
like_div.appendChild(num);
var comment_div = document.createElement("div");
comment_div.className = "items_comment fr";
comment_div.innerHTML = "<a href='" + replylinkArray[ran] + "' target='_blank'>回复<em class='bold pr5 pt5'>(" + repliesArray[ran] + ")</em></a>";
comment_div.style.paddingTop = "4px";

var divclear = document.createElement("div");
divclear.className = "clearfix";
divclear.appendChild(like_div);
divclear.appendChild(comment_div);

var div2 = document.createElement("div");
div2.className = "item_b clearfix pb10";
div2.appendChild(divclear);

var div = document.createElement("div");
div.className = "item";
div.appendChild(div1);
div.appendChild(div2);

ran++;

return [div,parseInt(heightArray[ran-1])+135];		//130为整个div除去图片的高度,heigthArray[ran-1]是图片的高度,结果返回的是div的高度
} 

//得到浏览器的名称 
function getBrowser() { 
var OsObject = ""; 
if (navigator.userAgent.indexOf("MSIE") > 0) { 
return "MSIE"; 
} 
if (isFirefox = navigator.userAgent.indexOf("Firefox") > 0) { 
return "Firefox"; 
} 
if (isSafari = navigator.userAgent.indexOf("Safari") > 0) { 
return "Safari"; 
} 
if (isCamino = navigator.userAgent.indexOf("Camino") > 0) { 
return "Camino"; 
} 
if (isMozilla = navigator.userAgent.indexOf("Gecko/") > 0) { 
return "Gecko"; 
} 
} 
