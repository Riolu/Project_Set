        var chars = ["#C62969", "#038360", "#AC170B", "#7C069D", "#006BA7", "#78B201", "#D15B03", "#B10101", "#FF7227", "#1788CA"];

        function generateMixed(n) {
            var char = 0;
            var len = n.length;
            for (var i = 0; i < len; i++) {
                var a = n.substr(i, 1);
                char = char ^ a.charCodeAt();
            }
            char = char % 10;
            var res = "";
            res = chars[char];
            return "background: none repeat scroll 0 0 "+res;
        }
