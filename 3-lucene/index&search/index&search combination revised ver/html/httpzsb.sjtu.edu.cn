<SCRIPT language="javascript">
    var u_u4_imgs = new VsbPreloadImgArray();
    
    //初始化图片
    function u_u4_init_img()
    {

            u_u4_imgs.addImg("\/images\/jdfg\/9.JPG", "0", u_u4_onimgload);

            u_u4_imgs.addImg("\/images\/jdfg\/8.jpg", "1", u_u4_onimgload);

