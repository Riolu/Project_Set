<BODY><script type="text/javascript" language="javascript">
    function changDiv(a){
        if(a==1){
            document.getElementById("ls1").style.display = "block";
            document.getElementById("ls").style.color = "#FFFF66";
            document.getElementById("ls2").style.display = "none";
            document.getElementById("web").style.color= "#FFFFFF";
        }
        else{
            document.getElementById("ls2").style.display = "block";
            document.getElementById("web").style.color = "#FFFF66";
            document.getElementById("ls1").style.display = "none";
            document.getElementById("ls").style.color = "#FFFFFF";
        }
    }
    function hideVideo(a){
        var div1 = document.getElementById("video"+a);
        var div = document.getElementById("video_show");
        div1.innerHTML = div.innerHTML;
        div.innerHTML="";
        document.getElementById("video_all").style.display="block";
    }
    function showVideo(a){
        var div1 = document.getElementById("video"+a);
        var div = document.getElementById("video_show");
        div.innerHTML = div1.innerHTML;
        div1.innerHTML = "";
        div.style.display="block";
        document.getElementById("video_all").style.display="none";
    }
    function changeColor(a){
        if(a==1){
            document.getElementById("zsjh").style.color = "#FFFF66";
            document.getElementById("check2").checked="true";
            document.getElementById("lnfs").style.color= "#FFFFFF";
            document.getElementById("check1").checked="false";
        }
        else{
            document.getElementById("lnfs").style.color = "#FFFF66";
            document.getElementById("check1").checked="true";
            document.getElementById("zsjh").style.color = "#FFFFFF";
            document.getElementById("check2").checked="false";
        }
    }
    function doSearch(){
        var type = 0;
        if(document.getElementById("check1").checked )
            type = 1;
        else
            type = 2;
        var province = document.getElementById("province").value;
        var year = document.getElementById("year").value;
        if(type == 2)
            window.open("lnfs.jsp?urltype=tree.TreeTempUrl&wbtreeid=1022&year="+year+"&sf="+province);
        else
            window.open("zsjh.jsp?urltype=tree.TreeTempUrl&wbtreeid=1021&year="+year+"&sf="+province);
    }
    function addEventHandler(oTarget, sEventType, fnHandler){
        if(oTarget.addEventListener){oTarget.addEventListener(sEventType, fnHandler, false);}
        else if(oTarget.attachEvent){oTarget.attachEvent('on' + sEventType, fnHandler);}
        else{oTarget['on' + sEventType] = fnHandler;}
    }
</script>
