	        function gdff(gdss){ 
				   var gds = gdss.substr(0, gdss.length);
				   var arr=gds.split(";");
				   setTimeout(function(){doset(arr, 0);}, 100);
	        }
			
		    function  doset(arr, i){
			   if(i<arr.length-1){
					var arr1=arr[i].split(",");
					//alert(i+'\n'+arr1[0]+'\n'+arr1[1]);
				    //$("#i1").attr("src",arr1[0]);
					//$("#a1").attr("href",arr1[1]); 
					document.getElementById('a1').setAttribute("href", arr1[1]);
				 	document.getElementById('i1').setAttribute("src", arr1[0]);
				 	document.getElementById("img1").style.display="";
					i++;
					setTimeout(function(){doset(arr, i);}, 3000);
				}else{
					i=0;
					setTimeout(function(){doset(arr, i);}, 3000);
				}
			}
			
			ff();
	      
