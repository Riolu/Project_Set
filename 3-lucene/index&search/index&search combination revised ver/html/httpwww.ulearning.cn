<script>
	//var url = window.location.href;
	
	var url = "/ulearning_web/user/login.do";
	//$("#loginBtnClick").attr("href","http://sso.ulearning.com.cn?service="+url+"");
	$("#loginBtnClick").attr("href",url);
	$(".personal").mouseover(function(){
		$(".pers-ope").show();
	});
	$(".personal").mouseout(function(){
		$(".pers-ope").hide();
	});
	
	//判断是否已登录
	var userID = $.cookie("lms_sessionuserid");
	var name = $.cookie("lms_sessionloginame_name");
	var sex = $.cookie("lms_sessionusersex");
	if(userID != "" && userID != null){
		$(".login").css("display","none");
		$(".personal").css("display","block");
		$(".li_spacer").css("display","inline");
		$("#logined_name").text(name);
		if(sex == 0){
			$("#headImg").attr("src","/ulearning_web/static/portal/images/stu_f.png");
		}
	}
	function checklogin(){
			if(typeof userID !="undefined" && userID!=null && userID!=""){
				var userRole = parseInt($.cookie("lms_user_role"));
				var url = "";
				switch(userRole){
					case 8:
						url = "/umooc/tutorv2/tutorManage.do?operation=index";
						break;
					case 9:
						url = "/umooc/learner/learner.do?operation=index";
						break;
					case 183:
						url = "/umooc/manager/manager.do?operation=index";
						break;
					default:
						url = "index.html";	
				}
				top.location=url;
			}
	
	}
	function logout(){
		$.cookie("lms_sessionuserid",null,{path:"/"}); 
		$.cookie("lms_sessionloginame_name",null,{path:"/"}); 
		$(".login").css("display","block");
		$(".personal").css("display","none");
		$(".li_spacer").css("display","none");
	}
	function closeTeleNav(){
		$(".teleNav").css("right","-251px");
	}
	function openTeleNav(){
		$(".teleNav").css("right","0");
	}
	function openapply(){
		window.open("/ulearning_web/web!apply.do");
	}
