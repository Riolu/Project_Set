						if(i%5 == 0){
							schoolRight.eq(i+4).css("border-right","1px solid #d1d1d1");
						}
					}
				}
				$(".schoolNum span").text(schoolNum);
            }
        });
        var schoolRight = $(".schools").children(".user_logo");
        var schoolNum = $(".schools").children(".user_logo").length;
});   
function copy(a,i){                             //将json转换为数组
    school[i]={"address":a.address,
        "text":a.text,
        "engname":a.engname,
        "imgsrc":a.imgsrc,
        "url": a.url,
        "vip":a.vip}
}
function nthchild(num,right,last){
    var sWidth = screen.width;
    if(sWidth>1080){
	    right.eq(num-4).css("margin-bottom","10px");
	    right.eq(num-3).css("margin-bottom","10px");
	    right.eq(num-2).css("margin-bottom","10px");
	    right.eq(num-1).css("margin-bottom","10px");
	    for(var i=1;i<=num;i++){
	        if(i%last==0){
	            right.eq(i-1).addClass("mr0"); 
	        }
	    }
    }
}
