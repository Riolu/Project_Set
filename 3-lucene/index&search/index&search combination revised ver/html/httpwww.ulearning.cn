					schoolRight.eq(schoolNum-3).css("border-bottom","1px solid #d1d1d1");
					schoolRight.eq(schoolNum-4).css("border-bottom","1px solid #d1d1d1");
					schoolRight.eq(schoolNum-5).css("border-bottom","1px solid #d1d1d1");
