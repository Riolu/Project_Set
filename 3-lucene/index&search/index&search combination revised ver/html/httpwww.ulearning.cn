                        copy(a[i],i);
                        i++;
                    }
                }
                /*产生10个不同的随机数*/
                var $random=new Array();
                for(var j=0;j<10;j++){
                    var random=Math.floor(Math.random()*school.length);
                    while($.inArray(random, $random)>=0){
                        random=Math.floor(Math.random()*school.length);
                    }
                    $random[j]=random;
                }
                for(var n=0;n<10;n++){
                    var sschool = getSchool(school[$random[n]].url,$random[n],school[$random[n]].text,school[$random[n]].imgsrc,school[$random[n]].engname);
                    html+=sschool;
                }
                $(".schools").append(html);
                var sWidth = screen.width;
                var schoolRight = $(".schools").children("div");
		    	var schoolNum = $(".schools").children(".user_logo").length;
				if(sWidth>1080){
					schoolRight.eq(schoolNum-1).css({"border-bottom":"1px solid #d1d1d1","border-right":"1px solid #d1d1d1"});
