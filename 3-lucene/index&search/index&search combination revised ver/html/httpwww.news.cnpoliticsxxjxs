<script type="text/javascript">
function float_nav(dom){
var right_nav=$(dom);
var nav_height=right_nav.height();
function right_nav_position(bool){
var window_height=$(window).height();
var nav_top=(window_height-nav_height)/2;
if(bool){
right_nav.stop(true,false).animate({top:nav_top+$(window).scrollTop()},0);
}else{
right_nav.stop(true,false).animate({top:nav_top},0);
}	
right_nav.show();
}

if(!+'\v1' && !window.XMLHttpRequest ){
$(window).bind('scroll resize',function(){
if($(window).scrollTop()>0){
right_nav_position(true);	
}else{
right_nav.hide();	
}
})
}else{
$(window).bind('scroll resize',function(){
if($(window).scrollTop()>0){
right_nav_position();
}else{
right_nav.hide();
}
})
}	
}
float_nav('#float');

</script>
<script type="text/javascript" src="http://www.xinhuanet.com/politics/xxjxs_footer.js"></script>
<script>
function btnSQTop() {
	var topBannerH = $('#topBanner').height() - $('.btnSQ').height()
	$('.btnSQ').css({top: topBannerH});	
}

$(window).resize(function() {	btnSQTop() });

$(function(){
	btnSQTop();
	
	time = window.setTimeout( function () { $(".btnSQ").hide(); $('#panel').slideUp(2000, function () { $(".btnSQ2").show(); } );}, 7000 );
	
	$('.btnSQ').click( function () {
		$(this).hide();
		$('#panel').slideUp(2000, function () { $('.btnSQ2').show() });
	    clearTimeout(time);
	});
	
	$('.btnSQ2').click( function () {
		$(this).hide();
		$('#panel').slideDown(2000, function () { $('.btnSQ').show() });
		time = window.setTimeout( function () { $(".btnSQ").hide(); $('#panel').slideUp(2000, function () { $(".btnSQ2").show(); } );}, 7000 );
	});

});
</script>
