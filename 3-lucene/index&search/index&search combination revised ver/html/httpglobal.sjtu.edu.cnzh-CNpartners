<script>
  (function() {
    $(function() {
      $('.tab-pane .sort-lists > li').map(function(i, e) {
        if ($(e).find('.box').size() > 3) {
          $(e).find('.box').slice(3).hide();
          return $(e).find('.expand').show();
        } else {
          $(e).find('.collapse').hide();
          return $(e).find('.expand').hide();
        }
      });
      $('.tab-pane .sort-lists').map(function(i, e) {
        if ($(e).find('h1').size() > 3) {
          return $(e).find('> li').slice(3).hide();
        } else {
          return $(e).find('.page-bottom .view-more').hide();
        }
      });
      $('.tab-content .view-more').click(function(e) {
        e.preventDefault();
        $('.tab-pane.active li').show();
        return $('.page-bottom .view-more').hide();
      });
      return $('.nav-area li').click(function(e) {
        var i;
        i = $('.nav-area li').index($(e.currentTarget));
        if ($('.tab-pane').eq(i).find('h1').size() > 3) {
          return $('.page-bottom .view-more').show();
        } else {
          return $('.page-bottom .view-more').hide();
        }
      });
    });
  
  }).call(this);
</script>
