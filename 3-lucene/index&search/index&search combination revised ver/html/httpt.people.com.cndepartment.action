$(function(){
	var time=5000;
	var data_num=0;
	var list_num=44;
	var row_count=0;
	var timer1;
	var timer2;
	var msgNoMatch;
	function displayblock(rn){
		return function(){
			$("#recent"+rn).remove().css("display","none").prependTo("#tw_recent")
			}
	}
	function addblock(){
		var h=(row_count+list_num)%data_num;
		var line_height=0;
		row_count=(row_count+1)%data_num;
		var content=$("#recent"+h).find("#nomatchmsg").html().length;
		var clq=parseInt(content/48); 
		switch(clq){
			case 0:
				line_height=85;
				break;
			case 1:
				line_height=105;
				break;
			case 2:
				line_height=125;
				break;
			case 3:
				line_height=150;
				break;
			case 4:
				line_height=175;
				break;												
				default:
				break;
		}
		$("#recent"+h).css("height","1px").animate({height:line_height+"px",opacity: 'toggle'},1000,displayblock(row_count));
		timer1=setTimeout(addblock,time);
	}
	window.showRecent=function(l){
	try{
		$("#tw_recent").html(l.recent);
		var k=data_num=$("#tw_recent").find("div.twi").length;
		$("#tw_recent").find("div.twi").each(function(){k--;$(this).attr("id","recent"+k)});
		for(var h=0;h<list_num;h++){$("#recent"+h).fadeIn("slow");}
		timer2=setTimeout(addblock,time);
	}
	catch(m){}
    };
    window.stopRecent=function(){
    	clearTimeout(timer1);
    	clearTimeout(timer2);//当未滚动之前
    }
    window.restarRecent=function(){
    	setTimeout(addblock,0);
    }
	var recent ;
	reporter.creatMsgList(function(data){
		recent={"recent":data};
		showRecent(recent);
	});
$("#on-off").toggle(
		  function () {
		    $(this).attr("src","/microblog/specialsubject/department/images/ico11.gif");
		    stopRecent();
		  },
		  function () {
		    $(this).attr("src","/microblog/specialsubject/department/images/ico10.gif");
		    restarRecent();
		  }
		);   
});
