            ___content: "id:wztj_div",
            //___content:"url:get?page/index_map.html",
            ___width: "980",
            ___height: "550",
            ___drag: "___boxTitle",
            ___showbg: true
        });
      });

        $.post("/wsbs/caseWeb_countInfoApply.do",function(data) {
            if(data) {
                $("#infoapplycount_div").html("建站以来共有【"+data.count+"】次<br/>请求公开信息");
            }
        });
        
        $.post("/wsbs/itemWeb_countItem.do", function(data) {
            if(data) {
                $("#shz_li").html("审核中的事项【"+data.shz+"】项");
                $("#slz_li").html("正在受理中的事项【"+data.slz+"】项");
                $("#ybj_li").html("已办结事项【"+data.ybj+"】项");
            }
        });
        $.post("/cms/countArticle.do?chnlid=915",function(data) {
            if(data) {
                $("#index_hyfw_count").html("【"+data.count+"】");
            }
        });
        $.post("/cms/countArticle.do?chnlid=916",function(data) {
            if(data) {
                $("#index_bmcx_count").html("【"+data.count+"】");
            }
        });
        // autocomplete
        $( "#searchKey" ).autocomplete({
            select: function(event, ui) {
                $("#searchKey").val(ui.item.value);
                $("#searchForm").attr("action","/cms/ArtiSearch.do");
                $("#searchForm").submit();
            }
        });
        $.post("/cms/keywordsuggest.do",function(data) {
            if(data) {
                $( "#searchKey" ).autocomplete( "option", "source", data);
            }
        });
    })
</script>

</body>
</html>
