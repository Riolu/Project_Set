            function(){
                $(this).find("div").slideUp();
            }
        );
    },
    goTop:function(){
        $("#productBody .goTop").click(function(){
            $('html,body').animate({
                scrollTop : '0px'
            }, 200);
        });
    },
    closeBody:function(){
        $("#productBody .close").click(function(){
            $("#productBody").slideUp();
        });
    },
    bodyEv:function(){
        $("#productBody").hover(
            function(){
                $(this).find(".lazyload").each(function(){
                    $(this).attr("src",$(this).attr("data-original")).removeClass("lazyload");
                })
            },
            function(){}
        )
    }
};

productBodyEv.init();

</script>
