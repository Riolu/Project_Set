		if(G_PLAYER_INSTANCE != null)
		{
			G_PLAYER_INSTANCE.close();
		}
		if(spArray[m-1]!='is undefined'){
			G_PLUGIN_MOVIE.playBysUrl(spArray[m-1].videoname,true);
		}
	};
}

