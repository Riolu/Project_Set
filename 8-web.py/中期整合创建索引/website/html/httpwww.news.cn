	G_PLUGIN_MOVIE.addCallBack("onEnd",function(){
		if(spn > 0 && spn < spArray.length + 1){
			//G_PLAYER_INSTANCE.close();
			G_PLUGIN_MOVIE.playBysUrl(spArray[spn].videoname,true);
