<script language="javascript">

/*tab style class*/
cssClass1="tab1";
selectedCssClass1="tab1-selected";

cssClass2="tab2";
selectedCssClass2="tab2-selected";

cssClass3="tab3";
selectedCssClass3="tab3-selected";
/*
	params: tabList --标签组的列表 (left_1_1,left_1_2 ...)
			src		--来源对象		(this)
			mode	--模式			1,2,3
*/
function changeTab(tabList,src,mode)
{
	//隐藏tablist和所有相对应的panel
	if(mode=="1")
	{
		cssClass=cssClass1;
		selectedCssClass=selectedCssClass1;
	}
	else if(mode=="2")
	{
		cssClass=cssClass2;
		selectedCssClass=selectedCssClass2;
	}
	else if(mode=="3")
	{
		cssClass=cssClass3;
		selectedCssClass=selectedCssClass3;
	}
	
	if(tabList!="")
	{
		var arr=tabList.split(";");
		for(i=0;i<arr.length;i++)
		{
			if(arr[i]!="")
			{
				document.all(arr[i]).className = mode=="ss" ? "ss"+cssClass : cssClass;
				
				try{
					document.all(arr[i]+"_p").style.display="none";
				}
				catch(e)
				{
					alert("没有找到对象:["+arr[i]+"_p]");
				}
			}
		}
	}
	//选择的tab
	src.className = mode=="ss" ? "ss"+selectedCssClass : selectedCssClass;
	
	//显示选择tab的panel
	document.all(src.id+"_p").style.display="block";
}

</script>
