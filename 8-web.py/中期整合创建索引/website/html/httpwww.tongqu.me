<script type='text/ng-template' id='tqOneAct.html'>
    <div class="one-home-act" fade exp="$parent.$index >= $parent.start && $parent.$index <= $parent.end" is-fade-in="true" is-fade-out="false" ng-click="showDetail(act.actid)">
        <div class="poster">
            <act-poster poster="{{act.poster}}" type="{{act.type}}" delay-load="{{$parent.$index * 100}}"></act-poster>
        </div>
        <div class="bottom">
            <div class="name">{{act.name}}</div>
            <div class="time" ng-switch="act.time_status">
                <span ng-switch-when="2">报名：{{act.sim_sign_start_time}}</span>
                <span ng-switch-when="1">截止：{{act.sim_sign_end_time}}</span>
                <span ng-switch-when="6">开始：{{act.sim_start_time}}</span>
                <span ng-switch-default>结束：{{act.sim_end_time}}</span>
