            <div>
                <div class="member pull-right">已有&nbsp;<span class="tq-orange">{{act.member_count}}</span>&nbsp;人报名</span>
                </div>
                <label class="status label pull-left"
                    ng-class="{'label-danger':act.time_status==0,
                              'label-success':act.time_status==1,
                              'label-info':act.time_status==2||act.time_status==3,
                              'label-default':act.time_status>3}"
                >
                    {{actStatus[act.time_status]}}
                </label>
                <div style="clear:both">
            </div>
        </div>
    </div>
</script>
