                $('#advancedOptions').show();
                $('#ToggleButton').html(hideOptionsMarkup);
            }
            else
            {
                $('#advancedOptions').hide();
                $('#ToggleButton').html(showOptionsMarkup);
            }

            return false;
        }

        $('#ToggleButtonDiv').show();


            
