    $(document).ready(function () {
        $('.mstHdr_Flyout').find('a:last').on('keydown', function(event) {
            if ((event.keyCode == $.ui.keyCode.TAB) && !event.shiftKey) {
                $('.mstHdr_MenuItemAccount').removeClass('selected');
                $('.mstHdr_Flyout').hide();
            };
        });
    });

    $('.mstHdr_MenuItemAccount').on('keydown', function (event) {
