	<!-- start to detect if client is in high contrast mode-->
	<div class="highcontrasttest" aria-hidden="true" style="display:none;background-image:url(//az623152.vo.msecnd.net/library/images/support/cn/1ptrans.gif);">
	</div>
	<script type="text/javascript">
		(function ()
		{
			var isHighcontrast = false;
			var bgImage = $('.highcontrasttest').css('background-image').toLowerCase();
			if (bgImage == 'none')
			{
				isHighcontrast = true;
			}
			else
			{
				//Chome doesn't support native high contrast mode. It depends on extension.
				var chromeExtension = $('#hc_extension_svg_filters');
				if (chromeExtension.length > 0)
				{
					var hcx = $('html').attr('hcx');
					if (hcx && hcx !== '0')
					{
						isHighcontrast = true
					}
				}
			}
			$('head meta').last().after('<meta name="ms.ishc" content="' + isHighcontrast + '" />');
		})();
	</script>
