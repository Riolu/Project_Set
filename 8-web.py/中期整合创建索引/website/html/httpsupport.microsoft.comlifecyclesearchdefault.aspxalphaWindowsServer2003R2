	<script type="text/javascript">
		//Display content when JavaScript is enabled
		document.getElementById("contentArea").style.display = "block";

		$(document).ready(function ()
		{
			var hasChild = $("#contentArea").children().children().hasClass("kb");

			$(function ()
			{
				var t = "euCookie", n = ".euCookie:visible";
				function setCookie(key, value)
				{
					var expires = new Date();
					expires.setTime(expires.getTime() + 365 / 2 * 864e5);
					document.cookie = key + '=' + value + ';path=/' + ';expires=' + expires.toUTCString();
				}
				$(".euCookie-close").click(function (r) { $(n).hide(), setCookie(t, "1") });
			})


			if (hasChild)
			{
				$("div.pdf.Content").addClass("kb-container");
			}

			$(".topNav li.hasChild > button").click(function ()
			{
				// workaround for href="javascript:void(0)" causing beforeunload event to fire in IE
				return false;
			});

			$('.topNav li button').on('click', function ()
			{
				toggleMenu(this);
			});

			SetTopNavBackgroundWidth();

			$(document).bind('click', document_click);

			function SetTopNavBackgroundWidth()
			{
				var width = document.body.clientWidth;
				var margin = width > 998 ? (width - 998) / 2 : 0;
				var adjustedMargin = margin + 40;

				$('.menuBackground').css('width', 917 + 2 * adjustedMargin);
				$('.menuBackground').css('margin-left', '-' + adjustedMargin + 'px');

				$('.menuContainer').css('margin-left', adjustedMargin + 'px')
			}

			function toggleMenu(menu)
			{
				$menu = $(menu);
				$closestLi = $menu.closest('li');

				$closestLi.siblings('li').find('.menuBackground').each(function (index, value)
				{
					var menuItem = $(this).siblings('a, button');

					if (menuItem.hasClass('clicked') && $(menuItem).closest('li').hasClass('hasChild'))
					{
						toggleAriaExpanded(menuItem);
					}

					menuItem.removeClass('clicked');

					$(this).hide();
				});

				if ($closestLi.hasClass('hasChild'))
				{
					var background = $closestLi.find('.menuBackground');
					background.toggle();
					toggleAriaHidden(background);
					$menu.toggleClass('clicked');

					toggleAriaExpanded($menu);
				}
			}

			function toggleAriaHidden(subMenuArea)
			{
				var $subMenuArea = $(subMenuArea);
				var ariaHidded = $subMenuArea.attr('aria-hidden');

				if (ariaHidded && ariaHidded == 'true')
				{
					$subMenuArea.attr('aria-hidden', 'false');
				}
				else if (!ariaHidded || ariaHidded == 'false')
				{
					$subMenuArea.attr('aria-hidden', 'true');
				}
			}

			function toggleAriaExpanded(menuItem)
			{
				var $menuItem = $(menuItem);
				var ariaExpanded = $menuItem.attr('aria-expanded');

				if (ariaExpanded && ariaExpanded == 'true')
				{
					$menuItem.attr('aria-expanded', 'false');
				}
				else if (!ariaExpanded || ariaExpanded == 'false')
				{
					$menuItem.attr('aria-expanded', 'true');
				}
			}

			function document_click()
			{
				$(document).find('.topNav button[aria-expanded="true"]').each(function (index, value)
				{
					toggleMenu(this);
				});
			}

			PDF.Events.Bind('ComponentLoaded', function (event, data)
			{
				if (data && data.componentName.toLowerCase() === 'productselector')
				{
					window.setTimeout(function ()
					{
						MS.Support.Menu($('.NavBarSection'), toggleMenu);
					}, 800);
				}
			});
		});

		var MS = MS || {};
		MS.Support = MS.Support || {};

		MS.Support.Menu = function (element, toggleSubmenu)
		{
			var menuTree = {};
			var menuRoleArray = [];
			var handledKeycodes = [27, 32, 37, 38, 39, 40];
			var traverseMenu = function (root, parent)
			{
				if (root)
				{
					var $root = $(root);
					if ($root.is('a, button') && menuRoleArray && menuRoleArray.length)
					{
						var length = menuRoleArray.length;
						var roleName = menuRoleArray[length - 1];
						var menuNode = {};
						menuNode.Link = $root;
						parent = parent || {};
						parent.Children = parent.Children || [];
						parent.Children[parent.Children.length] = menuNode;
						menuNode.Parent = parent;
						menuNode.ParentType = roleName;
						menuNode.Index = (parent.Children.length - 1);
						menuNode.Id = parent.Id ? parent.Id + menuNode.Index + '' : menuNode.Index + '';
					}

					if ($(root).attr('role'))
					{
						var roleName = $(root).attr('role').toLowerCase();
						if (roleName === 'menubar' || roleName === 'menu')
						{
							if (menuRoleArray)
							{
								var length = menuRoleArray.length;
								menuRoleArray[length] = roleName;
							}
							else
							{
								menuRoleArray = [roleName];
							}
							if (parent && parent.Children && parent.Children.length)
							{
								parent = parent.Children[parent.Children.length - 1];
							}
						}

					}

					if ($root.children() && $root.children().length > 0)
					{
						$root.children().each(function (index, element)
						{
							traverseMenu(element, parent);
						});
					}

					if ($(root).attr('role'))
					{
						var roleName = $(root).attr('role').toLowerCase();
						if (roleName === 'menubar' || roleName === 'menu')
						{
							if (menuRoleArray && menuRoleArray.length)
							{
								var length = menuRoleArray.length;
								menuRoleArray = menuRoleArray.slice(0, length - 1);
							}
							parent = parent.Parent;
						}
					}
					return;
				}
				else
				{
					return;
				}
			};

			var bindKeyboardToMenu = function (menuTreeRoot)
			{
				if (menuTreeRoot)
				{
					if (menuTreeRoot.Link)
					{
						menuTreeRoot.Link.unbind('keydown.support');
						menuTreeRoot.Link.bind('keydown.support', { currentMenu: menuTreeRoot }, keydownHandler);
					}
					if (menuTreeRoot.Children && menuTreeRoot.Children.length)
					{
						$.each(menuTreeRoot.Children, function (index, child)
						{
							bindKeyboardToMenu(child);
						});
					}
				}
			}

			var keydownHandler = function (e)
			{
				e = e || window.event;
				var keyCode = (typeof e.which == "number") ? e.which : e.keyCode;

				var newMenuRoot;
				var menuTreeRoot = e.data.currentMenu;
				if (menuTreeRoot.ParentType === 'menubar')
				{
					if (keyCode == 37 && !e.altKey)
					{
						if (menuTreeRoot.Index)
						{
							newMenuRoot = menuTreeRoot.Parent.Children[menuTreeRoot.Index - 1];
						}
						else if (menuTreeRoot.Parent && menuTreeRoot.Parent.Index)
						{
							newMenuRoot = menuTreeRoot.Parent.Parent.Children[menuTreeRoot.Parent.Index - 1];
						}
					}
					else if (keyCode == 38)
					{
						if (menuTreeRoot.Parent && menuTreeRoot.Parent.Parent)
						{
							newMenuRoot = menuTreeRoot.Parent;
						}
					}
					else if (keyCode == 39)
					{
						if (menuTreeRoot.Parent && menuTreeRoot.Parent.Children && menuTreeRoot.Index < menuTreeRoot.Parent.Children.length - 1)
						{
							newMenuRoot = menuTreeRoot.Parent.Children[menuTreeRoot.Index + 1];
						}
						else if (menuTreeRoot.Parent && menuTreeRoot.Parent.Parent && menuTreeRoot.Parent.Parent.Children && menuTreeRoot.Parent.Index < menuTreeRoot.Parent.Parent.Children.length - 1)
						{
							newMenuRoot = menuTreeRoot.Parent.Parent.Children[menuTreeRoot.Parent.Index + 1];
						}
					}
					else if (keyCode == 40)
					{
						if (menuTreeRoot.Children && menuTreeRoot.Children.length)
						{
							newMenuRoot = menuTreeRoot.Children[0];
						}
					}
				}
				else if (menuTreeRoot.ParentType === 'menu')
				{
					if (keyCode == 37 && !e.altKey)
					{
						if (menuTreeRoot.Parent && menuTreeRoot.Parent.Parent && menuTreeRoot.Parent.Index)
						{
							newMenuRoot = menuTreeRoot.Parent.Parent.Children[menuTreeRoot.Parent.Index - 1];
						}
					}
					else if (keyCode == 38)
					{
						if (menuTreeRoot.Index)
						{
							newMenuRoot = menuTreeRoot.Parent.Children[menuTreeRoot.Index - 1];
						}
						else
						{
							newMenuRoot = menuTreeRoot.Parent;
						}
					}
					else if (keyCode == 39)
					{
						if (menuTreeRoot.Parent && menuTreeRoot.Parent.Parent && menuTreeRoot.Parent.Index < menuTreeRoot.Parent.Parent.Children.length - 1)
						{
							newMenuRoot = menuTreeRoot.Parent.Parent.Children[menuTreeRoot.Parent.Index + 1];
						}
					}
					else if (keyCode == 40)
					{
						if (menuTreeRoot.Index < menuTreeRoot.Parent.Children.length - 1)
						{
							newMenuRoot = menuTreeRoot.Parent.Children[menuTreeRoot.Index + 1];
						}
					}
				}
				if (typeof toggleSubmenu === 'function')
				{

					if (keyCode == 38 && menuTreeRoot.Parent && !menuTreeRoot.Parent.Parent && isSubmenuOpen(menuTreeRoot))
					{
						toggleSubmenu(menuTreeRoot.Link);
					}
					else if (keyCode == 40 && menuTreeRoot.Parent && !menuTreeRoot.Parent.Parent && !isSubmenuOpen(menuTreeRoot))
					{
						toggleSubmenu(menuTreeRoot.Link);
					}
					else if (keyCode == 27)
					{
						var mainNavMenu = menuTreeRoot;
						while (mainNavMenu.Parent && mainNavMenu.Parent.Parent)
						{
							mainNavMenu = mainNavMenu.Parent;
							newMenuRoot = mainNavMenu;
						};

						if (isSubmenuOpen(mainNavMenu))
						{
							toggleSubmenu(mainNavMenu.Link);
						}
					}
					else if (keyCode == 32 && menuTreeRoot.Parent && !menuTreeRoot.Parent.Parent)
					{
						toggleSubmenu(menuTreeRoot.Link);
					}
					else if (newMenuRoot && isSubmenuOpen(menuTreeRoot) && !isSubmenuOpen(newMenuRoot) && !isDecendent(menuTreeRoot, newMenuRoot))
					{

						toggleSubmenu(menuTreeRoot.Link);
						toggleSubmenu(newMenuRoot.Link);
					}
				}
				if (newMenuRoot && newMenuRoot.Link && typeof newMenuRoot.Link.trigger === 'function')
				{
					newMenuRoot.Link.trigger('focus');
				}
				if ($.inArray(keyCode, handledKeycodes) > -1)
				{
					if (e.stopPropagation)
					{
						e.stopPropagation();
					} else
					{
						e.cancelBubble = true;
					}
					e.preventDefault();
					return false;
				}
				else
				{
					return true;
				}
			}

			var isSubmenuOpen = function (menu)
			{
				var menuExpanded = false;
				if (menu && menu.Parent)
				{
					if (menu.Children && menu.Children.length)
					{
						$.each(menu.Children, function (index, child)
						{
							if (!$(child.Link).is(':hidden'))
							{
								menuExpanded = true;
								return false;
							}
						});
					}
					if (!menuExpanded && menu.Parent.Parent && !$(menu.Link).is(':hidden'))
					{
						menuExpanded = true;
					};
				}
				return menuExpanded;
			};

			var isDecendent = function (ancester, decendent)
			{
				if (ancester && decendent && ancester.Id === decendent.Id)
				{
					return true;
				}
				if (decendent.Parent)
				{
					return isDecendent(ancester, decendent.Parent);
				}
				return false;
			};

			traverseMenu(element, menuTree);

			bindKeyboardToMenu(menuTree);

			return menuTree;
		}
	</script>
	<script src="//ajax.aspnetcdn.com/ajax/jquery.ui/1.10.4/jquery-ui.min.js"></script>
