					PDF.LoadComponent(el, componentName, { inlineTemplate: true });
				});
			});
		});

		/// Event handling for product selector
		/// data.PID, data.PName, data.Url
		///
		PDF.Events.Bind("LoadProductPage", function (e, data)
		{
			// If Url is passed open new window
			if (data.hasOwnProperty('URL') && data.URL)
			{
				window.open(data.URL);
				return;
			}



			var browserInfoKey = 'internet-explorer-11';
			var osInfoKey = 'windows-8-1';

			if (data.hasOwnProperty('PID') && data.PID)
			{
				// If it is more product redirect to all products page.
				if (data.PID === '00000')
				{
					var allProductsUrl = '/AllProducts';
					if (data.UseTarget)
					{
						window.open(allProductsUrl);
					}
					else
					{
						window.location.assign(allProductsUrl);
					}
				}
				else
				{
					var productHubUrlFormat = '/Product/{0}';
					var formattedProduct = "";
					if (data.ProductObj.Value.hasOwnProperty('Productlist') && data.ProductObj.Value.Productlist)
					{
						var versionKey = ''
						if (browserInfoKey && data.ProductObj.Value.Key == 'internet-explorer')
						{
							versionKey = browserInfoKey;
						}
						if (osInfoKey && data.ProductObj.Value.Key == 'windows')
						{
							versionKey = osInfoKey;
						}
						else
						{
							versionKey = data.ProductObj.Value.Productlist[0].Key.replace(' ', '-').replace('.', '-')
						}
						formattedProduct = "{0}/{1}".format(data.PName.replace(' ', '-').replace('.', '-'), versionKey);
					}
					else
					{
						formattedProduct = data.PName.replace(' ', '-').replace('.', '-');
					}
					if (data.UseTarget)
					{
						window.open(productHubUrlFormat.format(formattedProduct));
					}
					else
					{
						window.location.assign(productHubUrlFormat.format(formattedProduct));
					}
				}
			}
		});
	</script>

