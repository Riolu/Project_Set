<script type="text/javascript">

    var shiftpressed = "FALSE";

    function AccountSlideUp()
    {
        $('.mstHdr_MenuItemAccount').removeClass('selected');
        $('.mstHdr_Flyout').hide();
    }
    $('.topNav').on('keydown', function (event) {
        if (event.keyCode == $.ui.keyCode.ESCAPE) {
            $('.topNav').find("a.clicked, button.clicked").removeClass("clicked");
            $('.topNav').find("div.menuBackground").hide();
        }
    });
    function LoadTabEvent(event)
    {
        var key;
        var isShift;
        if (window.event) {
            key = window.event.keyCode;
            isShift = window.event.shiftKey ? true : false;
        } else {
            key = ev.which;
            isShift = ev.shiftKey ? true : false;
        }
        if ((key == 16) && (isShift))
        { shiftpressed = "TRUE";}

    }    
    function skipFoc() {
        if (shiftpressed == "TRUE") {
            $('.menuBackground').slideUp();
            shiftpressed = "FALSE"
        }
    }
 </script>
	</div>

