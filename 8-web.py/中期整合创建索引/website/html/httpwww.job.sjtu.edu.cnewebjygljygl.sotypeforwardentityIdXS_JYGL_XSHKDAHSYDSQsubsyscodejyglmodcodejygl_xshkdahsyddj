<script type="text/javascript" language="javascript"> 
//<!CDATA[ 
function g(o){return document.getElementById(o);} 
function HoverLi2(f,e,o){ 
//如果有N个标签,就将i<=N;  
//本功能非常OK,兼容IE7,FF,IE6 
	for(var t=1;t<=o;t++){
		g('tb2_'+t+'_'+e).className='normaltab2';
		g('tbc2_0'+t+'_'+e).className='undis2';
		g('m012_'+t+'_'+e).className='undis2';
	}
	g('tb2_'+f+'_'+e).className='hovertab2'; 
	g('tbc2_0'+f+'_'+e).className='dis2';
	g('m012_'+f+'_'+e).className='dis2';
} 
</script>