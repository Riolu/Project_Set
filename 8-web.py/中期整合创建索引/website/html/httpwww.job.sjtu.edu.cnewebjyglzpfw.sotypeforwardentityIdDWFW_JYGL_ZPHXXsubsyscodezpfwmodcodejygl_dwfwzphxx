<script type="text/javascript" language="javascript">
function reloadCode(){
	document.getElementById('rpdimg').src= '/eweb/wfc_r4d.so?type=r4d&t='+new Date().getTime();
}
String.prototype.trim=function(){
    return this.replace(/(^\s*)|(\s*$)/g, "");
}
function viewWrong(){
	document.getElementById('wrong').style.display='block';
}
function noneWrong(){
	document.getElementById('wrong').style.display='none';
}
function checkSubmitForm(){
var dlObj = document.getElementById('dlButton');
dlObj.innerHTML='正在登录...';
var usercodeObj = document.getElementById('usercode');
var pwdObj = document.getElementById('pwd');
var vpwdObj = document.getElementById('vpwd');
var msg = document.getElementById('wrongtxt');
if('' == usercodeObj.value || usercodeObj.value.trim().length == 0){
	msg.innerHTML='用户名不能为空！';
	viewWrong();
	usercodeObj.value = usercodeObj.value.trim();
	usercodeObj.focus();
	dlObj.innerHTML='登录';
	return false;
}
if('' == pwdObj.value || pwdObj.value.trim().length == 0){
    msg.innerHTML='密码不能为空！';
    viewWrong();
	pwdObj.value = pwdObj.value.trim();
	pwdObj.focus();
	dlObj.innerHTML='登录';
	return false;
}
if('' == vpwdObj.value || vpwdObj.value.trim().length == 0){
    msg.innerHTML='验证码不能为空！';
    viewWrong();
	vpwdObj.value = vpwdObj.value.trim();
	vpwdObj.focus();
	dlObj.innerHTML='登录';
	return false;
}else if(vpwdObj.value.trim().length !=4 ){
	viewWrong();
 	msg.innerHTML='验证码不正确！';
 	vpwdObj.focus();
 	dlObj.innerHTML='登录';
	return false;
}
document.getElementById('form1').submit();
}

document.onkeydown = function(evt){
	var evt = window.event?window.event:evt;
	if(evt.keyCode==13) {
		checkSubmitForm();
	}
}


</script>


