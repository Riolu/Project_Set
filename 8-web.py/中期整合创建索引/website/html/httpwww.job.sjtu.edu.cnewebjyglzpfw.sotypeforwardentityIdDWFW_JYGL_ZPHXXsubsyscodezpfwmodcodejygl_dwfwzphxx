//<!CDATA[ 
function g(o){return document.getElementById(o);} 
function HoverLi3(r,s,o){ 
//如果有N个标签,就将i<=N;  
//本功能非常OK,兼容IE7,FF,IE6 
	for(var h=1;h<=o;h++){
		g('tb3_'+h+'_'+s).className='normaltab3';
		g('tbc3_0'+h+'_'+s).className='undis3';
		g('m013_'+h+'_'+s).className='undis3';
	}
	g('tb3_'+r+'_'+s).className='hovertab3w'; 
	g('tbc3_0'+r+'_'+s).className='dis3';
	g('m013_'+r+'_'+s).className='dis3';
} 
</script>
<script type="text/javascript" language="javascript"> 
//<!CDATA[ 
function g(o){return document.getElementById(o);} 
function HoverLi2(f,e,o){ 
//如果有N个标签,就将i<=N;  
//本功能非常OK,兼容IE7,FF,IE6 
	for(var t=1;t<=o;t++){
		g('tb2_'+t+'_'+e).className='normaltab2';
		g('tbc2_0'+t+'_'+e).className='undis2';
		g('m012_'+t+'_'+e).className='undis2';
	}
	g('tb2_'+f+'_'+e).className='hovertab2w'; 
	g('tbc2_0'+f+'_'+e).className='dis2';
	g('m012_'+f+'_'+e).className='dis2';
} 
</script>
<script type="text/javascript" language="javascript"> 
//<!CDATA[ 
function g(o){return document.getElementById(o);} 
function HoverLi3(k,l,o){ 
//如果有N个标签,就将i<=N;  
//本功能非常OK,兼容IE7,FF,IE6 
	for(var p=1;p<=o;p++){
		g('tb5_'+p+'_'+l).className='normaltab5';
		g('tbc5_0'+p+'_'+l).className='undis5';
		g('m015_'+p+'_'+l).className='undis5';
	}
	g('tb5_'+k+'_'+l).className='hovertab5w'; 
	g('tbc5_0'+k+'_'+l).className='dis5';
	g('m015_'+k+'_'+l).className='dis5';
} 
</script>
