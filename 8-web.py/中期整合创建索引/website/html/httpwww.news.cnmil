        $(window).trigger("scroll")
    }).trigger("resize");
    h.click(function(a) {
        a = $(this).attr("href");
        $("html,body").addClass("scrolling").animate({
            scrollTop: $(a).offset().top - 65 + "px"
        },
        800).removeClass("scrolling");
        return false
    });
}
</script>
