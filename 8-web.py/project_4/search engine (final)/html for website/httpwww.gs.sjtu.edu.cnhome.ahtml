<script type="text/javascript" language="Javascript1.1"> 
    function required () { 
     	this.aa = new Array("username", "用户名 要填写.", new Function ("varName", " return this[varName];"));
     	this.ab = new Array("password", "密码 要填写.", new Function ("varName", " return this[varName];"));
     	this.ac = new Array("usertype", "用户类型 要填写.", new Function ("varName", " return this[varName];"));
    } 

	function validateRequired(form) {
                var isValid = true;
                var focusField = null;
                var i = 0;
                var fields = new Array();
                oRequired = new required();
                for (x in oRequired) {
                	var field = form[oRequired[x][0]];
                	
                    if (field.type == 'text' ||
                        field.type == 'textarea' ||
                        field.type == 'file' ||
                        field.type == 'select-one' ||
                        field.type == 'radio' ||
                        field.type == 'password') {
                        
                        var value = '';
						// get field's value
						if (field.type == "select-one") {
							var si = field.selectedIndex;
							if (si >= 0) {
								value = field.options[si].value;
							}
						} else {
							value = field.value;
						}
                        
                        if (trim(value).length == 0) {
                        
	                        if (i == 0) {
	                            focusField = field;
	                        }
	                        fields[i++] = oRequired[x][1];
	                        isValid = false;
                        }
                    }
                }
                if (fields.length > 0) {
                   focusField.focus();
                   alert(fields.join('\n'));
                }
                return isValid;
            }
            
	// Trim whitespace from left and right sides of s.
	function trim(s) {
            return s.replace( /^\s*/, "" ).replace( /\s*$/, "" );
    }
    
    function gotoURL(url){
		this.location=url;
	}
	function onon(){
	var a=formsou.wordt.value;
	a=a+" "+"site:gs.sjtu.edu.cn";
	formsou.word.value=a;
	formsou.submit();
	return 1;
	}
</script>

