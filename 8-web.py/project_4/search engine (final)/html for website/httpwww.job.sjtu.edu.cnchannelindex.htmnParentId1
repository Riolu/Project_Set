<script type="text/javascript">
	function testAuto(thisId, needLeng) {
		var element = document.getElementById(thisId);
		var nowLeng = element.innerText.length;
		if (nowLeng > needLeng) {
			var nowWord = element.innerText.substr(0, needLeng) + '...';
			element.innerHTML = nowWord;
		}
	}
</script>
